{
  String clusterName="clusterName";
  String body="{ \"type\":\"hdfs-site\", \"tag\":\"my-new-tag\"," + "\"properties\":{ \"key1\":\"value1\", \"key2\":\"value2\" } }";
  registerExpectations(Request.Type.POST,body,200,false);
