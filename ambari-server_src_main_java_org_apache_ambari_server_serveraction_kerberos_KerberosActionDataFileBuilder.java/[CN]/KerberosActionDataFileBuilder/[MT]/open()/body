{
  if (isClosed()) {
    if (file == null) {
      throw new IOException("Missing file path");
    }
 else {
      boolean writeHeader=file.length() == 0;
      csvPrinter=new CSVPrinter(new FileWriter(file,true),CSVFormat.DEFAULT);
      if (writeHeader) {
        csvPrinter.printRecord(HOSTNAME,SERVICE,COMPONENT,PRINCIPAL,PRINCIPAL_CONFIGURATION,KEYTAB_FILE_PATH,KEYTAB_FILE_OWNER_NAME,KEYTAB_FILE_OWNER_ACCESS,KEYTAB_FILE_GROUP_NAME,KEYTAB_FILE_GROUP_ACCESS,KEYTAB_FILE_CONFIGURATION);
      }
    }
  }
}
