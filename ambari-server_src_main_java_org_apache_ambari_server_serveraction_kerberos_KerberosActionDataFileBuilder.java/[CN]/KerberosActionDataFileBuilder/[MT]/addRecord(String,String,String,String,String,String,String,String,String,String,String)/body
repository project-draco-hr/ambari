{
  if (csvPrinter == null) {
    throw new IOException("Data file is not open");
  }
  csvPrinter.printRecord(hostName,serviceName,serviceComponentName,principal,principalConfiguration,keytabFilePath,keytabFileOwnerName,keytabFileOwnerAccess,keytabFileGroupName,keytabFileGroupAccess,keytabFileConfiguration);
}
