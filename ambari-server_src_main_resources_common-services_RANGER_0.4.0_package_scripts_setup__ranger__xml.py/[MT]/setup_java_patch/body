def setup_java_patch(rolling_upgrade=False):
    import params
    ranger_home = params.ranger_home
    if rolling_upgrade:
        ranger_home = format('/usr/hdp/{version}/ranger-admin')
    setup_java_patch = format('python {ranger_home}/db_setup.py -javapatch')
    Execute(setup_java_patch, environment={'RANGER_ADMIN_HOME': ranger_home, 'JAVA_HOME': params.java_home, }, logoutput=True, user=params.unix_user)
