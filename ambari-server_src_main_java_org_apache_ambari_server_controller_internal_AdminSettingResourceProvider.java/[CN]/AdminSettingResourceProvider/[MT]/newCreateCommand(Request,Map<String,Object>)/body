{
  return new Command<AdminSettingEntity>(){
    @Override public AdminSettingEntity invoke() throws AmbariException, AuthorizationException {
      AdminSettingEntity entity=toEntity(properties);
      if (dao.findByName(entity.getName()) != null) {
        throw new DuplicateResourceException(String.format("Setting already exists. setting name :%s ",entity.getName()));
      }
      dao.create(entity);
      notifyCreate(Resource.Type.AdminSetting,request);
      return entity;
    }
  }
;
}
