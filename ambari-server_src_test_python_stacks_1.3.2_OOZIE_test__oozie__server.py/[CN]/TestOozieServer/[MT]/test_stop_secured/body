def test_stop_secured(self):
    self.executeScript('1.3.2/services/OOZIE/package/scripts/oozie_server.py', classname='OozieServer', command='stop', config_file='secured.json')
    self.assertResourceCalled('Execute', "su -s /bin/bash - oozie -c  'cd /var/tmp/oozie && /usr/lib/oozie/bin/oozie-stop.sh' && rm -f /var/run/oozie/oozie.pid", only_if='ls /var/run/oozie/oozie.pid >/dev/null 2>&1 && ps `cat /var/run/oozie/oozie.pid` >/dev/null 2>&1')
    self.assertNoMoreResources()
