def getInstalledPkgsByRepo(self, repos, installedPackages):
    packagesFromRepo = []
    for repo in repos:
        subResult = []
        for item in installedPackages:
            if (repo == item[2]):
                subResult.append(item[0])
        packagesFromRepo = list(set((packagesFromRepo + subResult)))
    return packagesFromRepo
