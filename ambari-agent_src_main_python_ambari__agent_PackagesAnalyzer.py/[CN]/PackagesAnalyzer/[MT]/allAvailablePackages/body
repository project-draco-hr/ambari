def allAvailablePackages(self, allAvailablePackages):
    if self.hasZypper():
        return self.lookUpZypperPackages(['zypper', 'search', '--uninstalled-only', '--details'], allAvailablePackages)
    else:
        return self.lookUpYumPackages(['yum', 'list', 'available'], 'Available Packages', allAvailablePackages)
