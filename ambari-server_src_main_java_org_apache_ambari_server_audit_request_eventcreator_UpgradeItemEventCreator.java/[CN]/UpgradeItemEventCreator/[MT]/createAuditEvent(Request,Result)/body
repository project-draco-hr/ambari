{
  return UpdateUpgradeItemRequestAuditEvent.builder().withTimestamp(System.currentTimeMillis()).withRequestType(request.getRequestType()).withResultStatus(result.getStatus()).withUrl(request.getURI()).withRemoteIp(request.getRemoteAddress()).withStatus(RequestAuditEventCreatorHelper.getProperty(request,PropertyHelper.getPropertyId("UpgradeItem","status"))).withStageId(RequestAuditEventCreatorHelper.getProperty(request,UpgradeItemResourceProvider.UPGRADE_ITEM_STAGE_ID)).withRequestId(RequestAuditEventCreatorHelper.getProperty(request,UpgradeItemResourceProvider.UPGRADE_REQUEST_ID)).build();
}
