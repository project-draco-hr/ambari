{
  return UpdateUpgradeItemRequestAuditEvent.builder().withTimestamp(System.currentTimeMillis()).withRequestType(request.getRequestType()).withResultStatus(result.getStatus()).withUrl(request.getURI()).withRemoteIp(request.getRemoteAddress()).withStatus(getProperty(request,"status")).withStageId(getProperty(request,"stage_id")).withRequestId(getProperty(request,"request_id")).build();
}
