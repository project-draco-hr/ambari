def service_check(self, env):
    import params
    env.set_params(params)
    Execute(format('env JAVA_HOME={java_home} FALCON_LOG_DIR=/var/log/falcon FALCON_PID_DIR=/var/run/falcon FALCON_DATA_DIR={falcon_data_dir} {falcon_home}/bin/falcon admin -version'), user=params.smoke_user, logoutput=True, tries=3, try_sleep=20)
