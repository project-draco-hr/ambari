@patch.object(FileCache, 'provide_directory')
def test_get_service_base_dir(self, provide_directory_mock):
    provide_directory_mock.return_value = 'dummy value'
    fileCache = FileCache(self.config)
    command = {'commandParams': {'service_package_folder': 'HDP/2.1.1/services/ZOOKEEPER/package', }, }
    res = fileCache.get_service_base_dir(command, 'server_url_pref')
    self.assertEquals(pprint.pformat(provide_directory_mock.call_args_list[0][0]), "('/var/lib/ambari-agent/cache',\n 'stacks/HDP/2.1.1/services/ZOOKEEPER/package',\n 'server_url_pref')")
    self.assertEquals(res, 'dummy value')
