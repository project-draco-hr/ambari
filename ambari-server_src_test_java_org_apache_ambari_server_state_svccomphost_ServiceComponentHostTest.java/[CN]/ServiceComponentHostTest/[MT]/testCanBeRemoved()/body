{
  ServiceComponentHostImpl impl=(ServiceComponentHostImpl)createNewServiceComponentHost("HDFS","HDFS_CLIENT","h1",true);
  for (  State state : State.values()) {
    impl.setState(state);
    if (state.isRemovableState()) {
      Assert.assertTrue(impl.canBeRemoved());
    }
 else {
      Assert.assertFalse(impl.canBeRemoved());
    }
  }
}
