{
  m_version=version;
  m_originalStackId=sourceStackId;
switch (type) {
case ROLLING:
    m_effectiveStackId=targetStackId;
  break;
case NON_ROLLING:
m_effectiveStackId=(direction.isUpgrade()) ? sourceStackId : targetStackId;
break;
default :
m_effectiveStackId=targetStackId;
break;
}
m_targetStackId=targetStackId;
m_direction=direction;
m_resolver=resolver;
m_type=type;
}
