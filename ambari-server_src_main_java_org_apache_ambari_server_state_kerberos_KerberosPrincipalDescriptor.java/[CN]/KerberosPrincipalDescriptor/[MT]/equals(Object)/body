{
  if (object == null) {
    return false;
  }
 else   if (object == this) {
    return true;
  }
 else   if (object.getClass() == KerberosPrincipalDescriptor.class) {
    KerberosPrincipalDescriptor descriptor=(KerberosPrincipalDescriptor)object;
    return super.equals(object) && ((getConfiguration() == null) ? (descriptor.getConfiguration() == null) : getConfiguration().equals(descriptor.getConfiguration()));
  }
 else {
    return false;
  }
}
