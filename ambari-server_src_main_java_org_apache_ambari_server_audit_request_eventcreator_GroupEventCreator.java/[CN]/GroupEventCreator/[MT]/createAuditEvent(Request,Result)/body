{
  String username=((User)SecurityContextHolder.getContext().getAuthentication().getPrincipal()).getUsername();
switch (request.getRequestType()) {
case POST:
    return CreateGroupRequestAuditEvent.builder().withTimestamp(System.currentTimeMillis()).withRequestType(request.getRequestType()).withResultStatus(result.getStatus()).withUrl(request.getURI()).withRemoteIp(request.getRemoteAddress()).withUserName(username).withGroupName(getGroupName(request)).build();
case DELETE:
  return DeleteGroupRequestAuditEvent.builder().withTimestamp(System.currentTimeMillis()).withRequestType(request.getRequestType()).withResultStatus(result.getStatus()).withUrl(request.getURI()).withRemoteIp(request.getRemoteAddress()).withUserName(username).withGroupName(request.getResource().getKeyValueMap().get(Resource.Type.Group)).build();
default :
break;
}
return null;
}
