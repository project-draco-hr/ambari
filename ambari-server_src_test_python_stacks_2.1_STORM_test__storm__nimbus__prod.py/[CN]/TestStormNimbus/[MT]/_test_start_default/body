def _test_start_default(self):
    self.executeScript('2.1/services/STORM/package/scripts/nimbus_prod.py', classname='Nimbus', command='start', config_file='default.json')
    self.assert_configure_default()
    self.assertResourceCalled('Execute', 'supervisorctl start storm-nimbus', wait_for_finish=False)
    self.assertNoMoreResources()
