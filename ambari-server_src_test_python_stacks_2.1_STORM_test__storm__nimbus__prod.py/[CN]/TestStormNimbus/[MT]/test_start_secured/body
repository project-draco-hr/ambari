def test_start_secured(self):
    self.executeScript('2.1/services/STORM/package/scripts/nimbus_prod.py', classname='Nimbus', command='start', config_file='secured.json')
    self.assert_configure_secured()
    self.assertResourceCalled('Execute', 'supervisorctl start storm-nimbus', wait_for_finish=False)
    self.assertNoMoreResources()
