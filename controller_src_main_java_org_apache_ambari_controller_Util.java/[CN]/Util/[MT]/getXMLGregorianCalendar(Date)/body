{
  if (date == null) {
    return null;
  }
  GregorianCalendar cal=new GregorianCalendar();
  cal.setTime(date);
  try {
    return DatatypeFactory.newInstance().newXMLGregorianCalendar(cal);
  }
 catch (  NullPointerException ne) {
  }
catch (  DatatypeConfigurationException de) {
  }
  return null;
}
