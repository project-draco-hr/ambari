{
  boolean res=false;
  int activeMasters=0;
  for (  Map.Entry<ServiceComponentHost,Boolean> entry : scanResuls.entrySet()) {
    activeMasters+=(entry.getValue()) ? 1 : 0;
  }
  if (activeMasters == 0 || activeMasters > 1) {
    res=false;
  }
 else {
    res=true;
  }
  LOG.info("Results of HBASE_MASTER scan are " + ((res) ? "valid" : "invalid"));
  return res;
}
