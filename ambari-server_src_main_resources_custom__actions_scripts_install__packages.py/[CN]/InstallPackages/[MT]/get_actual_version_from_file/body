def get_actual_version_from_file(self):
    '\n    Search the repository version history file for a line that contains repository_version,actual_version\n    Notice that the parts are delimited by a comma.\n    :return: Return the actual_version if found, otherwise, return None.\n    '
    actual_version = None
    if os.path.isfile(self.REPO_VERSION_HISTORY_FILE):
        with open(self.REPO_VERSION_HISTORY_FILE, 'r') as f:
            for line in f.readlines():
                line_parts = line.split(',')
                if (line_parts and (len(line_parts) == 2) and (line_parts[0] == self.repository_version)):
                    item = line_parts[1].strip()
                    if (item != ''):
                        actual_version = item
                        break
    return actual_version
