def start(self, env, upgrade_type=None):
    import params
    env.set_params(params)
    self.configure(env)
    Execute(format('{solr_bindir}/solr start -cloud -noprompt -s {logsearch_solr_datadir} >> {logsearch_solr_log} 2>&1'), environment={'SOLR_INCLUDE': format('{logsearch_solr_conf}/logsearch-solr-env.sh'), }, user=params.logsearch_solr_user)
