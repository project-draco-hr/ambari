{
  if (null == transfers) {
    return Collections.<Transfer>emptyList();
  }
  List<Transfer> list=new ArrayList<Transfer>();
  for (  Transfer t : transfers) {
switch (t.operation) {
case COPY:
case MOVE:
      if (null != t.fromKey && null != t.toKey) {
        list.add(t);
      }
    break;
case DELETE:
  if (null != t.deleteKey) {
    list.add(t);
  }
break;
}
}
return list;
}
