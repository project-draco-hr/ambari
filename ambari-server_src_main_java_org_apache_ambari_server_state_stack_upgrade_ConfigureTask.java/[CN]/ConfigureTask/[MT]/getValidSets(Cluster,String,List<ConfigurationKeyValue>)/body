{
  List<ConfigurationKeyValue> allowedSets=new ArrayList<>();
  for (  ConfigurationKeyValue configurationKeyValue : sets) {
    if (isValidConditionSettings(cluster,configType,configurationKeyValue.key,configurationKeyValue.ifKey,configurationKeyValue.ifType,configurationKeyValue.ifValue,configurationKeyValue.ifKeyState))     allowedSets.add(configurationKeyValue);
  }
  return allowedSets;
}
