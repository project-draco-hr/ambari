{
  if (null == maintMap) {
    String entity=getHostStateEntity().getMaintenanceState();
    if (null == entity) {
      maintMap=new HashMap<Long,MaintenanceState>();
    }
 else {
      try {
        maintMap=gson.fromJson(entity,maintMapType);
      }
 catch (      Exception e) {
        maintMap=new HashMap<Long,MaintenanceState>();
      }
    }
  }
}
