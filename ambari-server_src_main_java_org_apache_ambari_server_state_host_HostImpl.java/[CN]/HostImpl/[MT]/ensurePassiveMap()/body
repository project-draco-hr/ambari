{
  if (null == passiveMap) {
    String entity=hostStateEntity.getPassiveState();
    if (null == entity) {
      passiveMap=new HashMap<Long,PassiveState>();
    }
 else {
      try {
        passiveMap=gson.fromJson(entity,passiveMapType);
      }
 catch (      Exception e) {
        passiveMap=new HashMap<Long,PassiveState>();
      }
    }
  }
}
