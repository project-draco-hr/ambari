def build(self):
    sh = shellRunner()
    list = []
    servers = sh.getServerTracker()
    for server in servers:
        (clusterId, bluePrintName, bluePrintRevision, component, role) = server.split('/')
        result = {'clusterId': clusterId, 'bluePrintName': bluePrintName, 'bluePrintRevision': bluePrintRevision, 'component': component, 'roleName': role, 'serverStatus': 'STARTED', }
        list.append(result)
    return list
