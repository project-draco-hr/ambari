def build(self):
    sh = shellRunner()
    list = []
    servers = sh.getServerTracker()
    for server in servers:
        (clusterId, clusterDefinitionRevision, component, role) = server.split('/')
        result = {'clusterId': clusterId, 'clusterDefinitionRevision': clusterDefinitionRevision, 'componentName': component, 'roleName': role, 'serverStatus': 'STARTED', }
        list.append(result)
    return list
