def ethtool(self):
    sh = shellRunner()
    script = ['ethtool', 'eth0', '|', 'grep', 'Speed:', '|', 'sed', "'s/\\s*Speed:\\s*//'", '|', 'sed', "'s/Mb\\/s//'"]
    result = sh.run(script)
    if ('ethtool: not found\n' in result['error']):
        self.netSpeed = 0
    else:
        self.netSpeed = int(result['output'].rstrip())
