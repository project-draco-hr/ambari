{
  try {
    return getClusterController().deleteResources(resource.getResourceDefinition().getType(),resource.getQuery().getPredicate());
  }
 catch (  AmbariException e) {
    throw new RuntimeException("Delete of resource failed: " + e,e);
  }
catch (  UnsupportedPropertyException e) {
    throw new RuntimeException("Delete of resource failed: " + e,e);
  }
}
