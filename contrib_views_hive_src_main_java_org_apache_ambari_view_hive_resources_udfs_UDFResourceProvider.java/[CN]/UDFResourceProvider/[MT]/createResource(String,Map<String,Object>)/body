{
  UDF item=null;
  try {
    item=new UDF(stringObjectMap);
  }
 catch (  InvocationTargetException e) {
    throw new SystemException("error on creating resource",e);
  }
catch (  IllegalAccessException e) {
    throw new SystemException("error on creating resource",e);
  }
  getResourceManager().create(item);
}
