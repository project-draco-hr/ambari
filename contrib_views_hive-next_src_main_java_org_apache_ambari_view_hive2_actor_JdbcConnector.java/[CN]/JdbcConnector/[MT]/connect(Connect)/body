{
  this.username=message.getUsername();
  if (connectable == null) {
    connectable=message.getConnectable();
  }
  try {
    if (!connectable.isOpen()) {
      connectable.connect();
    }
  }
 catch (  ConnectionException e) {
    this.notifyFailure();
    cleanUp();
    return;
  }
  this.terminateActorScheduler=system.scheduler().schedule(Duration.Zero(),Duration.create(60 * 1000,TimeUnit.MILLISECONDS),this.getSelf(),new TerminateInactivityCheck(),system.dispatcher(),null);
}
