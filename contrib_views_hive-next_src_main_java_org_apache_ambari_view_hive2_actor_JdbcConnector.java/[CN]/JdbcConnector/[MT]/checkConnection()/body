{
  if (connectable == null) {
    notifyConnectFailure(new SQLException("Hive connection is not created"));
    return false;
  }
  Optional<HiveConnection> connectionOptional=connectable.getConnection();
  if (!connectionOptional.isPresent()) {
    notifyConnectFailure(new SQLException("Hive connection is not created"));
    return false;
  }
  return true;
}
