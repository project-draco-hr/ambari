def to_json(self):
    '\n        create a map to hold the information of the Docker instance\n        :return: A map, which is JSON format object.\n        '
    docker_json = {}
    docker_json['weave_ip'] = '{0}/{1}'.format(self.ip, self.mask)
    docker_json['weave_domain_name'] = self.weave_domain_name
    return docker_json
