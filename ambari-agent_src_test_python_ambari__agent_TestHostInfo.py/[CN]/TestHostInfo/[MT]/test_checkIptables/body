@patch('subprocess.Popen')
def test_checkIptables(self, subproc_popen_mock):
    hostInfo = HostInfo()
    p = MagicMock()
    subproc_popen_mock.return_value = p
    result = hostInfo.checkIptables()
    self.assertTrue((result == True))
    result = hostInfo.checkIptables()
    self.assertFalse((result == False))
