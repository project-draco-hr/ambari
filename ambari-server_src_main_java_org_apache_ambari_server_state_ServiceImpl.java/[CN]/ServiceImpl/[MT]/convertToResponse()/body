{
  clusterGlobalLock.readLock().lock();
  try {
    readWriteLock.readLock().lock();
    try {
      ServiceResponse r=new ServiceResponse(cluster.getClusterId(),cluster.getClusterName(),getName(),getDesiredStackVersion().getStackId(),getDesiredState().toString());
      r.setPassiveState(getPassiveState().name());
      return r;
    }
  finally {
      readWriteLock.readLock().unlock();
    }
  }
  finally {
    clusterGlobalLock.readLock().unlock();
  }
}
