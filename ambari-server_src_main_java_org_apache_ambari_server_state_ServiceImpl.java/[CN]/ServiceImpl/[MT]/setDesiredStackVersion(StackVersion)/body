{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Setting DesiredStackVersion of Service" + ", clusterName=" + cluster.getClusterName() + ", clusterId="+ cluster.getClusterId()+ ", serviceName="+ serviceName+ ", oldDesiredStackVersion="+ this.desiredStackVersion+ ", newDesiredStackVersion="+ stackVersion);
  }
  this.desiredStackVersion=stackVersion;
}
