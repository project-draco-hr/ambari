{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Setting DesiredState of Service" + ", clusterName=" + cluster.getClusterName() + ", clusterId="+ cluster.getClusterId()+ ", serviceName="+ serviceName+ ", oldDesiredState="+ this.desiredState+ ", newDesiredState="+ state);
  }
  this.desiredState=state;
}
