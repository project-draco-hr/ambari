{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Setting DesiredState of Service" + ", clusterName=" + cluster.getClusterName() + ", clusterId="+ cluster.getClusterId()+ ", serviceName="+ getName()+ ", oldDesiredState="+ this.getDesiredState()+ ", newDesiredState="+ state);
  }
  this.serviceDesiredStateEntity.setDesiredState(state);
  saveIfPersisted();
}
