{
  List<Stage> stages=new ArrayList<Stage>();
  List<HostRoleStatus> statuses=Arrays.asList(HostRoleStatus.QUEUED,HostRoleStatus.IN_PROGRESS,HostRoleStatus.PENDING,HostRoleStatus.HOLDING,HostRoleStatus.HOLDING_FAILED,HostRoleStatus.HOLDING_TIMEDOUT);
  for (  StageEntity stageEntity : stageDAO.findByCommandStatuses(statuses)) {
    stages.add(stageFactory.createExisting(stageEntity));
  }
  return stages;
}
