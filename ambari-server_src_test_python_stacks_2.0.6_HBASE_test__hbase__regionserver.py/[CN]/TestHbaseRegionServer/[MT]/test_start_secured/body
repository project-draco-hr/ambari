def test_start_secured(self):
    self.executeScript('2.0.6/services/HBASE/package/scripts/hbase_regionserver.py', classname='HbaseRegionServer', command='start', config_file='secured.json')
    self.assert_configure_secured()
    self.assertResourceCalled('Execute', '/usr/lib/hbase/bin/hbase-daemon.sh --config /etc/hbase/conf start regionserver', not_if='ls /var/run/hbase/hbase-hbase-regionserver.pid >/dev/null 2>&1 && ps -p `cat /var/run/hbase/hbase-hbase-regionserver.pid` >/dev/null 2>&1', user='hbase')
    self.assertNoMoreResources()
