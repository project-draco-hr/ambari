{
  KerberosDescriptor defaultKerberosDescriptor=ambariMetaInfo.getKerberosDescriptor(currentStackVersion.getStackName(),currentStackVersion.getStackVersion());
  if (defaultKerberosDescriptor == null) {
    return kerberosDescriptor;
  }
 else {
    if (kerberosDescriptor != null) {
      defaultKerberosDescriptor.update(kerberosDescriptor);
    }
    return defaultKerberosDescriptor;
  }
}
