{
  return (desiredSecurityState == SecurityState.UNSECURED) && (maintenanceStateHelper.getEffectiveState(sch) == MaintenanceState.OFF) && ((sch.getDesiredSecurityState() != SecurityState.UNSECURED) || (sch.getSecurityState() != SecurityState.UNSECURED))&& (sch.getSecurityState() != SecurityState.UNSECURING);
}
