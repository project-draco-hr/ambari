{
  Stage stage=createServerActionStage(requestStageContainer.getLastStageId(),cluster,requestStageContainer.getId(),"Update Configurations",clusterHostInfoJson,"{}",hostParamsJson,UpdateKerberosConfigsServerAction.class,event,commandParameters,"Update Service Configurations",1200);
  RoleGraph roleGraph=new RoleGraph(roleCommandOrder);
  roleGraph.build(stage);
  requestStageContainer.addStages(roleGraph.getStages());
}
