{
  Stage stage=createServerActionStage(requestStageContainer.getLastStageId(),cluster,requestStageContainer.getId(),"Create Keytabs",clusterHostInfoJson,"{}",hostParamsJson,CreateKeytabFilesServerAction.class,event,commandParameters,"Create Keytabs",1200);
  RoleGraph roleGraph=new RoleGraph(roleCommandOrder);
  roleGraph.build(stage);
  requestStageContainer.addStages(roleGraph.getStages());
}
