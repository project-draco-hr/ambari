def test_service_check(self):
    self.executeScript('2.1.1/services/FALCON/package/scripts/service_check.py', classname='FalconServiceCheck', command='service_check', config_file='default.json')
    self.assertResourceCalled('Execute', 'env JAVA_HOME=/usr/jdk64/jdk1.7.0_45 FALCON_LOG_DIR=/var/log/falcon FALCON_PID_DIR=/var/run/falcon FALCON_DATA_DIR=/hadoop/falcon/activemq /usr/lib/falcon/bin/falcon admin -version', logoutput=True, tries=3, user='ambari-qa', try_sleep=20)
    self.assertNoMoreResources()
