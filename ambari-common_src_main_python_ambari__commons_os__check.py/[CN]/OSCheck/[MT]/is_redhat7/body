@staticmethod
def is_redhat7():
    "\n     Return true if it is so or false if not\n\n     This is safe check for redhat7 , doesn't generate exception\n    "
    try:
        ostemp = (OSCheck.get_os_family() + OSCheck().get_os_major_version())
        if (ostemp == 'redhat7'):
            return True
    except Exception:
        pass
    return False
