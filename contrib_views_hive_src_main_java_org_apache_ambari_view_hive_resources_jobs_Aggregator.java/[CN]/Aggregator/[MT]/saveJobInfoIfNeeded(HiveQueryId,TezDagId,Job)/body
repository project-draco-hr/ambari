{
  if (viewJob.getDagName() == null) {
    viewJob.setDagName(tezDagId.dagName);
    viewJobResourceManager.update(viewJob,viewJob.getId());
  }
  if (viewJob.getStatus().equals(tezDagId.status)) {
    viewJob.setStatus(tezDagId.status);
    viewJobResourceManager.update(viewJob,viewJob.getId());
  }
}
