{
  if (!metricName.equals(metric.metricName))   return false;
  if (hostName != null ? !hostName.equals(metric.hostName) : metric.hostName != null)   return false;
  if (!appId.equals(metric.appId))   return false;
  if (instanceId != null ? !instanceId.equals(metric.instanceId) : metric.instanceId != null)   return false;
  return true;
}
