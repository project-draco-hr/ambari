{
  if (getPropertyIds().isEmpty()) {
    return true;
  }
  String clusterName=(String)resource.getPropertyValue(clusterNamePropertyId);
  if (hostProvider.getGangliaCollectorHostName(clusterName) == null) {
    if (LOG.isWarnEnabled()) {
      LOG.warn("Attempting to get metrics but the Ganglia server is unknown. Resource=" + resource + " : Cluster="+ clusterName);
    }
    return true;
  }
  setProperties(resource,clusterName,request,PropertyHelper.getRequestPropertyIds(getPropertyIds(),request,predicate));
  return true;
}
