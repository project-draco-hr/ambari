{
  String query=dbmsHelper.getTableConstraintsStatement(connection.getCatalog(),tableName);
  ResultSet rs=executeSelect(query);
  if (rs != null) {
    while (rs.next()) {
      if (rs.getString("CONSTRAINT_NAME").equalsIgnoreCase(constraintName)) {
        return true;
      }
    }
  }
  return false;
}
