{
  ResultSet rs=getDatabaseMetaData().getPrimaryKeys(null,null,convertObjectName(tableName));
  if (rs != null && columnName != null) {
    while (rs.next()) {
      if (rs.getString("COLUMN_NAME").equalsIgnoreCase(columnName)) {
        return true;
      }
    }
  }
 else   if (rs != null) {
    return rs.next();
  }
  return false;
}
