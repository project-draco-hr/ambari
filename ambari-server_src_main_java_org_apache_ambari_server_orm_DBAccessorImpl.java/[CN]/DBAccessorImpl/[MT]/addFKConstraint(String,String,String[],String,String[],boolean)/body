{
  String query=dbmsHelper.getAddForeignKeyStatement(tableName,constraintName,Arrays.asList(keyColumns),referenceTableName,Arrays.asList(referenceColumns));
  try {
    executeQuery(query);
  }
 catch (  SQLException e) {
    LOG.warn("Add FK constraint failed" + ", constraintName = " + constraintName + ", tableName = "+ tableName,e);
    if (!ignoreFailure) {
      throw e;
    }
  }
}
