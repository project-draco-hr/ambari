def test_service_check_default(self):
    self.executeScript('2.0.6/services/SQOOP/package/scripts/service_check.py', classname='SqoopServiceCheck', command='service_check', config_file='default.json')
    self.assertResourceCalled('Execute', 'sqoop version', logoutput=True, path=['/usr/bin'], user='ambari-qa')
    self.assertNoMoreResources()
