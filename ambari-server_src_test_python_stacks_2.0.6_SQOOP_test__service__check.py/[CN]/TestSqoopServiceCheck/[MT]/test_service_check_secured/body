def test_service_check_secured(self):
    self.executeScript('2.0.6/services/SQOOP/package/scripts/service_check.py', classname='SqoopServiceCheck', command='service_check', config_file='secured.json')
    self.assertResourceCalled('Execute', '/usr/bin/kinit  -kt /etc/security/keytabs/smokeuser.headless.keytab ambari-qa')
    self.assertResourceCalled('Execute', 'sqoop version', logoutput=True, path=['/usr/bin'], user='ambari-qa')
    self.assertNoMoreResources()
