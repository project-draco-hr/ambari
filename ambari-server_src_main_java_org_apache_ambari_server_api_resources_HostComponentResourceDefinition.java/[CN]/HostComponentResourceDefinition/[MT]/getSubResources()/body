{
  Map<String,ResourceDefinition> mapChildren=new HashMap<String,ResourceDefinition>();
  ComponentResourceDefinition componentResource=new ComponentResourceDefinition(getId(),m_clusterId,null);
  PropertyId serviceIdProperty=getClusterController().getSchema(Resource.Type.Component).getKeyPropertyId(Resource.Type.Service);
  componentResource.getQuery().addProperty(serviceIdProperty);
  mapChildren.put(componentResource.getSingularName(),componentResource);
  return mapChildren;
}
