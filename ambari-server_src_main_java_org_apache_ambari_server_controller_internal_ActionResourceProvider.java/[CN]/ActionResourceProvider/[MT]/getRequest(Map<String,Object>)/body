{
  Map<String,String> params=new HashMap<String,String>();
  Iterator<Entry<String,Object>> it1=properties.entrySet().iterator();
  while (it1.hasNext()) {
    Entry<String,Object> entry=it1.next();
    String propertyid=entry.getKey();
    if (PropertyHelper.getPropertyCategory(propertyid).equals("parameters") && null != entry.getValue()) {
      params.put(PropertyHelper.getPropertyName(propertyid),entry.getValue().toString());
    }
  }
  return new ActionRequest((String)properties.get(ACTION_CLUSTER_NAME_PROPERTY_ID),(String)properties.get(ACTION_SERVICE_NAME_PROPERTY_ID),(String)properties.get(ACTION_ACTION_NAME_PROPERTY_ID),params);
}
