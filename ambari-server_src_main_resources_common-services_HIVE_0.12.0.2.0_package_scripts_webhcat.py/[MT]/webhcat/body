@OsFamilyFuncImpl(os_family=OSConst.WINSRV_FAMILY)
def webhcat():
    import params
    XmlConfig('webhcat-site.xml', conf_dir=params.hcat_config_dir, configurations=params.config['configurations']['webhcat-site'])
    ServiceConfig(params.webhcat_server_win_service_name, action='change_user', username=params.hcat_user, password=Script.get_password(params.hcat_user))
