def test_that_jce_is_required_in_secured_cluster(self):
    try:
        self.executeScript('1.3.2/hooks/before-INSTALL/scripts/hook.py', classname='BeforeConfigureHook', command='hook', config_file='secured_no_jce_name.json')
        self.fail('Should throw an exception')
    except Fail:
        pass
