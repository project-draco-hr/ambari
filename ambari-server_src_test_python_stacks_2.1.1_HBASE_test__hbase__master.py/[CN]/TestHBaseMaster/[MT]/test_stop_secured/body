def test_stop_secured(self):
    self.executeScript('2.1.1/services/HBASE/package/scripts/hbase_master.py', classname='HbaseMaster', command='stop', config_file='secured.json')
    self.assertResourceCalled('Execute', '/usr/lib/hbase/bin/hbase-daemon.sh --config /etc/hbase/conf stop master && rm -f /var/run/hbase/hbase-hbase-master.pid', not_if=None, user='hbase')
    self.assertNoMoreResources()
