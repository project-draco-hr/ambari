def test_port_alert(self):
    json = {'name': 'namenode_process', 'service': 'HDFS', 'component': 'NAMENODE', 'label': 'NameNode process', 'interval': 6, 'scope': 'host', 'source': {'type': 'PORT', 'uri': 'http://c6409.ambari.apache.org:50070', 'default_port': 50070, 'reporting': {'ok': {'text': 'TCP OK - {0:.4f} response time on port {1}', }, 'critical': {'text': 'Could not load process info: {0}', }, }, }, }
    collector = AlertCollector()
    pa = PortAlert(collector, json, json['source'])
    self.assertEquals(6, pa.interval())
    res = pa.collect()
    pass
