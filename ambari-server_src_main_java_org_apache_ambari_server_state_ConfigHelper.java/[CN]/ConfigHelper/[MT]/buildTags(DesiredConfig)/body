{
  Map<String,String> map=new LinkedHashMap<String,String>();
  map.put("tag",dc.getVersion());
  if (null != dc.getServiceName())   map.put("service_override_tag",dc.getServiceName());
  if (0 != dc.getHostOverrides().size())   map.put("host_override_tag",dc.getHostOverrides().get(0).getVersionTag());
  return map;
}
