{
  clusterGlobalLock.readLock().lock();
  try {
    readLock.lock();
    SecurityType securityType=null;
    try {
      securityType=clusterEntity.getSecurityType();
      if (null == securityType) {
        securityType=SecurityType.NONE;
      }
      return securityType;
    }
  finally {
      readLock.unlock();
    }
  }
  finally {
    clusterGlobalLock.readLock().unlock();
  }
}
