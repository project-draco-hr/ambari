{
  sb.append("Cluster={ clusterName=" + getClusterName() + ", clusterId="+ getClusterId()+ ", desiredStackVersion="+ desiredStackVersion.getStackId()+ ", services=[ ");
  boolean first=true;
  for (  Service s : services.values()) {
    if (!first) {
      sb.append(" , ");
      first=false;
    }
    sb.append("\n    ");
    s.debugDump(sb);
    sb.append(" ");
  }
  sb.append(" ] }");
}
