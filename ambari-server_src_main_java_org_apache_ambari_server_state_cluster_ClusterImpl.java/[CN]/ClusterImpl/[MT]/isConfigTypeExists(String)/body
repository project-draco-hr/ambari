{
  clusterGlobalLock.readLock().lock();
  try {
    for (    ClusterConfigMappingEntity e : clusterDAO.getClusterConfigMappingEntitiesByCluster(getClusterId())) {
      if (e.getType().equals(configType)) {
        return true;
      }
    }
    return false;
  }
  finally {
    clusterGlobalLock.readLock().unlock();
  }
}
