{
  ResourceEntity resourceEntity=clusterEntity.getResource();
  if (resourceEntity != null) {
    Integer permissionId=privilegeEntity.getPermission().getId();
    if (privilegeEntity.getResource().equals(resourceEntity)) {
      if ((readOnly && permissionId.equals(PermissionEntity.CLUSTER_READ_PERMISSION)) || permissionId.equals(PermissionEntity.CLUSTER_OPERATE_PERMISSION)) {
        return true;
      }
    }
  }
  return false;
}
