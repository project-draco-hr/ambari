{
  loadServices();
  clusterGlobalLock.readLock().lock();
  try {
    readLock.lock();
    try {
      if (!services.containsKey(serviceName)) {
        throw new ServiceNotFoundException(getClusterName(),serviceName);
      }
      return services.get(serviceName);
    }
  finally {
      readLock.unlock();
    }
  }
  finally {
    clusterGlobalLock.readLock().unlock();
  }
}
