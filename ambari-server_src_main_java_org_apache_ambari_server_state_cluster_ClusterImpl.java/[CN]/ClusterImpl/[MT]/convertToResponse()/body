{
  clusterGlobalLock.readLock().lock();
  try {
    readWriteLock.readLock().lock();
    try {
      ClusterResponse r=new ClusterResponse(getClusterId(),getClusterName(),clusters.getHostsForCluster(getClusterName()).keySet(),getDesiredStackVersion().getStackId());
      return r;
    }
  finally {
      readWriteLock.readLock().unlock();
    }
  }
  finally {
    clusterGlobalLock.readLock().unlock();
  }
}
