{
  clusterGlobalLock.readLock().lock();
  try {
    readWriteLock.readLock().lock();
    try {
      Map<String,Host> hosts=clusters.getHostsForCluster(getClusterName());
      ClusterResponse r=new ClusterResponse(getClusterId(),getClusterName(),getProvisioningState(),hosts.keySet(),hosts.size(),getDesiredStackVersion().getStackId(),getClusterHealthReport());
      return r;
    }
  finally {
      readWriteLock.readLock().unlock();
    }
  }
  finally {
    clusterGlobalLock.readLock().unlock();
  }
}
