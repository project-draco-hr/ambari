{
switch (wrapper.getType()) {
case RESTART:
    makeRestartStage(cluster,request,version,entity,wrapper);
  break;
case RU_TASKS:
makeActionStage(cluster,request,version,entity,wrapper);
break;
case SERVICE_CHECK:
makeServiceCheckStage(cluster,request,version,entity,wrapper);
break;
default :
break;
}
}
