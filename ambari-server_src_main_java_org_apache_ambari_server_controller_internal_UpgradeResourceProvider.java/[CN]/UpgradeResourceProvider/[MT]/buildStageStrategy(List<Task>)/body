{
  if (null == tasks)   return Collections.emptyList();
  List<TaskHolder> holders=new ArrayList<TaskHolder>();
  TaskHolder holder=new TaskHolder();
  holders.add(holder);
  int i=0;
  for (  Task t : tasks) {
    if (i > 0 && t.getType().isManual() != tasks.get(i - 1).getType().isManual()) {
      holder=new TaskHolder();
      holders.add(holder);
    }
    holder.tasks.add(t);
    i++;
  }
  return holders;
}
