{
  StringBuilder builder=new StringBuilder();
  builder.append("[");
  boolean first=true;
  for (  String host : hosts) {
    if (!first) {
      builder.append(",");
    }
 else {
      first=false;
    }
    builder.append("'");
    builder.append(host);
    builder.append("'");
  }
  return builder.toString();
}
