def get_ambari_jars():
    try:
        conf_dir = os.environ[AMBARI_SERVER_LIB]
        return conf_dir
    except KeyError:
        default_jar_location = '/usr/lib/ambari-server'
        print_info_msg(((AMBARI_SERVER_LIB + ' is not set, using default ') + default_jar_location))
        return default_jar_location
