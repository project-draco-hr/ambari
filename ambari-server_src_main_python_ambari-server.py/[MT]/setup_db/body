def setup_db(args):
    dbname = args.postgredbname
    username = args.postgreuser
    file = args.init_script_file
    command = SETUP_DB_CMD.format(dbname, username, file)
    (retcode, outdata, errdata) = run_os_command(command)
    if (not (retcode == 0)):
        print errdata
    return retcode
