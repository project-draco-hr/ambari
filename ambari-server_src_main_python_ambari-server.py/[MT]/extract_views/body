def extract_views():
    jdk_path = find_jdk()
    if (jdk_path is None):
        print_error_msg(('No JDK found, please run the "setup" command to install a JDK automatically or install any JDK manually to ' + JDK_INSTALL_DIR))
        return 1
    command = VIEW_EXTRACT_CMD.format(jdk_path, get_conf_dir(), get_ambari_classpath())
    (retcode, stdout, stderr) = run_os_command(command)
    print_info_msg(('Return code from view extraction: ' + str(retcode)))
    return retcode
