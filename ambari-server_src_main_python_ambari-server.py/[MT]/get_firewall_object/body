def get_firewall_object():
    if (OS == OS_UBUNTU):
        return UbuntuFirewallChecks()
    elif ((OS == OS_FEDORA) and (int(OS_VERSION) >= 18)):
        return Fedora18FirewallChecks()
    elif (OS == OS_OPENSUSE):
        return OpenSuseFirewallChecks()
    else:
        return FirewallChecks()
