def check_iptables():
    (retcode, out, err) = run_os_command(IP_TBLS_ST_CMD)
    " This check doesn't work on CentOS 6.2 if firewall AND iptables service are running\n  if out == IP_TBLS_ENABLED:\n    print 'iptables is enabled now'\n    print 'Stopping iptables service'\n  "
    (retcode, out, err) = run_os_command(IP_TBLS_STOP_CMD)
    print 'iptables is disabled now'
    if ((not (retcode == 0)) and err and (len(err) > 0)):
        print err
    if (err.strip() == IP_TBLS_SRVC_NT_FND):
        return 0
    else:
        return (retcode, out)
