def find_jdk():
    if get_JAVA_HOME():
        return get_JAVA_HOME()
    print ('Looking for available JDKs at ' + JDK_INSTALL_DIR)
    jdks = glob.glob(((JDK_INSTALL_DIR + os.sep) + 'jdk*'))
    jdks.sort()
    print ('Found: ' + str(jdks))
    count = len(jdks)
    if (count == 0):
        return
    jdkPath = jdks[(count - 1)]
    print 'Selected JDK {0}'.format(jdkPath)
    return jdkPath
