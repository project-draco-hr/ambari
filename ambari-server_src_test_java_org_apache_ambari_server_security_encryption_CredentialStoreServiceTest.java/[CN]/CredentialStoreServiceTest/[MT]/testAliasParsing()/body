{
  String strPasswd="${alias=ambari.password}";
  Pattern PASSWORD_ALIAS_PATTERN=Pattern.compile("\\$\\{alias=[\\w\\.]+\\}");
  Matcher matcher=PASSWORD_ALIAS_PATTERN.matcher(strPasswd);
  Assert.assertTrue(matcher.matches());
  Assert.assertEquals("ambari.password",strPasswd.substring(strPasswd.indexOf("=") + 1,strPasswd.length() - 1));
}
