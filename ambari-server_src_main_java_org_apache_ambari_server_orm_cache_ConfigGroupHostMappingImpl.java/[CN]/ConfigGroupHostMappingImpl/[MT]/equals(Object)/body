{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  ConfigGroupHostMappingImpl other=(ConfigGroupHostMappingImpl)obj;
  if (configGroup == null) {
    if (other.configGroup != null)     return false;
  }
 else   if (!configGroup.equals(other.configGroup))   return false;
  if (configGroupId == null) {
    if (other.configGroupId != null)     return false;
  }
 else   if (!configGroupId.equals(other.configGroupId))   return false;
  if (host == null) {
    if (other.host != null)     return false;
  }
 else   if (!host.equals(other.host))   return false;
  if (hostname == null) {
    if (other.hostname != null)     return false;
  }
 else   if (!hostname.equals(other.hostname))   return false;
  return true;
}
