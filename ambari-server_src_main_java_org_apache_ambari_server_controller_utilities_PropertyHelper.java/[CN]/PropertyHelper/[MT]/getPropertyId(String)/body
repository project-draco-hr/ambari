{
  String category;
  String name;
  int lastPathSep=absProperty.lastIndexOf(EXTERNAL_PATH_SEP);
  if (lastPathSep == -1) {
    category=null;
    name=absProperty;
  }
 else {
    category=absProperty.substring(0,lastPathSep);
    name=absProperty.substring(lastPathSep + 1);
  }
  return getPropertyId(name,category);
}
