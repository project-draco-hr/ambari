{
  String response;
  try {
    InputStream responseInputStream=context.getURLStreamProvider().readAsCurrent(atsUrl,"GET",(String)null,new HashMap<String,String>());
    response=IOUtils.toString(responseInputStream);
  }
 catch (  IOException e) {
    LOG.error("Error while reading from ATS",e);
    response=defaultResponse;
  }
  return response;
}
