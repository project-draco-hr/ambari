def __getitem__(self, name):
    '\n    Use Python types\n    '
    value = super(ConfigDictionary, self).__getitem__(name)
    if (value == 'true'):
        value = True
    elif (value == 'false'):
        value = False
    else:
        try:
            value = int(value)
        except (ValueError, TypeError):
            try:
                value = float(value)
            except (ValueError, TypeError):
                pass
    return value
