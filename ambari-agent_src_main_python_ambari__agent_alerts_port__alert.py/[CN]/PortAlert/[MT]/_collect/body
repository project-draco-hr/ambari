def _collect(self):
    s = None
    try:
        s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        s.settimeout(1.5)
        t = time.time()
        s.connect((self.host, self.port))
        millis = (time.time() - t)
        return (self.RESULT_OK, [(millis / 1000), self.port])
    finally:
        if (s is not None):
            try:
                s.close()
            except:
                pass
