{
  super(propertyIds,keyPropertyIds,managementController);
  EnumSet<RoleAuthorization> roleAuthorizations=EnumSet.of(RoleAuthorization.AMBARI_MANAGE_GROUPS,RoleAuthorization.AMBARI_MANAGE_USERS);
  setRequiredCreateAuthorizations(roleAuthorizations);
  setRequiredDeleteAuthorizations(roleAuthorizations);
}
