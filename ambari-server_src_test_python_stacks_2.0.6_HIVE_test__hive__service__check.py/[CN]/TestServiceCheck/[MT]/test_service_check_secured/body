@patch('sys.exit')
def test_service_check_secured(self, sys_exit_mock):
    self.executeScript('2.0.6/services/HIVE/package/scripts/service_check.py', classname='HiveServiceCheck', command='service_check', config_file='secured.json')
    self.assertResourceCalled('File', '/tmp/hcatSmoke.sh', content=StaticFile('hcatSmoke.sh'), mode=493)
    self.assertResourceCalled('Execute', '/usr/bin/kinit -kt /etc/security/keytabs/smokeuser.headless.keytab ambari-qa; env JAVA_HOME=/usr/jdk64/jdk1.7.0_45 /tmp/hcatSmoke.sh hcatsmoke prepare', logoutput=True, path=['/usr/sbin', '/usr/local/nin', '/bin', '/usr/bin'], tries=3, user='ambari-qa', try_sleep=5)
    self.assertResourceCalled('ExecuteHadoop', 'fs -test -e /apps/hive/warehouse/hcatsmoke', logoutput=True, user='hdfs', conf_dir='/etc/hadoop/conf', keytab='/etc/security/keytabs/hdfs.headless.keytab', kinit_path_local='/usr/bin/kinit', security_enabled=True)
    self.assertResourceCalled('Execute', '/usr/bin/kinit -kt /etc/security/keytabs/smokeuser.headless.keytab ambari-qa;  /tmp/hcatSmoke.sh hcatsmoke cleanup', logoutput=True, path=['/usr/sbin', '/usr/local/nin', '/bin', '/usr/bin'], tries=3, user='ambari-qa', try_sleep=5)
    self.assertNoMoreResources()
