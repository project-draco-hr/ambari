def test_service_check(self):
    self.executeScript('2.0.6/services/FLUME/package/scripts/flume_check.py', classname='FlumeServiceCheck', command='service_check', config_file='default.json')
    self.assertResourceCalled('Execute', 'env JAVA_HOME=/usr/jdk64/jdk1.7.0_45 /usr/bin/flume-ng version', logoutput=True, tries=3, try_sleep=20)
    self.assertNoMoreResources()
