{
  return new ServiceComponentHostRequest((String)properties.get(HOST_COMPONENT_CLUSTER_NAME_PROPERTY_ID),(String)properties.get(HOST_COMPONENT_SERVICE_NAME_PROPERTY_ID),(String)properties.get(HOST_COMPONENT_COMPONENT_NAME_PROPERTY_ID),(String)properties.get(HOST_COMPONENT_HOST_NAME_PROPERTY_ID),null,(String)properties.get(HOST_COMPONENT_STATE_PROPERTY_ID));
}
