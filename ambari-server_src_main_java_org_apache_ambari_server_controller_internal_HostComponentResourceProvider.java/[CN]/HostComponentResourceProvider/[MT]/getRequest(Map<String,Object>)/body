{
  ServiceComponentHostRequest serviceComponentHostRequest=new ServiceComponentHostRequest((String)properties.get(HOST_COMPONENT_CLUSTER_NAME_PROPERTY_ID),(String)properties.get(HOST_COMPONENT_SERVICE_NAME_PROPERTY_ID),(String)properties.get(HOST_COMPONENT_COMPONENT_NAME_PROPERTY_ID),(String)properties.get(HOST_COMPONENT_HOST_NAME_PROPERTY_ID),(String)properties.get(HOST_COMPONENT_STATE_PROPERTY_ID));
  serviceComponentHostRequest.setDesiredStackId((String)properties.get(HOST_COMPONENT_STACK_ID_PROPERTY_ID));
  return serviceComponentHostRequest;
}
