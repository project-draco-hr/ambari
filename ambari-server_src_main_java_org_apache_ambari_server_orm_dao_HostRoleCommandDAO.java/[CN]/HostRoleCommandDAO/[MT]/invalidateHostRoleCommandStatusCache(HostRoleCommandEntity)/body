{
  if (!hostRoleCommandStatusSummaryCacheEnabled)   return;
  if (hostRoleCommandEntity != null) {
    Long requestId=hostRoleCommandEntity.getRequestId();
    if (requestId == null) {
      StageEntity stageEntity=hostRoleCommandEntity.getStage();
      if (stageEntity != null)       requestId=stageEntity.getRequestId();
    }
    if (requestId != null)     invalidateHostRoleCommandStatusSummaryCache(requestId.longValue());
  }
}
