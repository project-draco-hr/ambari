{
  String queryStr="SELECT DISTINCT command.requestId " + " FROM HostRoleCommandEntity command WHERE " + " command.status";
  if (!match) {
    queryStr+=" NOT";
  }
  queryStr+=" IN ?1" + " ORDER BY command.requestId DESC";
  TypedQuery<Long> query=entityManagerProvider.get().createQuery(queryStr,Long.class);
  return daoUtils.selectList(query,statuses);
}
