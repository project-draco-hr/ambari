{
  TypedQuery<HostRoleCommandEntity> query=entityManagerProvider.get().createQuery("SELECT command " + "FROM HostRoleCommandEntity command " + "WHERE command.hostEntity.hostName=?1 AND command.requestId=?2 "+ "AND command.stageId=?3 AND command.role=?4 "+ "ORDER BY command.taskId",HostRoleCommandEntity.class);
  return daoUtils.selectList(query,hostName,requestId,stageId,role);
}
