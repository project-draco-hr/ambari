{
  final DBAccessor dbAccessor=createNiceMock(DBAccessor.class);
  UpgradeCatalog220 upgradeCatalog=(UpgradeCatalog220)getUpgradeCatalog(dbAccessor);
  expect(dbAccessor.executeUpdate("UPDATE adminpermission SET permission_label='Administrator' WHERE permission_id=1")).andReturn(1).once();
  expect(dbAccessor.executeUpdate("UPDATE adminpermission SET permission_label='Read-Only' WHERE permission_id=2")).andReturn(1).once();
  expect(dbAccessor.executeUpdate("UPDATE adminpermission SET permission_label='Operator' WHERE permission_id=3")).andReturn(1).once();
  expect(dbAccessor.executeUpdate("UPDATE adminpermission SET permission_label='Use View' WHERE permission_id=4")).andReturn(1).once();
  replay(dbAccessor);
  upgradeCatalog.executeDMLUpdates();
  verify(dbAccessor);
}
