{
  String modifiedUIField="";
  String temp=serviceLogsColumnMapping.get(uiField + LogSearchConstants.UI_SUFFIX);
  if (temp == null)   return uiField;
 else {
    String lastChar=uiField.substring(uiField.length() - 1,uiField.length());
    int k=1;
    try {
      k=Integer.parseInt(lastChar);
      k=k + 1;
      modifiedUIField=uiField.substring(0,uiField.length() - 2);
    }
 catch (    Exception e) {
    }
    modifiedUIField=uiField + "_" + k;
  }
  return getModifiedUIField(modifiedUIField);
}
