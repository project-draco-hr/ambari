def link_metrics_sink_jar(self):
    Link(format('{storm_lib_dir}/ambari-metrics-storm-sink.jar'), action='delete')
    Link('/usr/lib/storm/lib/ambari-metrics-storm-sink.jar', action='delete')
    Execute(format('{sudo} ln -s {metric_collector_sink_jar} {storm_lib_dir}/ambari-metrics-storm-sink.jar'), not_if=format('ls {storm_lib_dir}/ambari-metrics-storm-sink.jar'), only_if=format('ls {metric_collector_sink_jar}'))
