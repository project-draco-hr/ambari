def test_recommendTezConfigurations_amMemoryMoreThan3072(self):
    configurations = {}
    clusterData = {'mapMemory': 4000, 'amMemory': 3100, 'reduceMemory': 2056, 'containers': 3, 'ramPerContainer': 256, }
    expected = {'tez-site': {'properties': {'tez.am.resource.memory.mb': '3100', 'tez.am.java.opts': '-server -Xmx2480m -Djava.net.preferIPv4Stack=true -XX:+UseNUMA -XX:+UseParallelGC', 'tez.task.resource.memory.mb': '768', 'tez.runtime.io.sort.mb': '307', 'tez.runtime.unordered.output.buffer.size-mb': '57', }, }, }
    self.stackAdvisor.recommendTezConfigurations(configurations, clusterData, None, None)
    self.assertEquals(configurations, expected)
