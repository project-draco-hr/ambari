def get_unmanged_ad_use_case():
    config_file = 'stacks/2.2/configs/default.json'
    with open(config_file, 'r') as f:
        json_data = json.load(f)
    json_data['configurations']['kerberos-env'] = {'kdc_type': 'active-directory', }
    json_data['configurations']['krb5-conf'] = {'libdefaults_default_tgs_enctypes': 'aes256-cts-hmac-sha1-96', 'libdefaults_default_tkt_enctypes': 'aes256-cts-hmac-sha1-96', 'conf_dir': '/tmp', 'conf_file': 'krb5_ad.conf', 'content': krb5_conf_template, 'realm': 'OSCORPINDUSTRIES.COM', 'kdc_host': 'ad.oscorp_industries.com', 'admin_principal': 'admin/admin', 'admin_password': 'hadoop', }
    json_data['configurations']['kdc-conf'] = {'content': kdc_conf_template, }
    json_data['configurations']['kadm5-acl'] = {'content': kadm5_acl_template, }
    return json_data
