def get_manged_kdc_use_case():
    config_file = 'stacks/2.2/configs/default.json'
    with open(config_file, 'r') as f:
        json_data = json.load(f)
    json_data['clusterHostInfo']['kdc_server_hosts'] = ['c6401.ambari.apache.org']
    json_data['configurations']['krb5-conf'] = {'libdefaults_default_tgs_enctypes': 'aes256-cts-hmac-sha1-96', 'libdefaults_default_tkt_enctypes': 'aes256-cts-hmac-sha1-96', 'realm': 'MANAGED_REALM.COM', 'kdc_type': 'mit-kdc', 'kdc_host': 'c6401.ambari.apache.org', 'admin_principal': 'admin/admin', 'admin_password': 'hadoop', }
