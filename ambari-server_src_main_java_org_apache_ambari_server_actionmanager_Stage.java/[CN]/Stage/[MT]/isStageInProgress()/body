{
  for (  String host : hostActions.keySet()) {
    for (    HostRoleCommand role : hostActions.get(host).getRoleCommands()) {
      if (role.getStatus().equals(HostRoleStatus.PENDING) || role.getStatus().equals(HostRoleStatus.QUEUED) || role.getStatus().equals(HostRoleStatus.IN_PROGRESS)) {
        return true;
      }
    }
  }
  return false;
}
