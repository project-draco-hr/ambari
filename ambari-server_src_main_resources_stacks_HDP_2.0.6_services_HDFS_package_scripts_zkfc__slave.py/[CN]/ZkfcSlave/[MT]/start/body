def start(self, env):
    import params
    env.set_params(params)
    self.configure(env)
    Directory(params.hadoop_pid_dir_prefix, mode=493, owner=params.hdfs_user, group=params.user_group)
    service(action='start', name='zkfc', user=params.hdfs_user, create_pid_dir=True, create_log_dir=True)
