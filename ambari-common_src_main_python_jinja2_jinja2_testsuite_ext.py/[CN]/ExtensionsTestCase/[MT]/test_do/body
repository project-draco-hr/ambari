def test_do(self):
    env = Environment(extensions=['jinja2.ext.do'])
    tmpl = env.from_string('\n            {%- set items = [] %}\n            {%- for char in "foo" %}\n                {%- do items.append(loop.index0 ~ char) %}\n            {%- endfor %}{{ items|join(\', \') }}')
    assert (tmpl.render() == '0f, 1o, 2o')
