def test_with(self):
    env = Environment(extensions=['jinja2.ext.with_'])
    tmpl = env.from_string('        {% with a=42, b=23 -%}\n            {{ a }} = {{ b }}\n        {% endwith -%}\n            {{ a }} = {{ b }}        ')
    assert ([x.strip() for x in tmpl.render(a=1, b=2).splitlines()] == ['42 = 23', '1 = 2'])
