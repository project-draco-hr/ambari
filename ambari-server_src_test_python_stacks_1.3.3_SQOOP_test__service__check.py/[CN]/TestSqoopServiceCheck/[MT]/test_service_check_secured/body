def test_service_check_secured(self):
    self.executeScript('1.3.3/services/SQOOP/package/scripts/service_check.py', classname='SqoopServiceCheck', command='service_check', config_file='secured.json')
    self.assertResourceCalled('Execute', '/usr/bin/kinit  -kt /etc/security/keytabs/smokeuser.headless.keytab ambari-qa')
    self.assertResourceCalled('Execute', 'sqoop version', logoutput=True, user='ambari-qa')
    self.assertNoMoreResources()
