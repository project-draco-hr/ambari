{
  Stage stage=createServerActionStage(requestStageContainer.getLastStageId(),cluster,requestStageContainer.getId(),"Kerberization Clean Up",clusterHostInfoJson,"{}",hostParamsJson,CleanupServerAction.class,event,commandParameters,"Kerberization Clean Up",1200);
  RoleGraph roleGraph=roleGraphFactory.createNew(roleCommandOrder);
  roleGraph.build(stage);
  requestStageContainer.addStages(roleGraph.getStages());
}
