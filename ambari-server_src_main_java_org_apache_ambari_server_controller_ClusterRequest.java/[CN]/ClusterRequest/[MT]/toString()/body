{
  StringBuilder sb=new StringBuilder();
  sb.append("{" + " clusterName=" + clusterName + ", clusterId="+ clusterId+ ", hosts=[");
  if (hostNames != null) {
    for (int i=0; i < hostNames.size(); ++i) {
      if (i != 0) {
        sb.append(",");
      }
      sb.append(hostNames.get(i));
    }
  }
  sb.append("] }");
  return sb.toString();
}
