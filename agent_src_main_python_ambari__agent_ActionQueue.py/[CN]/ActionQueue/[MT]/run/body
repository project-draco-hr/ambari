def run(self):
    global clusterId, clusterDefinitionRevision
    while (not self.stopped()):
        while (not q.empty()):
            action = q.get()
            switches = {'START_ACTION': self.startAction, 'STOP_ACTION': self.stopAction, 'RUN_ACTION': self.runAction, 'CREATE_STRUCTURE_ACTION': self.createStructureAction, 'DELETE_STRUCTURE_ACTION': self.deleteStructureAction, 'WRITE_FILE_ACTION': self.writeFileAction, }
            result = switches.get(action['kind'], self.unknownAction)(action)
            r.put(result)
        if (not self.stopped()):
            time.sleep(5)
