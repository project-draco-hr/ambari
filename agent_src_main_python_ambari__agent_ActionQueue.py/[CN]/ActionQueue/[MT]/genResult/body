def genResult(self, action):
    result = {}
    if ((action['kind'] == 'INSTALL_AND_CONFIG_ACTION') or (action['kind'] == 'NO_OP_ACTION')):
        result = {'id': action['id'], }
    else:
        result = {'id': action['id'], 'clusterId': action['clusterId'], 'kind': action['kind'], 'clusterDefinitionRevision': action['clusterDefinitionRevision'], 'component': action['component'], 'role': action['role'], }
    return result
