def test_stop_secured(self):
    self.executeScript('2.0.6/services/HIVE/package/scripts/mysql_server.py', classname='MysqlServer', command='stop', config_file='secured.json')
    self.assertResourceCalled('Execute', 'service mysql stop', logoutput=True, path=['/usr/local/bin/:/bin/:/sbin/'], tries=1)
    self.assertNoMoreResources()
