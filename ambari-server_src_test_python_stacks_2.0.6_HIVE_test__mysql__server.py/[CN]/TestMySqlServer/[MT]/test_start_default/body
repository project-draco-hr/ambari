def test_start_default(self):
    self.executeScript('2.0.6/services/HIVE/package/scripts/mysql_server.py', classname='MysqlServer', command='start', config_file='default.json')
    self.assertResourceCalled('Execute', 'service mysql start', logoutput=True, path=['/usr/local/bin/:/bin/:/sbin/'], tries=1)
    self.assertNoMoreResources()
