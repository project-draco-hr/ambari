def assert_configure_secured(self):
    self.assertResourceCalled('File', '/tmp/addMysqlUser.sh', content=StaticFile('addMysqlUser.sh'), mode=493)
    self.assertResourceCalled('Execute', 'bash -x /tmp/addMysqlUser.sh mysql hive \'!`"\'"\'"\' 1\' c6402.ambari.apache.org', path=['/usr/sbin:/sbin:/usr/local/bin:/bin:/usr/bin'], tries=3, try_sleep=5)
