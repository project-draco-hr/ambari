def test_stop_default(self):
    self.executeScript('2.0.6/services/HIVE/package/scripts/mysql_server.py', classname='MysqlServer', command='stop', config_file='default.json')
    self.assertResourceCalled('Execute', 'service mysql stop', logoutput=True, only_if='service mysql status | grep running', user='mysql')
    self.assertNoMoreResources()
