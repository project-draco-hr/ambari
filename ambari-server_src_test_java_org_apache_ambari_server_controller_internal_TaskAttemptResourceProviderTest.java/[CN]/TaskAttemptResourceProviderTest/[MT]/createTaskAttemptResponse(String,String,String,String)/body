{
  Resource r=new ResourceImpl(Resource.Type.TaskAttempt);
  r.setProperty(TaskAttemptResourceProvider.TASK_ATTEMPT_CLUSTER_NAME_PROPERTY_ID,clusterName);
  r.setProperty(TaskAttemptResourceProvider.TASK_ATTEMPT_WORKFLOW_ID_PROPERTY_ID,workflowId);
  r.setProperty(TaskAttemptResourceProvider.TASK_ATTEMPT_JOB_ID_PROPERTY_ID,jobId);
  r.setProperty(TaskAttemptResourceProvider.TASK_ATTEMPT_ID_PROPERTY_ID,taskAttemptId);
  return r;
}
