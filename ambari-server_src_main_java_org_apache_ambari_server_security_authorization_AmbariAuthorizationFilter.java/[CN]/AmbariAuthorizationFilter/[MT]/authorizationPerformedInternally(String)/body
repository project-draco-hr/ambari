{
  return requestURI.matches(API_USERS_ALL_PATTERN) || requestURI.matches(API_GROUPS_ALL_PATTERN) || requestURI.matches(API_CREDENTIALS_ALL_PATTERN)|| requestURI.matches(API_PRIVILEGES_ALL_PATTERN)|| requestURI.matches(API_CLUSTER_REQUESTS_ALL_PATTERN)|| requestURI.matches(API_CLUSTER_SERVICES_ALL_PATTERN)|| requestURI.matches(API_CLUSTERS_PATTERN)|| requestURI.matches(API_STACK_VERSIONS_PATTERN)|| requestURI.matches(API_WIDGET_LAYOUTS_PATTERN)|| requestURI.matches(API_HOSTS_ALL_PATTERN)|| requestURI.matches(API_HOSTS_ALL)|| requestURI.matches(API_PRIVILEGES_ALL_PATTERN);
}
