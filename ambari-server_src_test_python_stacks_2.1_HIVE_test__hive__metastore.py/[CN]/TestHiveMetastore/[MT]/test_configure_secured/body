def test_configure_secured(self):
    self.executeScript((self.COMMON_SERVICES_PACKAGE_DIR + '/scripts/hive_metastore.py'), classname='HiveMetastore', command='configure', config_file='../../2.1/configs/secured.json', stack_version=self.STACK_VERSION, target=RMFTestCase.TARGET_COMMON_SERVICES)
    self.assert_configure_secured()
    self.assertNoMoreResources()
