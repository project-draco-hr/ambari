def test_stop_default(self):
    self.executeScript('2.0.6/services/HIVE/package/scripts/hive_metastore.py', classname='HiveMetastore', command='stop', config_file='../../2.1/configs/default.json')
    self.assertResourceCalled('Execute', 'kill `cat /var/run/hive/hive.pid` >/dev/null 2>&1 && rm -f /var/run/hive/hive.pid')
    self.assertNoMoreResources()
