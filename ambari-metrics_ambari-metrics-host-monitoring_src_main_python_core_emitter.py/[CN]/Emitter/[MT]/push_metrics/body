def push_metrics(self, data):
    headers = {'Content-Type': 'application/json', 'Accept': '*/*', 'Connection': ' Keep-Alive', }
    logger.debug(('message to sent: %s' % data))
    self.connection.request('POST', self.AMS_METRICS_POST_URL, data, headers)
    response = self.connection.getresponse()
    if response:
        logger.debug('POST response from server: retcode = {0}, reason = {1}'.format(response.status, response.reason))
        logger.debug(str(response.read()))
    return response
