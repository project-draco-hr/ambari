def test_start_secured(self):
    self.executeScript('2.1.1/services/HIVE/package/scripts/mysql_server.py', classname='MysqlServer', command='start', config_file='secured.json')
    self.assertResourceCalled('Execute', 'service mysqld start', logoutput=True, path=['/usr/local/bin/:/bin/:/sbin/'], tries=1)
    self.assertNoMoreResources()
