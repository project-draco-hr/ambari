{
  this.searchControls=null;
  this.velocityEngine=null;
  this.gson=null;
  if (this.ldapContext != null) {
    try {
      this.ldapContext.close();
    }
 catch (    NamingException e) {
      throw new KerberosOperationException("Unexpected error",e);
    }
 finally {
      this.ldapContext=null;
    }
  }
  setOpen(false);
}
