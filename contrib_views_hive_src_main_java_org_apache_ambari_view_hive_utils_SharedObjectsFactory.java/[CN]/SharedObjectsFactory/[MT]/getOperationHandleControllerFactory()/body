{
  if (!localObjects.get(OperationHandleControllerFactory.class).containsKey(getTagName()))   localObjects.get(OperationHandleControllerFactory.class).put(getTagName(),new OperationHandleControllerFactory(context,this));
  return (OperationHandleControllerFactory)localObjects.get(OperationHandleControllerFactory.class).get(getTagName());
}
