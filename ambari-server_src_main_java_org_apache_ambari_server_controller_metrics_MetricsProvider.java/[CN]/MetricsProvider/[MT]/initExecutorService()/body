{
  LinkedBlockingQueue<Runnable> queue=new LinkedBlockingQueue<Runnable>();
  ThreadPoolExecutor threadPoolExecutor=new ThreadPoolExecutor(THREAD_POOL_CORE_SIZE,THREAD_POOL_MAX_SIZE,THREAD_POOL_TIMEOUT_MILLIS,TimeUnit.MILLISECONDS,queue);
  threadPoolExecutor.allowCoreThreadTimeOut(true);
  return threadPoolExecutor;
}
