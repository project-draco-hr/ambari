def test_start_default(self):
    self.executeScript('2.1.1/services/ZOOKEEPER/package/scripts/zookeeper_server.py', classname='ZookeeperServer', command='start', config_file='default.json')
    self.assert_configure_default()
    self.assertResourceCalled('Execute', 'source /etc/zookeeper/conf/zookeeper-env.sh ; env ZOOCFGDIR=/etc/zookeeper/conf ZOOCFG=zoo.cfg /usr/lib/zookeeper/bin/zkServer.sh start', not_if='ls /var/run/zookeeper/zookeeper_server.pid >/dev/null 2>&1 && ps `cat /var/run/zookeeper/zookeeper_server.pid` >/dev/null 2>&1', user='zookeeper')
    self.assertNoMoreResources()
