{
  ScheduledExecutorService executorService=Executors.newSingleThreadScheduledExecutor();
  if (!aggregator.isDisabled()) {
    executorService.scheduleAtFixedRate(aggregator,SECONDS.toMillis(metricsConf.getInt(AGGREGATOR_CHECKPOINT_DELAY,120)),aggregator.getSleepIntervalMillis(),TimeUnit.MILLISECONDS);
  }
}
