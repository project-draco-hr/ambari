{
  ConfigVersion cVersion=new ConfigVersion("0.0.c");
  StackVersion sVersion=new StackVersion("hadoop-x.y.z");
  DeployState liveState=DeployState.INSTALL_FAILED;
  State expected=new State(cVersion,sVersion,liveState);
  c1.setServiceComponentHostState(s1,sc1,h1,expected);
  State actual=c1.getServiceComponentHostState(s1,sc1,h1);
  Assert.assertEquals(expected,actual);
  Assert.assertEquals(DeployState.INSTALL_FAILED,actual.getLiveState());
}
