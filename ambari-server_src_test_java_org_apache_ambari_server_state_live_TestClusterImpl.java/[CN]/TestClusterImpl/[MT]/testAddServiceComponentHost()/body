{
  try {
    c1.addServiceComponentHost("s2","sc2","h2",false);
    fail("Expected failure on invalid cluster/host");
  }
 catch (  Exception e) {
  }
  clusters.addCluster("c2");
  clusters.addHost("h2");
  clusters.mapHostToCluster("h2","c2");
  c1.addServiceComponentHost("s2","sc2","h2",false);
  try {
    c1.addServiceComponentHost("s2","sc2","h2",false);
    fail("Duplicate add should fail");
  }
 catch (  AmbariException e) {
  }
}
