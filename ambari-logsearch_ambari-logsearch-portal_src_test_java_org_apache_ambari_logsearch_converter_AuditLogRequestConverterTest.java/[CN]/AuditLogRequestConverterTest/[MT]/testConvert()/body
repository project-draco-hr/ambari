{
  AuditLogRequest request=new AuditLogRequest();
  fillBaseLogRequestWithTestData(request);
  SimpleQuery simpleQuery=underTest.convert(request);
  SolrQuery queryResult=new DefaultQueryParser().doConstructSolrQuery(simpleQuery);
  assertEquals("?q=*%3A*&start=0&rows=25&fq=repo%3A%28logsearch_app+secure_log%29&fq=-repo%3A%28hst_agent+system_message%29" + "&fq=log_message%3A*myincludemessage*&fq=-log_message%3A*myexcludemessage*&sort=evtTime+desc%2Cseq_num+desc",queryResult.toQueryString());
}
