def test_recommendYARNConfigurations(self):
    configurations = {}
    clusterData = {'containers': 5, 'ramPerContainer': 256, }
    expected = {'yarn-site': {'properties': {'yarn.nodemanager.resource.memory-mb': '1280', 'yarn.scheduler.minimum-allocation-mb': '256', 'yarn.scheduler.maximum-allocation-mb': '1280', }, }, }
    self.stackAdvisor.recommendYARNConfigurations(configurations, clusterData)
    self.assertEquals(configurations, expected)
