def test_validationCardinality01TwoHostsAssigned(self):
    servicesInfo = [{'name': 'GANGLIA', 'components': [{'name': 'GANGLIA_SERVER', 'cardinality': '0-1', 'category': 'MASTER', 'is_master': True, 'hostnames': ['host1', 'host2'], }], }]
    services = self.prepareServices(servicesInfo)
    hosts = self.prepareHosts(['host1', 'host2'])
    result = self.stackAdvisor.validateComponentLayout(services, hosts)
    expectedItems = [{'message': 'Cardinality violation, cardinality=0-1, hosts count=2', 'level': 'ERROR', }]
    self.assertValidationResult(expectedItems, result)
