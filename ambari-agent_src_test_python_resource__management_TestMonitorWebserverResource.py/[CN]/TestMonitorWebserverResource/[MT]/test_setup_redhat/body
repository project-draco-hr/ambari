@patch.object(System, 'os_family', new='redhat')
def test_setup_redhat(self):
    with Environment(test_mode=True) as env:
        MonitorWebserverProvider(MonitorWebserver('start')).action_start()
    defined_resources = env.resource_list
    expected_resources = "[MonitorWebserver['start'], Execute['grep -E 'KeepAlive (On|Off)' /etc/httpd/conf/httpd.conf && sed -i 's/KeepAlive Off/KeepAlive On/' /etc/httpd/conf/httpd.conf || echo 'KeepAlive On' >> /etc/httpd/conf/httpd.conf'], Execute['/etc/init.d/httpd start']]"
    self.assertEqual(str(defined_resources), expected_resources)
