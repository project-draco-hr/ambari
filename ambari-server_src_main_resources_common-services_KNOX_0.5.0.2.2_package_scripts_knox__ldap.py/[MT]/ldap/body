@OsFamilyFuncImpl(os_family=OSConst.WINSRV_FAMILY)
def ldap():
    import params
    from service_mapping import knox_ldap_win_service_name
    ServiceConfig(knox_ldap_win_service_name, action='change_user', username=params.knox_user, password=Script.get_password(params.knox_user))
    _ldap_common()
