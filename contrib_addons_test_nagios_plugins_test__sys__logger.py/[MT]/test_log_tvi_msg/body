def test_log_tvi_msg(msg):
    global tests_passed, tests_failed
    if (msg == expected_log_msg):
        print 'Test Passed'
        tests_passed += 1
    else:
        print '*** TEST FAILED ***'
        print 'Expected MSG: {0}'.format(expected_log_msg)
        print 'Actual MSG  : {0}'.format(msg)
        tests_failed += 1
