def generate_tvi_log_msg(alert_type, attempt, state, service, msg):
    severity = determine_severity(state, service)
    domain = determine_domain()
    msg_id = determine_msg_id(service, severity)
    if (alert_type == 'HARD'):
        log_tvi_msg('{0}: {1}: {2}# {3}'.format(severity, domain, msg_id, msg))
