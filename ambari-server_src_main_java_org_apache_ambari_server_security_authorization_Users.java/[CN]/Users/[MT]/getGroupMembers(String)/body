{
  final GroupEntity groupEntity=groupDAO.findGroupByName(groupName);
  if (groupEntity == null) {
    return null;
  }
 else {
    final Set<User> users=new HashSet<User>();
    for (    MemberEntity memberEntity : groupEntity.getMemberEntities()) {
      users.add(new User(memberEntity.getUser()));
    }
    return users;
  }
}
