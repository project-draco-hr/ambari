{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  QueryImpl query=(QueryImpl)o;
  return clusterController.equals(query.clusterController) && !(pageRequest != null ? !pageRequest.equals(query.pageRequest) : query.pageRequest != null) && queryPropertySet.equals(query.queryPropertySet)&& resourceDefinition.equals(query.resourceDefinition)&& resourceIds.equals(query.resourceIds)&& !(userPredicate != null ? !userPredicate.equals(query.userPredicate) : query.userPredicate != null);
}
