{
  if (m_resourceIterable == null) {
    if (m_pageRequest == null) {
      m_resourceIterable=getClusterController().getResources(getResourceType(),m_providerResources,getPredicate());
    }
 else {
      m_resourceIterable=getClusterController().getResources(getResourceType(),m_providerResources,getPredicate(),m_pageRequest).getIterable();
    }
  }
  return m_resourceIterable;
}
