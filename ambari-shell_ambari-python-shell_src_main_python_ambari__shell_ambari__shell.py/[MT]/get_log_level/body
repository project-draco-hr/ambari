def get_log_level(loglevel):
    loglev = loglevel.upper()
    if (loglev == 'DEBUG'):
        return logging.DEBUG
    elif (loglev == 'INFO'):
        return logging.INFO
    elif (loglev == 'WARNING'):
        return logging.WARNING
    elif (loglev == 'CRITICAL'):
        return logging.CRITICAL
    elif (loglev == 'ERROR'):
        return logging.ERROR
    elif (loglev == 'FATAL'):
        return logging.FATAL
    else:
        return logging.NOTSET
