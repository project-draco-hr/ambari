def get_key_func(self, name):
    '\n    Returns a number from filenames like 70-foobar.* or 999 for not matching\n    filenames\n    '
    parts = name.split('-', 1)
    if ((not parts) or (not parts[0].isdigit())):
        logger.warn(("Can't parse script filename number %s" % name))
        return self.NAME_PARSING_FAILED_CODE
    return int(parts[0])
