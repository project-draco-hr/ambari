def test_start_default(self):
    self.executeScript('2.0.6/services/NAGIOS/package/scripts/nagios_service.py', classname='NagiosServer', command='start', config_file='default.json')
    self.assert_configure_default()
    self.assertResourceCalled('Execute', 'service nagios start', path=['/usr/local/bin/:/bin/:/sbin/'])
    self.assertResourceCalled('MonitorWebserver', 'restart')
    self.assertNoMoreResources()
