def test_stop_default(self):
    self.executeScript('2.0.6/services/NAGIOS/package/scripts/nagios_service.py', classname='NagiosServer', command='stop', config_file='default.json')
    self.assertResourceCalled('Execute', 'service nagios stop && rm -f /var/run/nagios/nagios.pid', path=['/usr/local/bin/:/bin/:/sbin/'])
    self.assertResourceCalled('MonitorWebserver', 'restart')
    self.assertNoMoreResources()
