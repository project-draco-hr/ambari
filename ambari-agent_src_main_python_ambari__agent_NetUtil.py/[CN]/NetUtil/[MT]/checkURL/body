def checkURL(self, url):
    'Try to connect to a given url. Result is True if url returns HTTP code 200, in any other case\n    (like unreachable server or wrong HTTP code) result will be False\n    '
    try:
        p = urlparse(url)
        h = HTTP(p[1])
        h.putrequest('HEAD', p[2])
        h.endheaders()
        if (h.getreply()[0] == 200):
            return True
        else:
            return False
    except Exception as e:
        return False
