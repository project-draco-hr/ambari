{
  Exception lastException=null;
  for (int retries=0; retries < RETRY; retries++) {
    try {
      if (solrAmsClient.emitMetrics(metrics)) {
        metrics.getMetrics().clear();
        return;
      }
    }
 catch (    Exception e) {
      lastException=e;
    }
  }
  LOG.info("Could not emit metrics, last exception:",lastException);
}
