def hdfs_roll_edits():
    '\n  HDFS_CLIENT needs to be a dependency of JOURNALNODE\n  Roll the logs so that Namenode will be able to connect to the Journalnode.\n  '
    import params
    if params.security_enabled:
        Execute(params.dn_kinit_cmd, user=params.hdfs_user)
    command = 'hdfs dfsadmin -rollEdits'
    Execute(command, user=params.hdfs_user, tries=1)
