{
  KeyStore credStore=null;
  try {
    credStore=loadKeyStore(keyStoreFile,masterService.getMasterSecret(),storeType);
  }
 catch (  CertificateException e) {
    e.printStackTrace();
  }
catch (  KeyStoreException e) {
    e.printStackTrace();
  }
catch (  NoSuchAlgorithmException e) {
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
  return credStore;
}
