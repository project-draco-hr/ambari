def upload_conf_set(config_set, jaasFile):
    import params
    solr_cloud_util.upload_configuration_to_zk(zookeeper_quorum=params.zookeeper_quorum, solr_znode=params.logsearch_solr_znode, config_set_dir=format('{conf_dir}/solr'), config_set=config_set, tmp_dir=params.tmp_dir, java64_home=params.java64_home, user=params.metadata_user, solrconfig_content=InlineTemplate(params.metadata_solrconfig_content), jaas_file=jaasFile, retry=30, interval=5)
