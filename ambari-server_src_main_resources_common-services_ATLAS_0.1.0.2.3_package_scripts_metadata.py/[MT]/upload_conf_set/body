def upload_conf_set(config_set, random_num):
    import params
    tmp_config_set_folder = format('{tmp_dir}/solr_config_{config_set}_{random_num}')
    solr_cloud_util.upload_configuration_to_zk(zookeeper_quorum=params.zookeeper_quorum, solr_znode=params.logsearch_solr_znode, config_set_dir=format('{conf_dir}/solr'), config_set=config_set, tmp_config_set_dir=tmp_config_set_folder, java64_home=params.java64_home, user=params.metadata_user)
