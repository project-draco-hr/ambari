def create_collection(collection, config_set):
    import params
    jaasFile = (params.atlas_jaas_file if params.security_enabled else None)
    solr_cloud_util.create_collection(zookeeper_quorum=params.zookeeper_quorum, solr_znode=params.logsearch_solr_znode, collection=collection, config_set=config_set, java64_home=params.java64_home, user=params.metadata_user, jaas_file=jaasFile, shards=params.atlas_solr_shards, replication_factor=params.logsearch_solr_replication_factor)
