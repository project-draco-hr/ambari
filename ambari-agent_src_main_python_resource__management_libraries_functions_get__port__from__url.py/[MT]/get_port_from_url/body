def get_port_from_url(address):
    '\n  Return port from URL. If address is UnknownConfiguration,\n  UnknownConfiguration will be returned. If no port was found, Fail will be\n  raised.\n  '
    if (not is_empty(address)):
        port = re.findall(':([\\d]{1,5})(?=/|$)', address)
        if port:
            return port[0]
        raise Fail('No port in URL:{0}'.format(address))
    else:
        return address
