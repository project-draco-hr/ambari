def mysql_service(action='start'):
    import params
    if (action == 'start'):
        cmd = format('service {service_name} start')
    elif (action == 'stop'):
        cmd = format('service {service_name} stop')
    else:
        cmd = None
    if (cmd is not None):
        Execute(cmd, path='/usr/local/bin/:/bin/:/sbin/', logoutput=True)
