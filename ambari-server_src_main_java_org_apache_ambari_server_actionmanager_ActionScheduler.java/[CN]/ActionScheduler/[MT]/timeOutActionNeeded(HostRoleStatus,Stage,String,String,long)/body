{
  LOG.info("Last attempt time =" + stage.getLastAttemptTime(host,role) + ", actiontimeout ="+ this.actionTimeout+ ", current time="+ currentTime+ ", role="+ role+ ", status="+ status);
  if ((!status.equals(HostRoleStatus.QUEUED)) && (!status.equals(HostRoleStatus.IN_PROGRESS))) {
    return false;
  }
  if (currentTime > stage.getLastAttemptTime(host,role) + actionTimeout) {
    return true;
  }
  return false;
}
