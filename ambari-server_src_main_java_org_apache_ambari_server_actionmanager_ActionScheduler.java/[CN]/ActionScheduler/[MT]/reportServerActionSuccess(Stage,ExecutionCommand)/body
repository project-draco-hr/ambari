{
  CommandReport report=new CommandReport();
  report.setStatus(HostRoleStatus.COMPLETED.toString());
  report.setExitCode(0);
  report.setStdOut("Server action succeeded");
  report.setStdErr("");
  db.updateHostRoleState(cmd.getHostname(),stage.getRequestId(),stage.getStageId(),cmd.getRole().toString(),report);
}
