def __init__(self):
    from ambari_server.dbConfiguration_linux import createPGConfig, createOracleConfig, createMySQLConfig
    self.DBMS_KEYS_LIST = ['postgres', 'oracle', 'mysql']
    self.DBMS_LIST = [DBMSDesc(self.DBMS_KEYS_LIST[0], STORAGE_TYPE_LOCAL, 'PostgreSQL', 'Embedded', createPGConfig), DBMSDesc(self.DBMS_KEYS_LIST[1], STORAGE_TYPE_REMOTE, 'Oracle', '', createOracleConfig), DBMSDesc(self.DBMS_KEYS_LIST[2], STORAGE_TYPE_REMOTE, 'MySQL', '', createMySQLConfig), DBMSDesc(self.DBMS_KEYS_LIST[0], STORAGE_TYPE_REMOTE, 'PostgreSQL', '', createPGConfig)]
    self.DBMS_PROMPT_PATTERN = '[{0}] - {1}{2}\n'
    self.DBMS_CHOICE_PROMPT_PATTERN = '==============================================================================\nEnter choice ({0}): '
    self.JDK_VALID_CHOICES_PATTERN = '^[{0}]$'
