@staticmethod
def create(options, properties, dbId='Ambari'):
    if (dbId == 'Ambari'):
        return SQLServerAmbariDBConfig(options, properties)
    elif (dbId == 'Metrics'):
        return SQLServerMetricsDBConfig(options, properties)
    else:
        raise FatalException((-1), ('Invalid database requested: ' + str(dbId)))
