{
  if (object == null)   return false;
  if (object instanceof Date)   return true;
  String str=object.toString();
  for (  String format : DATE_FORMATS) {
    try {
      Date i=new SimpleDateFormat(format).parse(str);
      return true;
    }
 catch (    Exception e) {
    }
  }
  return false;
}
