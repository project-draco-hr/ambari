def test_determine_command_format_version(self):
    v1_command = {'commandParams': {'schema_version': '1.0', }, }
    v2_command = {'commandParams': {'schema_version': '2.0', }, }
    current_command = {}
    actionQueue = ActionQueue(AmbariConfig().getConfig(), 'dummy_controller')
    self.assertEqual(actionQueue.determine_command_format_version(v1_command), ActionQueue.COMMAND_FORMAT_V1)
    self.assertEqual(actionQueue.determine_command_format_version(v2_command), ActionQueue.COMMAND_FORMAT_V2)
    self.assertEqual(actionQueue.determine_command_format_version(current_command), ActionQueue.COMMAND_FORMAT_V1)
