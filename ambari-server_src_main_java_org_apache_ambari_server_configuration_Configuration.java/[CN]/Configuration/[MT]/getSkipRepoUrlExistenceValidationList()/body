{
  List<String> list=new ArrayList<String>();
  String propValue=getProperty(SKIP_REPO_URL_EXISTENCE_VALIDATION_LIST);
  for (  String repo : propValue.split(",")) {
    repo=repo.trim();
    if (!repo.isEmpty()) {
      list.add(repo);
    }
  }
  LOG.debug("Skip Repo URL Existence Validation on :" + list);
  return list;
}
