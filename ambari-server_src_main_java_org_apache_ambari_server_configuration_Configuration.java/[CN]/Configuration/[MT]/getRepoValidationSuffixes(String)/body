{
  String osFamily=this.osFamily.find_family(osType);
  String repoSuffixes;
  if (osFamily.equals(OsFamily.UBUNTU_FAMILY)) {
    repoSuffixes=properties.getProperty(REPO_SUFFIX_KEY_UBUNTU,REPO_SUFFIX_UBUNTU);
  }
 else {
    repoSuffixes=properties.getProperty(REPO_SUFFIX_KEY_DEFAULT,REPO_SUFFIX_DEFAULT);
  }
  return repoSuffixes.split(",");
}
