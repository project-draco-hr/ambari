{
  String value=properties.getProperty(AGENT_TASK_TIMEOUT_KEY,AGENT_TASK_TIMEOUT_DEFAULT);
  if (StringUtils.isNumeric(value)) {
    return value;
  }
 else {
    LOG.warn(String.format("Value of %s (%s) should be a number, " + "falling back to default value (%s)",AGENT_TASK_TIMEOUT_KEY,value,AGENT_TASK_TIMEOUT_DEFAULT));
    return AGENT_TASK_TIMEOUT_DEFAULT;
  }
}
