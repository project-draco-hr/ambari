{
  if (unknownPort) {
    return null;
  }
  if (componentName.equals("NAMENODE"))   return "50070";
 else   if (componentName.equals("DATANODE"))   return "50075";
 else   if (componentName.equals("JOBTRACKER"))   return "50030";
 else   if (componentName.equals("TASKTRACKER"))   return "50060";
 else   if (componentName.equals("HBASE_MASTER"))   return null == clusterName ? "60010" : "60011";
 else   if (componentName.equals("JOURNALNODE"))   return "8480";
 else   return null;
}
