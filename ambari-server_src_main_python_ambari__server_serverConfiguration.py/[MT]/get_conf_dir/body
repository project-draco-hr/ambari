def get_conf_dir():
    try:
        conf_dir = os.environ[AMBARI_CONF_VAR]
        return conf_dir
    except KeyError:
        default_conf_dir = AmbariPath.get('/etc/ambari-server/conf')
        print_info_msg(((AMBARI_CONF_VAR + ' is not set, using default ') + default_conf_dir))
        return default_conf_dir
