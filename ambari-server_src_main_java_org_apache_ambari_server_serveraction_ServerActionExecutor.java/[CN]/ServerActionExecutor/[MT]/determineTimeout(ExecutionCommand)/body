{
  Map<String,String> params=executionCommand.getCommandParams();
  String paramsTimeout=(params == null) ? null : params.get(ExecutionCommand.KeyNames.COMMAND_TIMEOUT);
  Long timeout;
  try {
    timeout=(paramsTimeout == null) ? null : (Long.parseLong(paramsTimeout) * 1000);
  }
 catch (  NumberFormatException e) {
    timeout=null;
  }
  return (timeout == null) ? EXECUTION_TIMEOUT_MS : ((timeout < 0) ? 0 : timeout);
}
