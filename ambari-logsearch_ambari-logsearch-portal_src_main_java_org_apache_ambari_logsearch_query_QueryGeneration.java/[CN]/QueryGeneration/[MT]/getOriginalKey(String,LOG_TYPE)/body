{
  String originalKey;
switch (logType) {
case AUDIT:
    originalKey=ConfigUtil.auditLogsColumnMapping.get(key + LogSearchConstants.UI_SUFFIX);
  break;
case SERVICE:
originalKey=ConfigUtil.serviceLogsColumnMapping.get(key + LogSearchConstants.UI_SUFFIX);
break;
default :
originalKey=null;
}
if (stringUtil.isEmpty(originalKey)) {
return key;
}
return originalKey;
}
