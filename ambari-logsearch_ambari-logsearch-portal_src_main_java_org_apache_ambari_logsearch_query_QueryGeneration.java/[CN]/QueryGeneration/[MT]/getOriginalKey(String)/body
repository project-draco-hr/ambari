{
  String originalServiceKey=ConfigUtil.serviceLogsColumnMapping.get(key + LogSearchConstants.UI_SUFFIX);
  String originalAuditKey=ConfigUtil.auditLogsColumnMapping.get(key + LogSearchConstants.UI_SUFFIX);
  if (originalAuditKey != null && originalServiceKey == null) {
    return originalAuditKey;
  }
  if (originalServiceKey != null && originalAuditKey == null) {
    return originalServiceKey;
  }
  if (originalAuditKey != null && originalServiceKey != null) {
    return originalServiceKey;
  }
  return key;
}
