{
  StringBuilder sb=new StringBuilder();
  sb.append("{" + " clusterName=" + clusterName + ", clusterId="+ clusterId+ ", hosts=[");
  if (hostNames != null) {
    int i=0;
    for (    String hostName : hostNames) {
      if (i != 0) {
        sb.append(",");
      }
      ++i;
      sb.append(hostName);
    }
  }
  sb.append("] }");
  return sb.toString();
}
