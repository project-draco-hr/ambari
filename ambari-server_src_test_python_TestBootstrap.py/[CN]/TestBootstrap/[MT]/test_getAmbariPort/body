def test_getAmbariPort(self):
    bootstrap_obj = BootStrap(['hostname'], 'root', 'sshKeyFile', 'scriptDir', 'bootdir', 'setupAgentFile', 'ambariServer', 'centos6', None, '8440')
    self.assertEquals(bootstrap_obj.getAmbariPort(), '8440')
    bootstrap_obj = BootStrap(['hostname'], 'root', 'sshKeyFile', 'scriptDir', 'bootdir', 'setupAgentFile', 'ambariServer', 'centos6', None, None)
    self.assertEquals(bootstrap_obj.getAmbariPort(), 'null')
