@patch('os.path.join')
def test_getSetupScript(self, join_mock):
    bootstrap_obj = BootStrap(['hostname'], 'root', 'sshKeyFile', 'scriptDir', 'bootdir', 'setupAgentFile', 'ambariServer', 'centos6', None, '8440')
    self.assertTrue(join_mock.called)
    self.assertEquals(bootstrap_obj.scriptDir, 'scriptDir')
