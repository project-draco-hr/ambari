{
  Resource configResource=configNode.getObject();
  type=(String)configResource.getPropertyValue("type");
  tag=(String)configResource.getPropertyValue("tag");
  properties=(Map)configNode.getObject().getPropertiesMap().get("properties");
  if (properties != null) {
    stripRequiredProperties(properties);
  }
 else {
    LOG.warn("Empty configuration found for configuration type = " + type + " during Blueprint export.  This may occur after an upgrade of Ambari, when"+ "attempting to export a Blueprint from a cluster started by an older version of "+ "Ambari.");
  }
}
