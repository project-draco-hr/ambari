def run(self):
    '\n    Run one of the supported commands: start, stop, restart, and status\n    '
    if (self.command == 'start'):
        self.cmd_start()
    elif (self.command == 'stop'):
        self.cmd_stop()
    elif (self.command == 'restart'):
        self.cmd_restart()
    elif (self.command == 'status'):
        self.cmd_status()
