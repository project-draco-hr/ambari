{
  String ambariServerVersion=getAmbariServerVersion();
  int rows=0;
  if (ambariServerVersion != null) {
    try {
      dbAccessor.executeQuery("INSERT INTO metainfo ('metainfo_key', " + "'metainfo_value') VALUES ('version', '${ambariVersion}')");
      rows=dbAccessor.updateTable("metainfo","metainfo_value",ambariServerVersion,String.format("WHERE metainfo_key = '%s'",version));
    }
 catch (    SQLException e) {
      LOG.error("Failed updating metainfo table.",e);
    }
  }
  return rows;
}
