{
  String ambariServerVersion=getAmbariServerVersion();
  int rows=0;
  if (ambariServerVersion != null) {
    try {
      dbAccessor.executeQuery(String.format("INSERT INTO metainfo (metainfo_key, " + "metainfo_value) VALUES ('version', '%s')",version),true);
      rows=dbAccessor.updateTable("metainfo","metainfo_value",version,"WHERE metainfo_key = 'version'");
    }
 catch (    SQLException e) {
      LOG.error("Failed updating metainfo table.",e);
    }
  }
  return rows;
}
