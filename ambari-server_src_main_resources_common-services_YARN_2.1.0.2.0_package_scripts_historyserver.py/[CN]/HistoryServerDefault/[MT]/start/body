def start(self, env, rolling_restart=False):
    import params
    env.set_params(params)
    self.configure(env)
    if (params.version and (compare_versions(format_hdp_stack_version(params.version), '2.2.0.0') >= 0)):
        params.HdfsResource(InlineTemplate(params.mapreduce_tar_destination).get_content(), type='file', action='create_on_execute', source=params.mapreduce_tar_source, owner=params.hdfs_user, group=params.user_group, mode=292)
        params.HdfsResource(None, action='execute')
    service('historyserver', action='start', serviceName='mapreduce')
