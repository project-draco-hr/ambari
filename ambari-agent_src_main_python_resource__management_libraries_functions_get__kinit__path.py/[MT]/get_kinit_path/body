def get_kinit_path(pathes_list):
    '\n  @param pathes: comma separated list\n  '
    kinit_path = ''
    for x in pathes_list:
        if (not x):
            continue
        path = os.path.join(x, 'kinit')
        if os.path.isfile(path):
            kinit_path = path
            break
    return kinit_path
