{
  StringBuilder sb=new StringBuilder();
  String origHref=request.getURI();
  String[] toks=origHref.split("/");
  for (int i=0; i < toks.length; ++i) {
    String s=toks[i];
    sb.append(s).append('/');
    if ("clusters".equals(s)) {
      sb.append(toks[i + 1]).append('/');
      break;
    }
  }
  Object requestId=requestStatus.getRequestResource().getPropertyValue(PropertyHelper.getPropertyId("id","Requests"));
  sb.append("requests/").append(requestId);
  return sb.toString();
}
