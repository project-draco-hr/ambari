{
  PermissionEntity permissionEntity=new PermissionEntity();
  permissionEntity.setResourceType(createResourceTypeEntity(ResourceType.CLUSTER));
  permissionEntity.setAuthorizations(Arrays.asList(createRoleAuthorizationEntity(RoleAuthorization.CLUSTER_VIEW_ALERTS),createRoleAuthorizationEntity(RoleAuthorization.CLUSTER_TOGGLE_ALERTS)));
  return permissionEntity;
}
