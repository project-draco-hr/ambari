def isSeLinux(self):
    try:
        (retcode, out, err) = run_os_command(GET_SE_LINUX_ST_CMD)
        se_status = re.search('(enforcing|permissive|enabled)', out)
        if se_status:
            return True
    except OSError:
        log.warn('Could not run {0}: OK'.format(GET_SE_LINUX_ST_CMD))
    return False
