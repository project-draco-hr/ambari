def unlock(self):
    '\n    Unlocks the lock file descriptor.\n    '
    Logger.info('Releasing the lock on {0}'.format(self.lock_file_name))
    fcntl.lockf(self.lock_file, fcntl.LOCK_UN)
    try:
        if (self.lock_file is not None):
            self.lock_file.close()
            self.lock_file = None
    except IOError:
        pass
