def build(self, id='-1'):
    global clusterId, clusterDefinitionRevision, firstContact
    timestamp = int((time.time() * 1000))
    hostInfo = HostInfo()
    agentEnv = {}
    hostInfo.register(agentEnv)
    register = {'responseId': int(id), 'timestamp': timestamp, 'hostname': socket.getfqdn(), 'publicHostname': self.pfqdn(), 'hardwareProfile': self.hardware.get(), 'agentEnv': agentEnv, }
    return register
