def test_rename_all_properties(self):
    site_properties = {'mapred.task.is.map': 'mapreduce.task.ismap', 'mapred.task.partition': 'mapreduce.task.partition', 'mapred.task.profile': 'mapreduce.task.profile', 'abc': 'abc', }
    site_properties = UpgradeHelper_HDP2.rename_all_properties(site_properties, UpgradeHelper_HDP2.PROPERTY_MAPPING)
    for key in site_properties.keys():
        self.assertEqual(key, site_properties[key])
    self.assertEqual(4, len(site_properties))
    pass
