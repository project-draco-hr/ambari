def test_service_check(self):
    self.executeScript('2.1.1/services/STORM/package/scripts/service_check.py', classname='ServiceCheck', command='service_check', config_file='default.json')
    self.assertResourceCalled('File', '/tmp/wordCount.jar', content=StaticFile('wordCount.jar'))
    self.assertResourceCalled('Execute', 'env JAVA_HOME=/usr/jdk64/jdk1.7.0_45 storm jar /tmp/wordCount.jar storm.starter.WordCountTopology WordCount -c nimbus.host=c6402.ambari.apache.org', logoutput=True)
    self.assertResourceCalled('Execute', 'env PATH=$PATH:/usr/jdk64/jdk1.7.0_45/bin storm kill WordCount')
