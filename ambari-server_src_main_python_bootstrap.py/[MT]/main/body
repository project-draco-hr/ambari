def main(argv=None):
    scriptDir = os.path.realpath(os.path.dirname(argv[0]))
    onlyargs = argv[1:]
    if (len(onlyargs) < 3):
        sys.stderr.write('Usage: <comma separated host> <sshkeyFile> <tmpdir for usage>\n')
        sys.exit(2)
        pass
    ' Parse the input'
    hostList = onlyargs[0].split(',')
    sshKeyFile = onlyargs[1]
    bootdir = onlyargs[2]
    logging.info(((((((('BootStrapping hosts ' + pprint.pformat(hostList)) + 'using ') + scriptDir) + ' with sshKey File ') + sshKeyFile) + ' using tmp dir ') + bootdir))
    bootstrap = BootStrap(hostList, sshKeyFile, scriptDir, bootdir)
    bootstrap.run()
