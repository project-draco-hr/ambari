def runOsCheckScript(self):
    params = self.shared_state
    self.host_log.write('Running os type check...')
    command = ('chmod a+x %s && %s %s' % (self.getOsCheckScriptRemoteLocation(), self.getOsCheckScriptRemoteLocation(), params.cluster_os_type))
    ssh = SSH(params.user, params.sshkey_file, self.host, command, params.bootdir, self.host_log)
    retcode = ssh.run()
    self.host_log.write('Running os type check  finished')
    return retcode
