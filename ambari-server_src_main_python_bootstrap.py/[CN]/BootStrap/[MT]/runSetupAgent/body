def runSetupAgent(self):
    commands = [('export AMBARI_PASSPHRASE=' + os.environ[AMBARI_PASSPHRASE_VAR]), '/tmp/setupAgent.py']
    pssh = PSSH(self.hostlist, self.sshkeyFile, commands)
    pssh.run()
    out = pssh.getstatus()
    logging.info(('Parallel ssh returns ' + pprint.pformat(out)))
    ' Test code for setting env var on agent host before starting setupAgent.py\n    commands = ["export AMBARI_PASSPHRASE=" + os.environ[AMBARI_PASSPHRASE_VAR], "set"]\n    pssh = PSSH(self.hostlist, self.sshkeyFile, commands)\n    pssh.run()\n    out = pssh.getstatus()\n    logging.info("Look for AMBARI_PASSPHRASE in out " + pprint.pformat(out))\n    '
