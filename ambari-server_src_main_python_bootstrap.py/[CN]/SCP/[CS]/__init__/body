def __init__(self, user, sshPort, sshkey_file, host, inputFile, remote, bootdir, host_log):
    self.user = user
    self.sshPort = sshPort
    self.sshkey_file = sshkey_file
    self.host = host
    self.inputFile = inputFile
    self.remote = remote
    self.bootdir = bootdir
    self.host_log = host_log
    pass
