def __init__(self, sshKeyFile, host, inputFile, remote, bootdir):
    self.sshKeyFile = sshKeyFile
    self.host = host
    self.inputFile = inputFile
    self.remote = remote
    self.bootdir = bootdir
    self.ret = {'exitstatus': (-1), 'log': 'FAILED', }
    threading.Thread.__init__(self)
    pass
