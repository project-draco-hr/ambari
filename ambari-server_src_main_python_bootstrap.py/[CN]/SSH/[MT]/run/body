def run(self):
    sshcommand = ['ssh', '-o', 'ConnectTimeOut=3', '-o', 'StrictHostKeyChecking=no', '-i', self.sshKeyFile, self.host, ';'.join(self.commands)]
    sshstat = subprocess.Popen(sshcommand, stdout=subprocess.PIPE, stderr=subprocess.PIPE)
    log = sshstat.communicate()
    self.ret['exitstatus'] = sshstat.returncode
    self.ret['log'] = ((log[0] + '\n') + log[1])
    pass
