{
  Stack.ConfigProperty pwdProp=new Stack.ConfigProperty("test-type","pwdProp",null);
  service3RequiredPwdConfigs.add(pwdProp);
  bpGroup2Config.getProperties().put("test-type",Collections.singletonMap("pwdProp","secret"));
