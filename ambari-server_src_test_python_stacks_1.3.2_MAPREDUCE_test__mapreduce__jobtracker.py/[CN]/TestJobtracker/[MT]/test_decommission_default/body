def test_decommission_default(self):
    self.executeScript('1.3.2/services/MAPREDUCE/package/scripts/jobtracker.py', classname='Jobtracker', command='decommission', config_file='default.json')
    self.assertResourceCalled('File', '/etc/hadoop/conf/mapred.exclude', owner='mapred', content=Template('exclude_hosts_list.j2'), group='hadoop')
    self.assertResourceCalled('ExecuteHadoop', 'mradmin -refreshNodes', conf_dir='/etc/hadoop/conf', kinit_override=True, user='mapred')
    self.assertNoMoreResources()
