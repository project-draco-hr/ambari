  widgetMap.put("HDFS_SUMMARY",hdfsSummaryWidgets);
  sectionLayoutMap.put("HDFS_SUMMARY","default_hdfs_dashboard");
  updateWidgetDefinitionsForService("HDFS",widgetMap,sectionLayoutMap);
}
