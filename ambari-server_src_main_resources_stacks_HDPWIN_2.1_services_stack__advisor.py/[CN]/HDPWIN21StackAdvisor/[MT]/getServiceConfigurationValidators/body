def getServiceConfigurationValidators(self):
    return {'HDFS': {'hadoop-env': self.validateHDFSConfigurationsEnv, }, 'MAPREDUCE2': {'mapred-site': self.validateMapReduce2Configurations, }, 'YARN': {'yarn-site': self.validateYARNConfigurations, }, 'HBASE': {'hbase-env': self.validateHbaseEnvConfigurations, }, 'HIVE': {'hive-site': self.validateHiveConfigurations, }, 'TEZ': {'tez-site': self.validateTezConfigurations, }, 'AMBARI_METRICS': {'ams-hbase-site': self.validateAmsHbaseSiteConfigurations, 'ams-hbase-env': self.validateAmsHbaseEnvConfigurations, 'ams-site': self.validateAmsSiteConfigurations, }, }
