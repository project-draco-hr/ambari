def test_tail_few_lines(self):
    fragment = self.grep.tail(self.string_good, 3)
    desired = '\ndebug: Finishing transaction 70060456663980\ndebug: Received report to process from ambari-dmi.cybervisiontech.com.ua\ndebug: Processing report from ambari-dmi.cybervisiontech.com.ua with processor Puppet::Reports::Store\n'.strip()
    self.assertEquals(fragment, desired, 'Grep tail function should return only last 3 lines of file')
