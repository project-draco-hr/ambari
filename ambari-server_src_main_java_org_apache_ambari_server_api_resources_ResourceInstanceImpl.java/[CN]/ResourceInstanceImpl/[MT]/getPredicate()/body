{
  if (m_predicate == null) {
    Predicate internalPredicate=createInternalPredicate(this);
    if (internalPredicate == null) {
      if (m_userPredicate != null) {
        m_predicate=m_userPredicate;
      }
    }
 else {
      m_predicate=(m_userPredicate == null ? internalPredicate : new AndPredicate(m_userPredicate,internalPredicate));
    }
  }
  return m_predicate;
}
