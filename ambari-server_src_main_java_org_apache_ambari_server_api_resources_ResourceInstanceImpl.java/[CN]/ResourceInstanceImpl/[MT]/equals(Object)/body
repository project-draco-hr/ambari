{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  ResourceInstanceImpl that=(ResourceInstanceImpl)o;
  return m_mapResourceIds.equals(that.m_mapResourceIds) && m_query == that.m_query && m_resourceDefinition.equals(that.m_resourceDefinition) && m_mapSubResources == null ? that.m_mapSubResources == null : m_mapSubResources.equals(that.m_mapSubResources);
}
