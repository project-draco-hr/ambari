{
  Stage s=new Stage(requestId,"/a/b","cluster1");
  s.setStageId(stageId);
  HostAction ha=new HostAction(hostname);
  HostRoleCommand cmd=new HostRoleCommand("HBASE",Role.HBASE_MASTER,new ServiceComponentHostStartEvent(Role.HBASE_MASTER.toString(),hostname,System.currentTimeMillis()));
  ha.addHostRoleCommand(cmd);
  s.addHostAction(hostname,ha);
  List<Stage> stages=new ArrayList<Stage>();
  stages.add(s);
  db.persistActions(stages);
}
