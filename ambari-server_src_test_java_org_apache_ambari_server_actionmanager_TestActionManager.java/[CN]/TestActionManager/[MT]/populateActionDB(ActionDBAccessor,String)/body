{
  Stage s=stageFactory.createNew(requestId,"/a/b","cluster1",1L,"action manager test","clusterHostInfo","commandParamsStage","hostParamsStage");
  s.setStageId(stageId);
  s.addHostRoleExecutionCommand(hostname,Role.HBASE_MASTER,RoleCommand.START,new ServiceComponentHostStartEvent(Role.HBASE_MASTER.toString(),hostname,System.currentTimeMillis()),"cluster1","HBASE",false);
  List<Stage> stages=new ArrayList<Stage>();
  stages.add(s);
  Request request=new Request(stages,clusters);
  db.persistActions(request);
}
