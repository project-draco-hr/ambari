def test_stop_default(self):
    self.executeScript('2.1/services/STORM/package/scripts/ui_server.py', classname='UiServer', command='stop', config_file='default.json')
    self.assertResourceCalled('Execute', 'kill `cat /var/run/storm/ui.pid` >/dev/null 2>&1', not_if='! (ls /var/run/storm/ui.pid >/dev/null 2>&1 && ps -p `cat /var/run/storm/ui.pid` >/dev/null 2>&1)')
    self.assertResourceCalled('Execute', 'kill -9 `cat /var/run/storm/ui.pid` >/dev/null 2>&1', not_if='sleep 2; ! (ls /var/run/storm/ui.pid >/dev/null 2>&1 && ps -p `cat /var/run/storm/ui.pid` >/dev/null 2>&1) || sleep 20; ! (ls /var/run/storm/ui.pid >/dev/null 2>&1 && ps -p `cat /var/run/storm/ui.pid` >/dev/null 2>&1)', ignore_failures=True)
    self.assertResourceCalled('Execute', 'rm -f /var/run/storm/ui.pid')
    self.assertNoMoreResources()
