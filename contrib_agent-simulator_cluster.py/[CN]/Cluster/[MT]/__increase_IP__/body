def __increase_IP__(self, base_IP, increase):
    IP = [int(base_IP[0]), int(base_IP[1]), int(base_IP[2]), int(base_IP[3])]
    IP[3] = (IP[3] + increase)
    for index in reversed(range(0, 4)):
        if (IP[index] > 255):
            IP[(index - 1)] = (IP[(index - 1)] + (IP[index] / 256))
            IP[index] = (IP[index] % 256)
    return IP
