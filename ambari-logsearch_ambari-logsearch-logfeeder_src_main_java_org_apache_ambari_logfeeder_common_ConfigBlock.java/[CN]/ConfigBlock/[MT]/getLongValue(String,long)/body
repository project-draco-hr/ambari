{
  String strValue=getStringValue(key);
  Long retValue=defaultValue;
  if (!StringUtils.isEmpty(strValue)) {
    try {
      retValue=Long.parseLong(strValue);
    }
 catch (    Throwable t) {
      logger.error("Error parsing long value. key=" + key + ", value="+ strValue);
    }
  }
  return retValue;
}
