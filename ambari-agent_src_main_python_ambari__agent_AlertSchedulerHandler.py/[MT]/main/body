def main():
    args = list(sys.argv)
    del args[0]
    try:
        logger.setLevel(logger.debug)
    except TypeError:
        logger.setLevel(12)
    ash = AlertSchedulerHandler(args[0], False)
    ash.start()
    i = 0
    try:
        while (i < 10):
            time.sleep(1)
            i += 1
    except KeyboardInterrupt:
        pass
    print str(ash.collector().alerts())
    ash.stop()
