def __json_to_callable(self, json_definition):
    source = json_definition['source']
    source_type = source.get('type', '')
    alert = None
    if (source_type == 'METRIC'):
        pass
    elif (source_type == 'PORT'):
        alert = PortAlert(json_definition, source)
    elif (type == 'SCRIPT'):
        pass
    return alert
