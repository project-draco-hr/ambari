def __find_config_values(self, configmap, obj_keylist):
    ' \n    finds templated values in the configuration map provided by the server\n    and returns a dictionary of template key to value \n    '
    if (configmap is None):
        return {}
    result = {}
    for key in obj_keylist:
        try:
            obj = configmap
            for layer in key.split('/'):
                obj = obj[layer]
            result[key] = obj
        except KeyError:
            pass
    return result
