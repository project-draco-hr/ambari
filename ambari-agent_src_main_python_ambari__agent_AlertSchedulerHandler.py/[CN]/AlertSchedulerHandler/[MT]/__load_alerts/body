def __load_alerts(self):
    definitions = []
    try:
        with open(self.filename) as fp:
            cluster_defs = json.load(fp)
            for deflist in cluster_defs.values():
                for definition in deflist:
                    obj = self.__json_to_callable(definition)
                    if (obj is not None):
                        definitions.append(obj)
    except:
        import traceback
        traceback.print_exc()
        pass
    return definitions
