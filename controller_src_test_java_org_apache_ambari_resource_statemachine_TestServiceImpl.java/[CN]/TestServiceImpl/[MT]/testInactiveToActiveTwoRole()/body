{
  String roles[]={"role1","role2"};
  setRoles(roles);
  ServiceEventType[] events={ServiceEventType.START,ServiceEventType.PRESTART_SUCCESS,ServiceEventType.ROLE_START_SUCCESS,ServiceEventType.ROLE_START_SUCCESS,ServiceEventType.AVAILABLE_CHECK_SUCCESS};
  ServiceState[] states={ServiceState.PRESTART,ServiceState.STARTING,ServiceState.STARTING,ServiceState.STARTED,ServiceState.ACTIVE};
  verifyTransitions(ServiceState.INACTIVE,events,states);
}
