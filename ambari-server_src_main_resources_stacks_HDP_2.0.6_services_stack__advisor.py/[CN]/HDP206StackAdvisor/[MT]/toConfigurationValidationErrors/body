def toConfigurationValidationErrors(self, items, siteName):
    result = []
    for item in items:
        if (item['message'] is not None):
            error = {'type': 'configuration', 'level': 'ERROR', 'message': item['message'], 'config-type': siteName, 'config-name': item['config-name'], }
            result.append(error)
    return result
