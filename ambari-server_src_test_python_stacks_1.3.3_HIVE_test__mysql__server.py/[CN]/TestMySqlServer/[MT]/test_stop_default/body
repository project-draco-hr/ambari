def test_stop_default(self):
    self.executeScript('2.1.1/services/HIVE/package/scripts/mysql_server.py', classname='MysqlServer', command='stop', config_file='default.json')
    self.assertResourceCalled('Execute', 'service mysqld stop', logoutput=True, path=['/usr/local/bin/:/bin/:/sbin/'], tries=1)
    self.assertNoMoreResources()
