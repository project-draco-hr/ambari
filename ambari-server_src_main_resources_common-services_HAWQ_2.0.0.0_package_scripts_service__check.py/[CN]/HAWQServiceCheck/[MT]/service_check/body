def service_check(self, env):
    Logger.info('Starting HAWQ service checks..')
    try:
        try:
            self.check_state()
            self.drop_table()
            self.create_table()
            self.insert_data()
            self.query_data()
            self.check_data_correctness()
        except:
            Logger.error('Service check failed')
            sys.exit(1)
    finally:
        self.drop_table()
    Logger.info('Service check completed successfully')
