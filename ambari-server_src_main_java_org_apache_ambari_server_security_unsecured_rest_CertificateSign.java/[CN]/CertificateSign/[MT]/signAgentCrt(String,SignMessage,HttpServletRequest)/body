{
  SignCertResponse response=new SignCertResponse();
  String signedCa;
  signedCa=certMan.signAgentCrt(hostname,message.getCsr(),message.getPassphrase());
  response.setSignedCa(signedCa);
  if (response.getSignedCa().length() > 0)   response.setResult(SignCertResponse.OK_STATUS);
 else   response.setResult(SignCertResponse.ERROR_STATUS);
  return response;
}
