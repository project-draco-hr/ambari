{
  Object object=null;
  try {
    object=webApplicationException.getResponse().getEntity();
  }
 catch (  Exception e) {
    LOG.error(e.getLocalizedMessage());
  }
  String errMsg=null;
  if (object != null) {
    errMsg=object.toString();
  }
 else {
    errMsg=webApplicationException.getMessage();
  }
  errMsg="URL: " + url + errMsg;
  LOG.error(errMsg);
  return errMsg;
}
