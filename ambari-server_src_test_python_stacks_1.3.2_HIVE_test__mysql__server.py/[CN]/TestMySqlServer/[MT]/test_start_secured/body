def test_start_secured(self):
    self.executeScript('1.3.2/services/HIVE/package/scripts/mysql_server.py', classname='MysqlServer', command='start', config_file='secured.json')
    self.assertResourceCalled('Execute', ('service', 'mysql', 'start'), logoutput=True, path=['/usr/local/bin/:/bin/:/sbin/'], sudo=True)
    self.assertNoMoreResources()
