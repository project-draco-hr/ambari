def test_stop_default(self):
    self.executeScript('1.3.2/services/HIVE/package/scripts/mysql_server.py', classname='MysqlServer', command='stop', config_file='default.json')
    self.assertResourceCalled('Execute', 'service mysql stop', logoutput=True, path=['/usr/local/bin/:/bin/:/sbin/'], user='mysql')
    self.assertNoMoreResources()
