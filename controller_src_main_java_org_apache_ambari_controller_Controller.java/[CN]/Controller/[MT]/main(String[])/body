{
  Injector injector=Guice.createInjector(new ControllerModule());
  DaemonWatcher.createInstance(System.getProperty("PID"),9100);
  try {
    Controller controller=injector.getInstance(Controller.class);
    if (controller != null) {
      controller.run();
    }
  }
 catch (  Throwable t) {
    DaemonWatcher.bailout(1);
  }
}
