def start(self, env, rolling_restart=False):
    import params
    env.set_params(params)
    self.configure(env)
    if params.is_supported_yarn_ranger:
        setup_ranger_yarn()
    if (not Script.is_hdp_stack_greater_or_equal('2.2')):
        install_tez_jars()
    else:
        if os.path.exists(params.tez_tar_source):
            params.HdfsResource(InlineTemplate(params.tez_tar_destination).get_content(), type='file', action='create_on_execute', source=params.tez_tar_source, group=params.user_group, owner=params.hdfs_user)
        params.HdfsResource(None, action='execute')
    service('resourcemanager', action='start')
