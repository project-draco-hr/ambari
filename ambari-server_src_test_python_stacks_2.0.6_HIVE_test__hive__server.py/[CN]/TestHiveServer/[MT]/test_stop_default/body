def test_stop_default(self):
    self.executeScript('2.0.6/services/HIVE/package/scripts/hive_server.py', classname='HiveServer', command='stop', config_file='default.json')
    self.assertResourceCalled('Execute', 'kill `cat /var/run/hive/hive-server.pid` >/dev/null 2>&1 && rm -f /var/run/hive/hive-server.pid')
    self.assertNoMoreResources()
