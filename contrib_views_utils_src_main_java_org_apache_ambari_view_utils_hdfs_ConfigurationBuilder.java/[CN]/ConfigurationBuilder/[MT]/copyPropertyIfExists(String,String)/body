{
  String value;
  try {
    value=ambariApi.getCluster().getConfigurationValue(type,key);
    if (value != null) {
      conf.set(key,value);
      LOG.debug("set " + key + " = "+ value);
    }
 else {
      LOG.debug("No such property " + type + "/"+ key);
    }
  }
 catch (  NoClusterAssociatedException e) {
    LOG.debug("No such property " + type + "/"+ key);
  }
}
