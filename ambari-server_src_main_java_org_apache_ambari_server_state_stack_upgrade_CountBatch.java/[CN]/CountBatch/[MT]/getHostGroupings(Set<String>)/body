{
  List<Set<String>> groupings=new ArrayList<Set<String>>();
  Set<String> set=new HashSet<String>();
  groupings.add(set);
  int i=1;
  for (  String host : hosts) {
    set.add(host);
    if (i < hosts.size() && 0 == (i++ % count)) {
      set=new HashSet<String>();
      groupings.add(set);
    }
  }
  return groupings;
}
