{
  SearchCriteria searchCriteria=new SearchCriteria(request);
  searchCriteria.addParam(LogSearchConstants.USER_NAME,request.getParameter("userId"));
  searchCriteria.addParam(LogSearchConstants.FILTER_NAME,request.getParameter("filterName"));
  searchCriteria.addParam(LogSearchConstants.ROW_TYPE,request.getParameter("rowType"));
  return userConfigMgr.getUserConfig(searchCriteria);
}
