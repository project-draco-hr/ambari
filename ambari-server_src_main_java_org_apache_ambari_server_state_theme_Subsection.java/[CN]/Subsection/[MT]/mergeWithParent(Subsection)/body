{
  if (rowSpan == null) {
    rowSpan=parent.rowSpan;
  }
  if (rowIndex == null) {
    rowIndex=parent.rowIndex;
  }
  if (columnSpan == null) {
    columnSpan=parent.columnSpan;
  }
  if (columnIndex == null) {
    columnIndex=parent.columnIndex;
  }
  if (displayName == null) {
    displayName=parent.displayName;
  }
  if (border == null) {
    border=parent.border;
  }
}
