def enable(self):
    if (win32service.QueryServiceConfig(self._service_handle)[1] == win32service.SERVICE_DISABLED):
        win32service.ChangeServiceConfig(self._service_handle, win32service.SERVICE_NO_CHANGE, win32service.SERVICE_DEMAND_START, win32service.SERVICE_NO_CHANGE, None, None, 0, None, None, None, None)
