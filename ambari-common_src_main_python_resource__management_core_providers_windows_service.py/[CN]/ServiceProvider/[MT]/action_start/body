def action_start(self):
    self._service_handle = (self._service_handle if hasattr(self, '_service_handle') else win32service.OpenService(_schSCManager, self.resource.service_name, win32service.SERVICE_ALL_ACCESS))
    if (not self.status()):
        self.enable()
        win32service.StartService(self._service_handle, None)
        self.wait_status(win32service.SERVICE_RUNNING)
