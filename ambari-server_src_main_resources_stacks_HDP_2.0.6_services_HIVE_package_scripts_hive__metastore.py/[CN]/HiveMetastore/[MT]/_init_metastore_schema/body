def _init_metastore_schema(self, env):
    import params
    env.set_params(params)
    if params.init_metastore_schema:
        mysql_service(daemon_name=params.daemon_name, action='start')
        cmd = format('export JAVA_HOME={java_home} ; {hive_bin}/schematool -dbType {hive_metastore_db_type} -initSchema -userName {hive_metastore_user_name} -passWord {hive_metastore_user_passwd}')
        Execute(cmd)
        mysql_service(daemon_name=params.daemon_name, action='stop')
