{
  Set<RepositoryResponse> response=new HashSet<RepositoryResponse>();
  for (  RepositoryRequest request : requests) {
    try {
      response.addAll(getRepositories(request));
    }
 catch (    StackAccessException e) {
      if (requests.size() == 1) {
        throw e;
      }
    }
  }
  return response;
}
