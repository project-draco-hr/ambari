{
  if (request.getClusterName() == null || request.getClusterName().isEmpty()) {
  }
  Cluster cluster=clusters.getCluster(request.getClusterName());
  for (  PerServiceRequest service : request.getServices()) {
    Map<String,Config> configs=new HashMap<String,Config>();
    cluster.addService(new ServiceImpl(cluster,service.getServiceName(),DeployState.valueOf(service.getDesiredState()),configs));
  }
}
