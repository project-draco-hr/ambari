{
  Set<OperatingSystemResponse> response=new HashSet<OperatingSystemResponse>();
  for (  OperatingSystemRequest request : requests) {
    try {
      response.addAll(getStackOperatingSystems(request));
    }
 catch (    StackAccessException e) {
      if (requests.size() == 1) {
        throw e;
      }
    }
  }
  return response;
}
