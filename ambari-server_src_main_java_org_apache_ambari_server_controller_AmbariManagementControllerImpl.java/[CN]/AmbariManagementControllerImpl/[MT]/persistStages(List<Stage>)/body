{
  if (stages != null && stages.size() > 0) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Triggering Action Manager" + ", clusterName=" + stages.get(0).getClusterName() + ", requestId="+ stages.get(0).getRequestId()+ ", stagesCount="+ stages.size());
    }
    actionManager.sendActions(stages);
  }
}
