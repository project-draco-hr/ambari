{
switch (newState) {
case INSTALLED:
    if (oldState == State.INIT || oldState == State.UNINSTALLED || oldState == State.INSTALLED || oldState == State.STARTED || oldState == State.STOPPING) {
      return true;
    }
  break;
case STARTED:
if (oldState == State.INSTALLED || oldState == State.STARTED) {
  return true;
}
break;
}
return false;
}
