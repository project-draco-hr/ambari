{
  Set<StackServiceResponse> response=new HashSet<StackServiceResponse>();
  for (  StackServiceRequest request : requests) {
    try {
      response.addAll(getStackServices(request));
    }
 catch (    StackAccessException e) {
      if (requests.size() == 1) {
        throw e;
      }
    }
  }
  return response;
}
