{
  Set<StackVersionResponse> response=new HashSet<StackVersionResponse>();
  for (  StackVersionRequest request : requests) {
    try {
      response.addAll(getStackVersions(request));
    }
 catch (    StackAccessException e) {
      if (requests.size() == 1) {
        throw e;
      }
    }
  }
  return response;
}
