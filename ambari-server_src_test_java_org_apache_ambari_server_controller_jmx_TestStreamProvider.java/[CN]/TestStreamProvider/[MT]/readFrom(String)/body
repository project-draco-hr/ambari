{
  lastSpec=spec;
  String filename=FILE_MAPPING.get(getPort(spec));
  if (filename == null) {
    throw new IOException("Can't find JMX source for " + spec);
  }
  if (delay > 0) {
    try {
      Thread.sleep(delay);
    }
 catch (    InterruptedException e) {
    }
  }
  return ClassLoader.getSystemResourceAsStream(filename);
}
