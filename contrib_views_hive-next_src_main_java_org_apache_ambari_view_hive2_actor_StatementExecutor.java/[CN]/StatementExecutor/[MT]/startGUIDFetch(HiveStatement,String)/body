{
  if (guidFetcher == null) {
    guidFetcher=getContext().actorOf(Props.create(YarnAtsGUIDFetcher.class,storage).withDispatcher("akka.actor.misc-dispatcher"),"YarnAtsGUIDFetcher:" + UUID.randomUUID().toString());
  }
  LOG.info("Fetching guid for Job Id: {}",jobId);
  guidFetcher.tell(new UpdateYarnAtsGuid(statement,jobId),self());
}
