{
  if (auditLogger.isEnabled()) {
    AuditEvent loginSucceededAuditEvent=LoginAuditEvent.builder().withRemoteIp(RequestUtils.getRemoteAddress(request)).withUserName(authResult.getName()).withTimestamp(System.currentTimeMillis()).withRoles(permissionHelper.getPermissionLabels(authResult)).build();
    auditLogger.log(loginSucceededAuditEvent);
  }
}
