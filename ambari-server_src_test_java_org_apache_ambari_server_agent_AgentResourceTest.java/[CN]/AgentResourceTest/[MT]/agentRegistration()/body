{
  RegistrationResponse response;
  WebResource webResource=resource();
  response=webResource.path("/register/dummyhost").type(MediaType.APPLICATION_JSON).post(RegistrationResponse.class,createDummyJSONRegister());
  LOG.info("Returned from Server " + response.getResponseStatus());
  Assert.assertEquals(response.getResponseStatus(),RegistrationStatus.OK);
}
