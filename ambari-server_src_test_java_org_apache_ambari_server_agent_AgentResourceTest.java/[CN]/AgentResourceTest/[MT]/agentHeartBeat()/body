{
  HeartBeatResponse response;
  WebResource resource=resource();
  response=resource.path("/heartbeat/dummyhost").type(MediaType.APPLICATION_JSON).post(HeartBeatResponse.class,createDummyHeartBeat());
  LOG.info("Returned from Server: " + "clusterid = " + response.getClusterId() + " responseid="+ response.getResponseId());
  Assert.assertEquals(response.getResponseId(),0L);
}
