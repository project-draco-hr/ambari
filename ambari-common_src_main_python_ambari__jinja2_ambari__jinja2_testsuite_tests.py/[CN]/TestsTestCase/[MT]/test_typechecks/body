def test_typechecks(self):
    tmpl = env.from_string('\n            {{ 42 is undefined }}\n            {{ 42 is defined }}\n            {{ 42 is none }}\n            {{ none is none }}\n            {{ 42 is number }}\n            {{ 42 is string }}\n            {{ "foo" is string }}\n            {{ "foo" is sequence }}\n            {{ [1] is sequence }}\n            {{ range is callable }}\n            {{ 42 is callable }}\n            {{ range(5) is iterable }}\n        ')
    assert (tmpl.render().split() == ['False', 'True', 'False', 'True', 'True', 'False', 'True', 'True', 'True', 'True', 'False', 'True'])
