{
  StringBuilder sb=new StringBuilder();
  sb.append("Service name:");
  sb.append(name);
  sb.append("\nversion:");
  sb.append(version);
  sb.append("\ncomment:");
  sb.append(comment);
  for (  ComponentInfo component : getComponents()) {
    sb.append("\n\n\nComponent:\n");
    sb.append("name=");
    sb.append(component.getName());
    sb.append("\tcategory=");
    sb.append(component.getCategory());
  }
  return sb.toString();
}
