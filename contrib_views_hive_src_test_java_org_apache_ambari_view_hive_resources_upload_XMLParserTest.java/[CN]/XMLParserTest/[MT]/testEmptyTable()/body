{
  String xml="<table></table>";
  StringReader sr=new StringReader(xml);
  XMLParser jp=null;
  try {
    jp=new XMLParser(sr,null);
    Iterator<Row> iterator=jp.iterator();
    Assert.assertEquals("Iterator Empty!",false,iterator.hasNext());
  }
  finally {
    if (null != jp)     jp.close();
    sr.close();
  }
}
