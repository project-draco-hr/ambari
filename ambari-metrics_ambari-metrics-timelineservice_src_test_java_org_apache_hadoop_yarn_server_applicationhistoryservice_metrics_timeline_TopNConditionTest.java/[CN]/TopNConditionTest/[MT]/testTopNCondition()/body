{
  List<String> metricNames=new ArrayList<>();
  List<String> hostnames=new ArrayList<>();
  hostnames.add("h1");
  hostnames.add("h2");
  metricNames.add("m1");
  Assert.assertTrue(TopNCondition.isTopNHostCondition(metricNames,hostnames));
  Assert.assertFalse(TopNCondition.isTopNMetricCondition(metricNames,hostnames));
  hostnames.clear();
  hostnames.add("h%");
  hostnames.add("g1");
  Assert.assertTrue(TopNCondition.isTopNHostCondition(metricNames,hostnames));
  Assert.assertFalse(TopNCondition.isTopNMetricCondition(metricNames,hostnames));
  hostnames.clear();
  metricNames.add("m2");
  metricNames.add("m3");
  Assert.assertFalse(TopNCondition.isTopNHostCondition(metricNames,hostnames));
  Assert.assertTrue(TopNCondition.isTopNMetricCondition(metricNames,hostnames));
  metricNames.add("m2");
  metricNames.add("m%");
  Assert.assertFalse(TopNCondition.isTopNHostCondition(metricNames,hostnames));
  Assert.assertTrue(TopNCondition.isTopNMetricCondition(metricNames,hostnames));
  metricNames.add("m2");
  metricNames.add("m%");
  hostnames.add("h1");
  Assert.assertFalse(TopNCondition.isTopNHostCondition(metricNames,hostnames));
  Assert.assertTrue(TopNCondition.isTopNMetricCondition(metricNames,hostnames));
  metricNames.clear();
  hostnames.clear();
  metricNames.add("m1");
  metricNames.add("m2");
  hostnames.add("h1");
  hostnames.add("h2");
  Assert.assertFalse(TopNCondition.isTopNHostCondition(metricNames,hostnames));
  Assert.assertFalse(TopNCondition.isTopNMetricCondition(metricNames,hostnames));
  metricNames.clear();
  hostnames.clear();
  metricNames.add("m1");
  metricNames.add("m2");
  hostnames.add("%");
  Assert.assertFalse(TopNCondition.isTopNHostCondition(metricNames,hostnames));
  Assert.assertFalse(TopNCondition.isTopNMetricCondition(metricNames,hostnames));
  metricNames.clear();
  hostnames.clear();
  metricNames.add("m%");
  hostnames.add("%");
  Assert.assertFalse(TopNCondition.isTopNHostCondition(metricNames,hostnames));
  Assert.assertFalse(TopNCondition.isTopNMetricCondition(metricNames,hostnames));
}
