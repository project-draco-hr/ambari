{
  return AddCredentialRequestAuditEvent.builder().withTimestamp(System.currentTimeMillis()).withRequestType(request.getRequestType()).withResultStatus(result.getStatus()).withUrl(request.getURI()).withRemoteIp(request.getRemoteAddress()).withClusterName(getProperty(request,"cluster_name")).withType(getProperty(request,"type")).withAlias(getProperty(request,"alias")).withPrincipal(getProperty(request,"principal")).build();
}
