def is_unchanged_datasource_url(datasource_url):
    import params
    parsed_url = urlparse(datasource_url)
    Logger.debug(('parsed url: scheme = %s, host = %s, port = %s' % (parsed_url.scheme, parsed_url.hostname, parsed_url.port)))
    Logger.debug(('collector: scheme = %s, host = %s, port = %s' % (params.metric_collector_protocol, params.metric_collector_host, params.metric_collector_port)))
    return ((parsed_url.scheme.strip() == params.metric_collector_protocol.strip()) and (parsed_url.hostname.strip() == params.metric_collector_host.strip()) and (str(parsed_url.port) == params.metric_collector_port))
