{
  Stage s=new Stage(requestId,"/var/log",clusterName,"execution command wrapper test","clusterHostInfo");
  s.setStageId(stageId);
  s.addHostRoleExecutionCommand(hostName,Role.NAMENODE,RoleCommand.START,new ServiceComponentHostStartEvent(Role.NAMENODE.toString(),hostName,System.currentTimeMillis()),clusterName,"HDFS");
  List<Stage> stages=new ArrayList<Stage>();
  stages.add(s);
  Request request=new Request(stages,clusters);
  db.persistActions(request);
}
