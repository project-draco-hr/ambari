{
  readWriteLock.readLock().lock();
  try {
    StackEntity stackEntity=getDesiredStateEntity().getDesiredStack();
    if (null != stackEntity) {
      return new StackId(stackEntity.getStackName(),stackEntity.getStackVersion());
    }
 else {
      return null;
    }
  }
  finally {
    readWriteLock.readLock().unlock();
  }
}
