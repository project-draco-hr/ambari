{
  readWriteLock.readLock().lock();
  try {
    ServiceComponentResponse r=new ServiceComponentResponse(getClusterId(),service.getCluster().getClusterName(),service.getName(),getName(),this.desiredConfigs,getDesiredStackVersion().getStackId(),getDesiredState().toString());
    return r;
  }
  finally {
    readWriteLock.readLock().unlock();
  }
}
