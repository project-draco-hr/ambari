{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Setting DesiredStackVersion of Service" + ", clusterName=" + service.getCluster().getClusterName() + ", clusterId="+ service.getCluster().getClusterId()+ ", serviceName="+ service.getName()+ ", serviceComponentName="+ componentName+ ", oldDesiredStackVersion="+ this.desiredStackVersion+ ", newDesiredStackVersion="+ stackVersion);
  }
  this.desiredStackVersion=stackVersion;
}
