{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Setting DesiredState of Service" + ", clusterName=" + service.getCluster().getClusterName() + ", clusterId="+ service.getCluster().getClusterId()+ ", serviceName="+ service.getName()+ ", serviceComponentName="+ componentName+ ", oldDesiredState="+ this.desiredState+ ", newDesiredState="+ state);
  }
  this.desiredState=state;
}
