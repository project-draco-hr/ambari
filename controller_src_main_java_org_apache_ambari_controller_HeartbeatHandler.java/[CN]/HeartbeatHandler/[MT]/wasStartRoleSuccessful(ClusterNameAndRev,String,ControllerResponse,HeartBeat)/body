{
  if (!heartbeat.getStateChangeStatus()) {
    return false;
  }
  List<Action> actions=response.getActions();
  for (  Action action : actions) {
    if (action.kind != Action.Kind.START_ACTION) {
      continue;
    }
    if (action.getClusterId().equals(clusterIdAndRev.getClusterName()) && action.getClusterDefinitionRevision() == clusterIdAndRev.getRevision() && action.getRole().equals(roleName)) {
      return true;
    }
  }
  return false;
}
