{
  List<ActionResult> actionResults=heartbeat.getActionResults();
  for (  ActionResult actionResult : actionResults) {
    if (actionResult.getId().contains(SpecialServiceIDs.SERVICE_AVAILABILITY_CHECK_ID.toString()) || actionResult.getId().contains(SpecialServiceIDs.SERVICE_PREINSTALL_CHECK_ID.toString())) {
      installOrStartedComponents.recordActionId(actionResult.getId(),actionResult);
    }
  }
}
