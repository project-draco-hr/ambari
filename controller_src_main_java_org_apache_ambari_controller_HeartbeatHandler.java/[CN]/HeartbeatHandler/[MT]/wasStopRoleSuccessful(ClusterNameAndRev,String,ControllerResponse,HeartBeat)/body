{
  if (!heartbeat.getStateChangeStatus()) {
    return false;
  }
  List<Action> actions=response.getActions();
  for (  Action action : actions) {
    if (action.getClusterId() == clusterIdAndRev.getClusterName() && action.getClusterDefinitionRevision() == clusterIdAndRev.getRevision() && action.getRole().equals(roleName) && action.kind == Action.Kind.START_ACTION) {
      return false;
    }
  }
  return true;
}
