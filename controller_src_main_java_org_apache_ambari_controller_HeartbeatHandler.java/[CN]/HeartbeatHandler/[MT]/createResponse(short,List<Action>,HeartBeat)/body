{
  ControllerResponse r=new ControllerResponse();
  r.setResponseId(responseId);
  if (allActions.size() > 0) {
    Action a=new Action();
    a.setKind(Kind.NO_OP_ACTION);
    allActions.add(a);
  }
  r.setActions(allActions);
  agentToHeartbeatResponseMap.put(heartbeat.getHostname(),r);
  return r;
}
