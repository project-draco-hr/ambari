def assert_service_check(self):
    self.assertResourceCalled('File', '/tmp/oozieSmoke2.sh', content=StaticFile('oozieSmoke2.sh'), mode=493)
    self.assertResourceCalled('Execute', '/tmp/oozieSmoke2.sh', logoutput=True, tries=3, command='/tmp/oozieSmoke2.sh suse /var/lib/oozie /etc/oozie/conf /usr/bin /etc/hadoop/conf /usr/bin ambari-qa False', path=['/usr/bin:/usr/bin'], try_sleep=5)
