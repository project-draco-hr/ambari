{
  try {
    Stat stat=new Stat();
    String clusterStatePath=ZOOKEEPER_CLUSTERS_ROOT_PATH + "/" + clusterName+ "/state";
    ClusterState clsState=JAXBUtil.read(zk.getData(clusterStatePath,false,stat),ClusterState.class);
    return clsState;
  }
 catch (  Exception e) {
    throw new IOException(e);
  }
}
