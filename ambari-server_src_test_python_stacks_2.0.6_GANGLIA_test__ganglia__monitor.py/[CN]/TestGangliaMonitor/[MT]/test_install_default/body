def test_install_default(self):
    self.executeScript('2.0.6/services/GANGLIA/package/scripts/ganglia_monitor.py', classname='GangliaMonitor', command='install', config_file='default.json')
    self.assert_configure_default()
    self.assert_gmond_master_conf_generated()
    self.assertResourceCalled('Execute', 'chkconfig gmond off', path=['/usr/sbin:/sbin:/usr/local/bin:/bin:/usr/bin'])
    self.assertResourceCalled('Execute', 'chkconfig gmetad off', path=['/usr/sbin:/sbin:/usr/local/bin:/bin:/usr/bin'])
    self.assertNoMoreResources()
