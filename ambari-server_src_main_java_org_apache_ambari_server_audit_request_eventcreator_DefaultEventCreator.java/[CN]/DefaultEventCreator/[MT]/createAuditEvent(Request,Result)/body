{
  String username=((User)SecurityContextHolder.getContext().getAuthentication().getPrincipal()).getUsername();
  return RequestAuditEvent.builder().withTimestamp(System.currentTimeMillis()).withUserName(username).withRemoteIp(request.getRemoteAddress()).withRequestType(request.getRequestType()).withUrl(request.getURI()).withResultStatus(result.getStatus()).build();
}
