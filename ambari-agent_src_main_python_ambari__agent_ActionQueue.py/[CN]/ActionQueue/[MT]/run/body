def run(self):
    while (not self.stopped()):
        while (not self.statusCommandQueue.empty()):
            try:
                command = self.statusCommandQueue.get(False)
                self.process_command(command)
            except Queue.Empty:
                pass
        try:
            command = self.commandQueue.get(True, self.EXECUTION_COMMAND_WAIT_TIME)
            self.process_command(command)
        except Queue.Empty:
            pass
