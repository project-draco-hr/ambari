def run(self):
    result = []
    while (not self.stopped()):
        while (not commandQueue.empty()):
            command = commandQueue.get()
            try:
                commandCopy = copy.copy(command)
                result = self.executeCommand(commandCopy)
            except Exception as err:
                traceback.print_exc()
                logger.warn(err)
                pass
            for entry in result:
                resultQueue.put(entry)
            pass
        if (not self.stopped()):
            time.sleep(5)
