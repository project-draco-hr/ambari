def put_status(self, commands):
    self.statusCommandQueue.queue.clear()
    for command in commands:
        logger.info((((((('Adding ' + command['commandType']) + ' for service ') + command['serviceName']) + ' of cluster ') + command['clusterName']) + ' to the queue.'))
        logger.debug(pprint.pformat(command))
        self.statusCommandQueue.put(command)
