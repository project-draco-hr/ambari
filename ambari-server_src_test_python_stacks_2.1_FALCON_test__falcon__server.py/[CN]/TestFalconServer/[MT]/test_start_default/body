def test_start_default(self):
    self.executeScript('2.1/services/FALCON/package/scripts/falcon_server.py', classname='FalconServer', command='start', config_file='default.json')
    self.assert_configure_default()
    self.assertResourceCalled('Execute', '/usr/lib/falcon/bin/falcon-start -port 15000', user='falcon')
    self.assertNoMoreResources()
