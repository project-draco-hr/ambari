{
  if (valueList == null || valueList.length == 0) {
    return "";
  }
  String expr="";
  int count=-1;
  for (  String value : valueList) {
    count++;
    if (count > 0) {
      expr+=" AND ";
    }
    expr+=fieldName + ":*" + value+ "*";
  }
  if (valueList.length == 0) {
    return expr;
  }
 else {
    return "(" + expr + ")";
  }
}
