{
  return new ServiceComponentRequest((String)properties.get(COMPONENT_CLUSTER_NAME_PROPERTY_ID),(String)properties.get(COMPONENT_SERVICE_NAME_PROPERTY_ID),(String)properties.get(COMPONENT_COMPONENT_NAME_PROPERTY_ID),(String)properties.get(COMPONENT_STATE_PROPERTY_ID),(String)properties.get(COMPONENT_RECOVERY_ENABLED_ID),(String)properties.get(COMPONENT_CATEGORY_PROPERTY_ID));
}
