{
  Stage s=new Stage(requestId,"/a/b","cluster1",1L,"action db accessor test","clusterHostInfo");
  s.setStageId(stageId);
  s.addHostRoleExecutionCommand(hostname,Role.HBASE_MASTER,RoleCommand.START,new ServiceComponentHostStartEvent(Role.HBASE_MASTER.toString(),hostname,System.currentTimeMillis()),"cluster1","HBASE");
  s.addHostRoleExecutionCommand(hostname,Role.HBASE_REGIONSERVER,RoleCommand.START,new ServiceComponentHostStartEvent(Role.HBASE_REGIONSERVER.toString(),hostname,System.currentTimeMillis()),"cluster1","HBASE");
  return s;
}
