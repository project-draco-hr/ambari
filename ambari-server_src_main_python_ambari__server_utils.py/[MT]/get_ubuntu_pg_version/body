def get_ubuntu_pg_version():
    'Return installed version of postgre server. In case of several\n  installed versions will be returned a more new one.\n  '
    postgre_ver = ''
    if os.path.isdir(PG_HBA_INIT_FILES['ubuntu']):
        postgre_ver = sorted([fld for fld in os.listdir(PG_HBA_INIT_FILES[OSConst.UBUNTU_FAMILY]) if os.path.isdir(os.path.join(PG_HBA_INIT_FILES[OSConst.UBUNTU_FAMILY], fld))], reverse=True)
        if (len(postgre_ver) > 0):
            return postgre_ver[0]
    return postgre_ver
