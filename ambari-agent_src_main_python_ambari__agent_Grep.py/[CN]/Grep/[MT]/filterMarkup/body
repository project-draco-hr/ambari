def filterMarkup(self, string):
    '\n    Filters given string from puppet colour markup done using escape codes like \x1b[0;36m\n    '
    if (string is None):
        result = None
    else:
        regexp = ('\x1b' + '\\[[\\d;]{1,4}m')
        result = re.sub(regexp, '', string)
    return result
