{
  boolean isFilterExpired=false;
  if (logfeederFilter != null) {
    Date filterEndDate=parseFilterExpireDate(logfeederFilter);
    if (filterEndDate != null) {
      Date currentDate=getCurrentDate();
      if (currentDate.compareTo(filterEndDate) >= 0) {
        logger.debug("Filter for  Component :" + logfeederFilter.getLabel() + " and Hosts :"+ listToStr(logfeederFilter.getHosts())+ "Filter is expired because of filter endTime : "+ dateToStr(filterEndDate)+ " is older than currentTime :"+ dateToStr(currentDate));
        isFilterExpired=true;
      }
    }
  }
  return isFilterExpired;
}
