def getServiceConfigurationRecommenderDict(self):
    parentRecommendConfDict = super(HDP25StackAdvisor, self).getServiceConfigurationRecommenderDict()
    childRecommendConfDict = {'RANGER': self.recommendRangerConfigurations, 'HBASE': self.recommendHBASEConfigurations, 'HIVE': self.recommendHIVEConfigurations, 'ATLAS': self.recommendAtlasConfigurations, 'RANGER_KMS': self.recommendRangerKMSConfigurations, 'STORM': self.recommendStormConfigurations, }
    parentRecommendConfDict.update(childRecommendConfDict)
    return parentRecommendConfDict
