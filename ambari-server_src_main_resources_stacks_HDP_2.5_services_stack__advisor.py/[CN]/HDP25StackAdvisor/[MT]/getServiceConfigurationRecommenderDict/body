def getServiceConfigurationRecommenderDict(self):
    parentRecommendConfDict = super(HDP25StackAdvisor, self).getServiceConfigurationRecommenderDict()
    childRecommendConfDict = {'RANGER': self.recommendRangerConfigurations, 'HIVE': self.recommendHIVEConfigurations, 'ATLAS': self.recommendAtlasConfigurations, }
    parentRecommendConfDict.update(childRecommendConfDict)
    return parentRecommendConfDict
