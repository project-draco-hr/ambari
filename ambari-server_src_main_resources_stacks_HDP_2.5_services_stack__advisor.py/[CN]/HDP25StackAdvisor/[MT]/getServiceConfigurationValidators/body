def getServiceConfigurationValidators(self):
    parentValidators = super(HDP25StackAdvisor, self).getServiceConfigurationValidators()
    childValidators = {'HIVE': {'hive-interactive-env': self.validateHiveInteractiveEnvConfigurations, }, 'YARN': {'yarn-site': self.validateYarnConfigurations, }, }
    self.mergeValidators(parentValidators, childValidators)
    return parentValidators
