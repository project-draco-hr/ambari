def create_cluster_from_blueprint(self, cluster_name, blueprint_name, host_groups, configurations=None, default_password=None):
    '\n        Create a new cluster.\n        @param cluster_name: Cluster cluster_name\n        @param blueprint_name: the name of the blueprint\n        @param host_groups: an array of host_group information\n        @param configurations: an array of configuration overrides\n        @param default_password: the default password to use for all password-requiring services\n        @return  ClusterModel object.\n        '
    return cluster._create_cluster_from_blueprint(self, cluster_name, blueprint_name, host_groups, configurations=configurations, default_password=default_password)
