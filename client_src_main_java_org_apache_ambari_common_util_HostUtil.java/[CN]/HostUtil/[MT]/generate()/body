{
  for (  String exp : expressions) {
    if (exp.indexOf('[') >= 0) {
      ranges.clear();
      rangesPaddingSize.clear();
      findRange(exp);
      String template=generateTemplate(exp);
      indices=new Integer[ranges.size()];
      compute(0,ranges.get(0),template);
    }
 else {
      hosts.add(exp);
    }
  }
  return hosts;
}
