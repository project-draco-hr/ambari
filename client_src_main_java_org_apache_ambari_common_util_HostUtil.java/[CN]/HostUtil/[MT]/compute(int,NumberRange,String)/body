{
  int depth2=depth;
  for (int i=range.getMinimumInteger(); i <= range.getMaximumInteger(); i++) {
    indices[depth]=new Integer(i);
    if (depth < ranges.size() - 1) {
      depth++;
      compute(depth,ranges.get(depth),template);
      depth=depth2;
    }
 else {
      String buffer=String.format(template,indices);
      hosts.add(buffer);
    }
  }
}
