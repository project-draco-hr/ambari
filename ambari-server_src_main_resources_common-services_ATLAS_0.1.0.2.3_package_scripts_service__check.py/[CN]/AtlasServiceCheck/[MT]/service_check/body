def service_check(self, env):
    import params
    env.set_params(params)
    if params.security_enabled:
        Execute(format('{kinit_path_local} -kt {smokeuser_keytab} {smokeuser_principal}'), user=params.smoke_test_user)
    try:
        Execute(params.smoke_cmd, user=params.metadata_user, tries=5, try_sleep=10)
        Logger.info('Atlas server up and running')
    except:
        Logger.debug('Atlas server not running')
