{
  TypedQuery<Long> query=entityManagerProvider.get().createQuery("SELECT stage.requestId " + "FROM StageEntity stage" + " WHERE stage.requestId = (SELECT max(stage.requestId) FROM StageEntity stage)",Long.class);
  Long result=daoUtils.selectSingle(query);
  if (result != null) {
    return result;
  }
 else {
    return 0;
  }
}
