def is_monitor_process_live(pid_file):
    '\n  Gets whether the AMS monitor represented by the specified file is running.\n  :param pid_file: the PID file of the monitor to check\n  :return: True if the monitor is running, False otherwise\n  '
    live = False
    try:
        check_process_status(pid_file)
        live = True
    except ComponentIsNotRunning:
        pass
    return live
