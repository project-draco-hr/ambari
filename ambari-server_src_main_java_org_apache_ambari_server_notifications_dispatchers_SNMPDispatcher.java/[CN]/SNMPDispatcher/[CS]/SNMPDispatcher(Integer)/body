{
  if (port != null && port >= 0 && port <= '\uffff') {
    this.port=port;
  }
  if (port != null) {
    LOG.info("Setting SNMP dispatch port: " + port);
    this.snmp=new Snmp(new DefaultUdpTransportMapping(new UdpAddress(port),true));
  }
 else {
    this.snmp=new Snmp(new DefaultUdpTransportMapping());
  }
}
