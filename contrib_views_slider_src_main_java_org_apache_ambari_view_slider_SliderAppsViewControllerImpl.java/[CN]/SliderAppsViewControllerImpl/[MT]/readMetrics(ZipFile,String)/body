{
  Map<String,Map<String,Map<String,Metric>>> metrics=null;
  try {
    InputStream inputStream=zipFile.getInputStream(zipFile.getEntry("jmx_metrics.json"));
    ObjectMapper mapper=new ObjectMapper();
    metrics=mapper.readValue(inputStream,new TypeReference<Map<String,Map<String,Map<String,Metric>>>>(){
    }
);
  }
 catch (  IOException e) {
    logger.info("Error reading metrics. " + e.getMessage());
  }
  return metrics;
}
