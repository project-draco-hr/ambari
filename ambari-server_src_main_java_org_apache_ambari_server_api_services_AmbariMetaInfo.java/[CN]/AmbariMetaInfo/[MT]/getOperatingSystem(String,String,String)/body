{
  Set<OperatingSystemInfo> operatingSystems=getOperatingSystems(stackName,version);
  if (operatingSystems.size() == 0)   throw new StackAccessException("stackName=" + stackName + ", stackVersion="+ version+ ", osType="+ osType);
  OperatingSystemInfo resultOperatingSystem=null;
  for (  OperatingSystemInfo operatingSystem : operatingSystems) {
    if (operatingSystem.getOsType().equals(osType))     resultOperatingSystem=operatingSystem;
  }
  if (resultOperatingSystem == null)   throw new StackAccessException("stackName=" + stackName + ", stackVersion="+ version+ ", osType="+ osType);
  return resultOperatingSystem;
}
