{
  Map<String,Map<String,PropertyInfo>> requiredStackProps=requiredProperties.get(new StackId(stackName,stackVersion));
  if (requiredStackProps != null) {
    Map<String,PropertyInfo> requiredServiceProperties=requiredStackProps.get(service);
    return requiredServiceProperties == null ? Collections.<String,PropertyInfo>emptyMap() : requiredServiceProperties;
  }
  return Collections.emptyMap();
}
