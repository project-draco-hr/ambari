{
  try {
    Unmarshaller um=jaxbContext.createUnmarshaller();
    String resourceName="org/apache/ambari/stacks/" + name + "-"+ revision+ ".xml";
    InputStream in=ClassLoader.getSystemResourceAsStream(resourceName);
    Stack bp=(Stack)um.unmarshal(in);
    bp.setName(name);
    bp.setRevision(Integer.toString(revision));
    addStack(bp);
  }
 catch (  IOException e) {
    Log.warn("Problem loading stack " + name + " rev "+ revision,e);
  }
catch (  JAXBException e) {
    Log.warn("Problem loading stack " + name + " rev "+ revision,e);
  }
}
