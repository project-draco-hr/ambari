{
  if (FilterLogData.INSTANCE.isAllowed(jsonBlock)) {
    for (    Output output : inputMarker.input.getOutputList()) {
      try {
        output.write(jsonBlock,inputMarker);
      }
 catch (      Exception e) {
        logger.error("Error writing. to " + output.getShortDescription(),e);
      }
    }
  }
}
