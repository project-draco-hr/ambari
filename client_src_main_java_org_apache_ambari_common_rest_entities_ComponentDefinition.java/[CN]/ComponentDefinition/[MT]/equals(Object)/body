{
  if (other == null || other.getClass() != getClass()) {
    return false;
  }
 else   if (other == this) {
    return true;
  }
 else {
    ComponentDefinition otherDefn=(ComponentDefinition)other;
    return isStringEqual(name,otherDefn.name) && isStringEqual(provider,otherDefn.provider) && isStringEqual(version,otherDefn.version);
  }
}
