{
  Injector injector=Guice.createInjector(new ControllerModule());
  try {
    LOG.info("Getting the controller");
    AmbariServer server=injector.getInstance(AmbariServer.class);
    if (server != null) {
      server.run();
    }
  }
 catch (  Throwable t) {
    LOG.error("Failed to run the Ambari Server",t);
  }
}
