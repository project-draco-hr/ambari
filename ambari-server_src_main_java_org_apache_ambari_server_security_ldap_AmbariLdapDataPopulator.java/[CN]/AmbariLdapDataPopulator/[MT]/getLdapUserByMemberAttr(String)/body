{
  LdapUserDto dto=getLdapUser(memberAttributeValue);
  if (dto == null) {
    Set<LdapUserDto> filteredLdapUsers=getFilteredLdapUsers(new EqualsFilter(OBJECT_CLASS_ATTRIBUTE,ldapServerProperties.getUserObjectClass()),getMemberFilter(memberAttributeValue));
    dto=(filteredLdapUsers.isEmpty()) ? null : filteredLdapUsers.iterator().next();
  }
  return dto;
}
