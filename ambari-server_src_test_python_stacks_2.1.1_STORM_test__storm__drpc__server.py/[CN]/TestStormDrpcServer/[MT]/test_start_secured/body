def test_start_secured(self):
    self.executeScript('2.1.1/services/STORM/package/scripts/drpc_server.py', classname='DrpcServer', command='start', config_file='secured.json')
    self.assert_configure_secured()
    self.assertResourceCalled('Execute', 'env PATH=$PATH:/usr/jdk64/jdk1.7.0_45/bin /usr/bin/storm drpc', wait_for_finish=False, not_if='ls /var/run/storm/drpc.pid >/dev/null 2>&1 && ps `cat /var/run/storm/drpc.pid` >/dev/null 2>&1', user='storm')
    self.assertResourceCalled('Execute', 'pgrep -f "^java.+backtype.storm.daemon.drpc$" > /var/run/storm/drpc.pid', logoutput=True, tries=6, user='storm', try_sleep=10)
    self.assertNoMoreResources()
