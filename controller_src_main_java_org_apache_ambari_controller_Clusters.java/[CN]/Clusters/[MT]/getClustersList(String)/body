{
  List<Cluster> list=new ArrayList<Cluster>();
  if (state.equals("ALL")) {
    list.addAll(this.operational_clusters.values());
  }
 else {
    for (    Cluster cls : this.operational_clusters.values()) {
      if (cls.getClusterState().getState().equals(state)) {
        list.add(cls);
      }
    }
  }
  return list;
}
