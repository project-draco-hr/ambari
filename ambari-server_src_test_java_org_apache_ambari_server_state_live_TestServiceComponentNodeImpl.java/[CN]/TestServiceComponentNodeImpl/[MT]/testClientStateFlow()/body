{
  ServiceComponentNodeImpl impl=createNewServiceComponentNode("svcComp","h1",true);
  runStateChanges(impl,ServiceComponentNodeEventType.NODE_SVCCOMP_INSTALL,ServiceComponentNodeLiveState.INIT,ServiceComponentNodeLiveState.INSTALLING,ServiceComponentNodeLiveState.INSTALL_FAILED,ServiceComponentNodeLiveState.INSTALLED);
  boolean exceptionThrown=false;
  try {
    runStateChanges(impl,ServiceComponentNodeEventType.NODE_SVCCOMP_START,ServiceComponentNodeLiveState.INSTALLED,ServiceComponentNodeLiveState.STARTING,ServiceComponentNodeLiveState.START_FAILED,ServiceComponentNodeLiveState.STARTED);
  }
 catch (  Exception e) {
    exceptionThrown=true;
  }
  Assert.assertTrue("Exception not thrown on invalid event",exceptionThrown);
  runStateChanges(impl,ServiceComponentNodeEventType.NODE_SVCCOMP_UNINSTALL,ServiceComponentNodeLiveState.INSTALLED,ServiceComponentNodeLiveState.UNINSTALLING,ServiceComponentNodeLiveState.UNINSTALL_FAILED,ServiceComponentNodeLiveState.UNINSTALLED);
  runStateChanges(impl,ServiceComponentNodeEventType.NODE_SVCCOMP_WIPEOUT,ServiceComponentNodeLiveState.UNINSTALLED,ServiceComponentNodeLiveState.WIPING_OUT,ServiceComponentNodeLiveState.WIPEOUT_FAILED,ServiceComponentNodeLiveState.INIT);
}
