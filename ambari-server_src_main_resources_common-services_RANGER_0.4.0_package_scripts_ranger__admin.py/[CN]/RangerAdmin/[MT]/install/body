def install(self, env):
    self.install_packages(env)
    import params
    env.set_params(params)
    if params.xml_configurations_supported:
        from setup_ranger_xml import setup_ranger_db
        setup_ranger_db()
    self.configure(env)
    if params.xml_configurations_supported:
        from setup_ranger_xml import setup_java_patch
        setup_java_patch()
