def selectionScheme(self, componentName):
    scheme = super(HDP21StackAdvisor, self).selectionScheme(componentName)
    if (scheme is None):
        return {'APP_TIMELINE_SERVER': {31: 1, 'else': 2, }, 'FALCON_SERVER': {6: 1, 31: 2, 'else': 3, }, }.get(componentName, None)
    else:
        return scheme
