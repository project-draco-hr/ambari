def getDBTypeAlias(self, databaseType):
    driverDict = {'NEW MYSQL DATABASE': 'mysql', 'NEW DERBY DATABASE': 'derby', 'EXISTING MYSQL DATABASE': 'mysql', 'EXISTING POSTGRESQL DATABASE': 'postgres', 'EXISTING ORACLE DATABASE': 'oracle', 'EXISTING SQL ANYWHERE DATABASE': 'sqla', }
    return driverDict.get(databaseType.upper())
