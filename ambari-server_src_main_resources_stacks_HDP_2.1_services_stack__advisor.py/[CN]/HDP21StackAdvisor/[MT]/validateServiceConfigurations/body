def validateServiceConfigurations(self, serviceName):
    validator = super(HDP21StackAdvisor, self).validateServiceConfigurations(serviceName)
    if (validator is None):
        return {'STORM': ['storm-site', self.validateStormConfigurations], 'HIVE': ['hive-site', self.validateHiveConfigurations], 'TEZ': ['tez-site', self.validateTezConfigurations], }.get(serviceName, None)
    else:
        return validator
