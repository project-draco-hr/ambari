@staticmethod
def is_hdp_stack_greater_or_equal_to(formatted_hdp_stack_version, compare_to_version):
    "\n    Gets whether the provided formatted_hdp_stack_version (normalized)\n    is greater than or equal to the specified stack version\n    :param formatted_hdp_stack_version: the version of stack to compare\n    :param compare_to_version: the version of stack to compare to\n    :return: True if the command's stack is greater than or equal to the specified version\n    "
    if ((formatted_hdp_stack_version is None) or (formatted_hdp_stack_version == '')):
        return False
    return (compare_versions(formatted_hdp_stack_version, compare_to_version) >= 0)
