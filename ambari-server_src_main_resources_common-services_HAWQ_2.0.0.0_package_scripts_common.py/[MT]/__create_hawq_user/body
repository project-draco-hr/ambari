def __create_hawq_user():
    '\n  Creates HAWQ user with default password and group.\n  '
    import params
    Group(constants.hawq_group, ignore_failures=True)
    User(constants.hawq_user, gid=constants.hawq_group, password=crypt.crypt(constants.hawq_password, 'salt'), groups=[constants.hawq_group, params.user_group], ignore_failures=True)
