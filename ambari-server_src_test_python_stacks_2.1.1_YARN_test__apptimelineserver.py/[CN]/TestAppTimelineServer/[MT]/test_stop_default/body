def test_stop_default(self):
    self.executeScript('2.0.6/services/YARN/package/scripts/application_timeline_server.py', classname='ApplicationTimelineServer', command='stop', config_file='default.json')
    self.assertResourceCalled('Execute', 'export HADOOP_LIBEXEC_DIR=/usr/lib/hadoop/libexec && /usr/lib/hadoop-yarn/sbin/yarn-daemon.sh --config /etc/hadoop/conf stop historyserver', user='yarn')
    self.assertResourceCalled('Execute', 'rm -f /var/run/hadoop-yarn/yarn/yarn-yarn-historyserver.pid', user='yarn')
    self.assertNoMoreResources()
