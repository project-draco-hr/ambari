{
  List<TaskWrapper> tasks=new ArrayList<TaskWrapper>();
  for (  String service : m_servicesToCheck) {
    tasks.add(new TaskWrapper(service,"",Collections.<String>emptySet(),new ServiceCheckTask()));
  }
  if (m_serviceCheck && m_servicesToCheck.size() > 0) {
    StageWrapper wrapper=new StageWrapper(StageWrapper.Type.SERVICE_CHECK,"Service Check " + StringUtils.join(m_servicesToCheck,", "),tasks.toArray(new TaskWrapper[0]));
    m_stages.add(wrapper);
  }
  return m_stages;
}
