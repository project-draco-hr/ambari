{
  List<TaskWrapper> tasks=new ArrayList<TaskWrapper>();
  for (  String service : serviceChecks) {
    tasks.add(new TaskWrapper(service,"",Collections.<String>emptySet(),new ServiceCheckTask()));
  }
  if (serviceChecks.size() > 0) {
    StageWrapper wrapper=new StageWrapper(StageWrapper.Type.SERVICE_CHECK,"Service Check " + StringUtils.join(serviceChecks,", "),tasks.toArray(new TaskWrapper[0]));
    stages.add(wrapper);
  }
  return stages;
}
