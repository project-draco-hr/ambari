{
switch (initial.getType()) {
case CONFIGURE:
case MANUAL:
    type=StageWrapper.Type.SERVER_SIDE_ACTION;
  break;
case EXECUTE:
type=StageWrapper.Type.RU_TASKS;
break;
case RESTART:
type=StageWrapper.Type.RESTART;
break;
case SERVICE_CHECK:
type=StageWrapper.Type.SERVICE_CHECK;
break;
}
tasks.add(initial);
}
