{
  if (!getResultsCache().containsKey(key)) {
    Cursor resultSet=null;
    try {
      resultSet=makeResultsSet.call();
    }
 catch (    Exception ex) {
      throw new ServiceFormattedException(ex.getMessage(),ex);
    }
    getResultsCache().put(key,resultSet);
  }
  return getResultsCache().get(key);
}
