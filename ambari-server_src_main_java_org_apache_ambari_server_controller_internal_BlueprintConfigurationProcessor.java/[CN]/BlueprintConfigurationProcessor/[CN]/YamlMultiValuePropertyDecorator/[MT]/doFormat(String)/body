{
  StringBuilder sb=new StringBuilder();
  if (origValue != null) {
    sb.append("[");
    boolean isFirst=true;
    for (    String value : origValue.split(",")) {
      if (!isFirst) {
        sb.append(",");
      }
 else {
        isFirst=false;
      }
      sb.append("'");
      sb.append(value);
      sb.append("'");
    }
    sb.append("]");
  }
  return sb.toString();
}
