{
  Condition condition=new Condition(null,null,null,null,startTime,endTime,null,true);
  condition.setNoLimit();
  condition.setFetchSize(resultsetFetchSize);
  condition.setStatement(String.format(GET_CLUSTER_AGGREGATE_SQL,PhoenixTransactSQL.getNaiveTimeRangeHint(startTime,NATIVE_TIME_RANGE_DELTA)));
  condition.addOrderByColumn("METRIC_NAME");
  condition.addOrderByColumn("APP_ID");
  condition.addOrderByColumn("INSTANCE_ID");
  condition.addOrderByColumn("SERVER_TIME");
  return condition;
}
