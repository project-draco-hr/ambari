{
  if (null == executionStages) {
    return Collections.emptyList();
  }
  List<StageWrapper> results=new ArrayList<StageWrapper>();
  if (executionStages != null) {
    for (    ExecuteStage execution : executionStages) {
      if (null != execution.intendedDirection && execution.intendedDirection != ctx.getDirection()) {
        continue;
      }
      Task task=execution.task;
      StageWrapper wrapper=null;
switch (task.getType()) {
case MANUAL:
        wrapper=getManualStageWrapper(ctx,execution);
      break;
case CONFIGURE:
case SERVER_ACTION:
    wrapper=new StageWrapper(StageWrapper.Type.SERVER_SIDE_ACTION,execution.title,new TaskWrapper(null,null,Collections.<String>emptySet(),task));
  break;
case EXECUTE:
wrapper=getExecuteStageWrapper(ctx,execution);
break;
default :
break;
}
if (null != wrapper) {
results.add(wrapper);
}
}
}
return results;
}
