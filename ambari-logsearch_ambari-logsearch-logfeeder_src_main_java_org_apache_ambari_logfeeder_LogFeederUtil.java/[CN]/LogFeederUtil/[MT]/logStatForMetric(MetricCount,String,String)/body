{
  long currStat=metric.count;
  long currMS=System.currentTimeMillis();
  if (currStat > metric.prevLogCount) {
    if (postFix == null) {
      postFix="";
    }
    logger.info(prefixStr + ": total_count=" + metric.count+ ", duration="+ (currMS - metric.prevLogMS) / 1000 + " secs, count=" + (currStat - metric.prevLogCount) + postFix);
  }
  metric.prevLogCount=currStat;
  metric.prevLogMS=currMS;
}
