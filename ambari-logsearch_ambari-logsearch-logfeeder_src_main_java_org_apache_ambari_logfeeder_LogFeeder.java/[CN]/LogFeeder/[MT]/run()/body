{
  try {
    Date startTime=new Date();
    this.init();
    Date endTime=new Date();
    logger.info("Took " + (endTime.getTime() - startTime.getTime()) + " ms to initialize");
    this.monitor();
    this.waitOnAllDaemonThreads();
  }
 catch (  Throwable t) {
    logger.fatal("Caught exception in main.",t);
    System.exit(1);
  }
}
