{
  this.executing=true;
  executeJob(new Operation<SyncJob>(){
    @Override SyncJob getJob(){
      return job;
    }
    @Override Optional<ResultSet> call(    HiveConnection connection) throws SQLException {
      return connectionDelegate.executeSync(connection,job);
    }
    @Override String notConnectedErrorMessage(){
      return "Cannot execute sync job for user: " + job.getUsername() + ". Not connected to Hive";
    }
    @Override String executionFailedErrorMessage(){
      return "Failed to execute Jdbc Statement";
    }
  }
);
}
