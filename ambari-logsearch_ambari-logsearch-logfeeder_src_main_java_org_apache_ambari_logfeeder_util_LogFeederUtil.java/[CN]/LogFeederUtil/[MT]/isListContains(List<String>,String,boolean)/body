{
  if (list != null) {
    for (    String value : list) {
      if (value != null) {
        if (caseSensitive) {
          if (value.equals(str)) {
            return true;
          }
        }
 else {
          if (value.equalsIgnoreCase(str)) {
            return true;
          }
        }
        if (value.equalsIgnoreCase(LogFeederConstants.ALL)) {
          return true;
        }
      }
    }
  }
  return false;
}
