{
  if (objValue == null) {
    return retValue;
  }
  String strValue=objValue.toString();
  if (!StringUtils.isEmpty(strValue)) {
    try {
      retValue=Integer.parseInt(strValue);
    }
 catch (    Throwable t) {
      logger.error("Error parsing integer value. str=" + strValue + ", "+ errMessage);
    }
  }
  return retValue;
}
