{
  Class clazz=findLoadedClass(name);
  if (clazz == null) {
    try {
      clazz=this.findClass(name);
    }
 catch (    ClassNotFoundException e) {
      ClassLoader parentClassLoader=getParent();
      if (parentClassLoader != null) {
        clazz=parentClassLoader.loadClass(name);
      }
      if (clazz == null) {
        throw e;
      }
    }
  }
  if (resolve) {
    resolveClass(clazz);
  }
  return clazz;
}
