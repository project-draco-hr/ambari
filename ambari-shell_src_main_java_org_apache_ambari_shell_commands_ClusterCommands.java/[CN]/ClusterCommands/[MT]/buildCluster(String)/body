{
  String message;
  if (client.doesBlueprintExists(id)) {
    context.setFocus(id,FocusType.CLUSTER_BUILD);
    context.setHint(Hints.ASSIGN_HOSTS);
    message=String.format("%s\n%s",renderSingleMap(client.getHostNames(),"HOSTNAME","STATE"),renderMultiValueMap(client.getBlueprintMap(id),"HOSTGROUP","COMPONENT"));
    createNewHostGroups();
  }
 else {
    message="Not a valid blueprint id";
  }
  return message;
}
