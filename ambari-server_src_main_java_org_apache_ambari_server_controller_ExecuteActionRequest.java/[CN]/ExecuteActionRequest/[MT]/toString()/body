{
  return (new StringBuilder()).append("isCommand :" + isCommand().toString()).append(", action :" + actionName).append(", command :" + commandName).append(", inputs :" + parameters.toString()).append(", targetService :" + serviceName).append(", targetComponent :" + componentName).append(", targetHosts :" + hosts.toString()).append(", clusterName :" + clusterName).toString();
}
