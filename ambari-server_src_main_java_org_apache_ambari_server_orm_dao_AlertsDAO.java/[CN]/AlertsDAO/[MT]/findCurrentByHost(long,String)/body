{
  TypedQuery<AlertCurrentEntity> query=entityManagerProvider.get().createNamedQuery("AlertCurrentEntity.findByHost",AlertCurrentEntity.class);
  query.setParameter("clusterId",clusterId);
  query.setParameter("hostName",hostName);
  query.setParameter("inlist",EnumSet.of(Scope.ANY,Scope.HOST));
  query=setQueryRefreshHint(query);
  return daoUtils.selectList(query);
}
