{
  TypedQuery<AlertCurrentEntity> query=m_entityManagerProvider.get().createNamedQuery("AlertCurrentEntity.removeByService",AlertCurrentEntity.class);
  query.setParameter("serviceName",serviceName);
  int removedItems=query.executeUpdate();
  m_alertEventPublisher.publish(new AggregateAlertRecalculateEvent(clusterId));
  return removedItems;
}
