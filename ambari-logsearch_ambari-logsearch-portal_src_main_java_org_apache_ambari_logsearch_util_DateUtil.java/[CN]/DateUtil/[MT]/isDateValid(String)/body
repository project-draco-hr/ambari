{
  if (stringUtil.isEmpty(value)) {
    return false;
  }
  Date date=null;
  try {
    SimpleDateFormat sdf=new SimpleDateFormat(LogSearchConstants.SOLR_DATE_FORMAT_PREFIX_Z);
    date=sdf.parse(value);
    if (!value.equals(sdf.format(date))) {
      date=null;
    }
  }
 catch (  Exception ex) {
  }
  return date != null;
}
