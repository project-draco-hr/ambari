def _create_empty_options_mock(self):
    options = MagicMock()
    options.mpack_path = None
    options.purge = None
    options.purge_list = ','.join([STACK_DEFINITIONS_RESOURCE_NAME, MPACKS_RESOURCE_NAME])
    options.force = None
    options.verbose = None
    options.replay_mode = False
    return options
