{
synchronized (pendingTasks) {
    Iterator<TopologyTask> iter=pendingTasks.get(TopologyTask.Type.INSTALL).iterator();
    while (iter.hasNext()) {
      iter.next().run();
      iter.remove();
    }
    iter=pendingTasks.get(TopologyTask.Type.START).iterator();
    while (iter.hasNext()) {
      iter.next().run();
      iter.remove();
    }
  }
}
