{
  if (executor == null) {
    LinkedBlockingQueue<Runnable> queue=new LinkedBlockingQueue<Runnable>();
    int THREAD_POOL_CORE_SIZE=2;
    int THREAD_POOL_MAX_SIZE=100;
    int THREAD_POOL_TIMEOUT=Integer.MAX_VALUE;
    ThreadPoolExecutor threadPoolExecutor=new ThreadPoolExecutor(THREAD_POOL_CORE_SIZE,THREAD_POOL_MAX_SIZE,THREAD_POOL_TIMEOUT,TimeUnit.SECONDS,queue);
    executor=threadPoolExecutor;
  }
  return executor;
}
