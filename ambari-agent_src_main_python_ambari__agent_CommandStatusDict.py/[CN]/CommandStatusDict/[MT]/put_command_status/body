def put_command_status(self, command, new_report):
    '\n    Stores new version of report for command (replaces previous)\n    '
    if ('taskId' in command):
        key = command['taskId']
    else:
        key = id(command)
    with self.lock:
        self.current_state[key] = (command, new_report)
    self.callback_action()
