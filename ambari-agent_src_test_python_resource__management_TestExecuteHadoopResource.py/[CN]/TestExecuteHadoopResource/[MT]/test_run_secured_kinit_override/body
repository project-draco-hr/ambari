@patch('resource_management.core.providers.system.ExecuteProvider')
def test_run_secured_kinit_override(self, execute_mock):
    '\n    Test security_enabled=True and kinit_override=True behaviour\n    '
    with Environment('/') as env:
        ExecuteHadoop('command', action='run', kinit_path_local='path', conf_dir='conf_dir', user='user', tries=1, keytab='keytab', security_enabled=True, kinit_override=True, try_sleep=0, logoutput=True)
        self.assertEqual(execute_mock.call_count, 1)
        self.assertEqual(execute_mock.call_args_list[0][0][0].command, 'hadoop --config conf_dir command')
