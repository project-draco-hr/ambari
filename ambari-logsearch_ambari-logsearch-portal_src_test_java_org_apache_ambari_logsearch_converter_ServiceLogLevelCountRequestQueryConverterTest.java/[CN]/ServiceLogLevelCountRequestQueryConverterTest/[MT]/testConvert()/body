{
  ServiceLogLevelCountRequest request=new ServiceLogLevelCountRequest();
  fillBaseLogRequestWithTestData(request);
  SolrQuery query=new DefaultQueryParser().doConstructSolrQuery(underTest.convert(request));
  assertEquals("?q=*%3A*&rows=0&fq=logtime%3A%5B2016-09-13T22%3A00%3A01.000Z+TO+2016-09-14T22%3A00%3A01.000Z%5D" + "&fq=log_message%3A*myincludemessage*&fq=-log_message%3A*myexcludemessage*&fq=type%3A%28logsearch_app+secure_log%29" + "&fq=-type%3A%28hst_agent+system_message%29&facet=true&facet.mincount=1&facet.limit=-1&facet.field=level",query.toQueryString());
}
