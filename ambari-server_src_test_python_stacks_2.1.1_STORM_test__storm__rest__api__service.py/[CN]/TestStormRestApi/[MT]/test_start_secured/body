def test_start_secured(self):
    self.executeScript('2.1.1/services/STORM/package/scripts/rest_api.py', classname='StormRestApi', command='start', config_file='secured.json')
    self.assert_configure_secured()
    self.assertResourceCalled('Execute', 'env PATH=$PATH:/usr/jdk64/jdk1.7.0_45/bin java -jar /usr/lib/storm/contrib/storm-rest/`ls /usr/lib/storm/contrib/storm-rest | grep -wE storm-rest-[0-9.-]+\\.jar` server /etc/storm/conf/config.yaml > /var/log/storm/restapi.log', wait_for_finish=False, not_if='ls /var/run/storm/restapi.pid >/dev/null 2>&1 && ps `cat /var/run/storm/restapi.pid` >/dev/null 2>&1', user='storm')
    self.assertResourceCalled('Execute', 'pgrep -f "java -jar /usr/lib/storm/contrib/storm-rest/`ls /usr/lib/storm/contrib/storm-rest | grep -wE storm-rest-[0-9.-]+\\.jar` server" > /var/run/storm/restapi.pid', logoutput=True, tries=6, user='storm', try_sleep=10)
    self.assertNoMoreResources()
