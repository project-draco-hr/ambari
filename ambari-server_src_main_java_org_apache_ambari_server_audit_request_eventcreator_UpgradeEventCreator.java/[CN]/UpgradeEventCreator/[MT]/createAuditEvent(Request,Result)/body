{
  return AddUpgradeRequestAuditEvent.builder().withTimestamp(System.currentTimeMillis()).withRequestType(request.getRequestType()).withResultStatus(result.getStatus()).withUrl(request.getURI()).withRemoteIp(request.getRemoteAddress()).withRepositoryVersion(getProperty(request,"repository_version")).withUpgradeType(getProperty(request,"upgrade_type")).withClusterName(getProperty(request,"cluster_name")).build();
}
