{
  for (  Stage s : stageList) {
    if (s.getRequestId() == requestId) {
      for (      String host : s.getHosts()) {
        for (        ExecutionCommand cmd : s.getExecutionCommands(host)) {
          HostRoleStatus status=s.getHostRoleStatus(host,cmd.getRole().toString());
          if (status.equals(HostRoleStatus.IN_PROGRESS) || status.equals(HostRoleStatus.QUEUED) || status.equals(HostRoleStatus.PENDING)) {
            s.setHostRoleStatus(host,cmd.getRole().toString(),HostRoleStatus.ABORTED);
          }
        }
      }
    }
  }
}
