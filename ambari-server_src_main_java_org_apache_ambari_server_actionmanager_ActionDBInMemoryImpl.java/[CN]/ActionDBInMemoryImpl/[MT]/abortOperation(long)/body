{
  for (  Stage s : stageList) {
    if (s.getRequestId() == requestId) {
      for (      String host : s.getHostActions().keySet()) {
        for (        HostRoleCommand role : s.getHostActions().get(host).getRoleCommands()) {
          role.setStatus(HostRoleStatus.ABORTED);
        }
      }
    }
  }
}
