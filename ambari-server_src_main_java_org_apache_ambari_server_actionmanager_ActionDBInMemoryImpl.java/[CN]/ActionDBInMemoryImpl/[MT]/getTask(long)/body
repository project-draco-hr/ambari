{
  for (  Stage s : stageList) {
    for (    String host : s.getHosts()) {
      for (      ExecutionCommand cmd : s.getExecutionCommands(host)) {
        if (cmd.getTaskId() == taskId) {
          return s.getHostRoleCommand(host,cmd.getRole().toString());
        }
      }
    }
  }
  return null;
}
