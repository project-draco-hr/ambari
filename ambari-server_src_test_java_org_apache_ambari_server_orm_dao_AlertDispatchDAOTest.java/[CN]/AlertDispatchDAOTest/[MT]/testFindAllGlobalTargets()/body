{
  List<AlertTargetEntity> targets=m_dao.findAllGlobalTargets();
  assertNotNull(targets);
  assertEquals(0,targets.size());
  m_helper.createGlobalAlertTarget();
  m_helper.createGlobalAlertTarget();
  m_helper.createGlobalAlertTarget();
  targets=m_dao.findAllGlobalTargets();
  assertEquals(3,targets.size());
}
