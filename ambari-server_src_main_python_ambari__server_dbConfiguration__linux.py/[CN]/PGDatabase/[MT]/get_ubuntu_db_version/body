@staticmethod
def get_ubuntu_db_version():
    'Return installed version of postgre server. In case of several\n    installed versions will be returned a more new one.\n    '
    postgre_ver = ''
    if os.path.isdir(UBUNTU_PG_HBA_ROOT):
        postgre_ver = sorted([fld for fld in os.listdir(UBUNTU_PG_HBA_ROOT) if os.path.isdir(os.path.join(UBUNTU_PG_HBA_ROOT, fld))], reverse=True)
        if (len(postgre_ver) > 0):
            return postgre_ver[0]
    return postgre_ver
