{
  if (this == obj) {
    return true;
  }
  if (!super.equals(obj)) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  WebSource other=(WebSource)obj;
  if (uri == null) {
    if (other.uri != null) {
      return false;
    }
  }
 else   if (!uri.equals(other.uri)) {
    return false;
  }
  return true;
}
