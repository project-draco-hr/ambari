{
  assertEquals("Incorrect number of cluster requests ",expectedConfigTypes.length,request.getDesiredConfig().size());
  Set<String> foundConfigTypes=new HashSet<String>();
  for (  ConfigurationRequest configRequest : request.getDesiredConfig()) {
    foundConfigTypes.add(configRequest.getType());
  }
  for (  String expectedType : expectedConfigTypes) {
    assertTrue("Expected config type not found in this config request",foundConfigTypes.contains(expectedType));
  }
}
