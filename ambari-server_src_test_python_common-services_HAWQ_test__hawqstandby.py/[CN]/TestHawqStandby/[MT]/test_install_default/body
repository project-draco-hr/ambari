@patch('common.__set_osparams')
def test_install_default(self, set_osparams_mock):
    self.executeScript((self.HAWQ_PACKAGE_DIR + '/scripts/hawqstandby.py'), classname='HawqStandby', command='install', config_dict=self.config_dict, stack_version=self.STACK_VERSION, target=self.TARGET_COMMON_SERVICES)
    self.asserts_for_configure()
    self.assertNoMoreResources()
