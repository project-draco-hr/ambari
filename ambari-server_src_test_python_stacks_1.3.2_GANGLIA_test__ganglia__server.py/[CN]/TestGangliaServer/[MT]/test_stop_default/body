def test_stop_default(self):
    self.executeScript('1.3.2/services/GANGLIA/package/scripts/ganglia_server.py', classname='GangliaServer', command='stop', config_file='default.json')
    self.assertResourceCalled('Execute', 'service hdp-gmetad stop >> /tmp/gmetad.log  2>&1 ; /bin/ps auwx | /bin/grep [g]metad  >> /tmp/gmetad.log  2>&1', path=['/usr/sbin:/sbin:/usr/local/bin:/bin:/usr/bin'])
    self.assertResourceCalled('MonitorWebserver', 'restart')
    self.assertNoMoreResources()
