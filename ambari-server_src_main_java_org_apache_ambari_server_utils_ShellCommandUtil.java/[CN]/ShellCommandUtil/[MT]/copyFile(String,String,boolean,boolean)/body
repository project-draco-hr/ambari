{
  ArrayList<String> command=new ArrayList<String>();
  if (WINDOWS) {
    command.add("copy");
    if (force) {
      command.add("/Y");
    }
  }
 else {
    command.add("cp");
    command.add("-p");
    if (force) {
      command.add("-f");
    }
  }
  command.add(srcFile);
  command.add(destFile);
  return runCommand(command.toArray(new String[command.size()]),null,sudo);
}
