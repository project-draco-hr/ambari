{
  if (hostPortRestrictionHandler == null) {
    HostPortRestrictionHandler hostPortRestrictionHandlerTmp=new HostPortRestrictionHandler(viewContext.getAmbariProperty(Configuration.PROXY_ALLOWED_HOST_PORTS.getKey()));
    hostPortRestrictionHandler=hostPortRestrictionHandlerTmp;
  }
  return hostPortRestrictionHandler;
}
