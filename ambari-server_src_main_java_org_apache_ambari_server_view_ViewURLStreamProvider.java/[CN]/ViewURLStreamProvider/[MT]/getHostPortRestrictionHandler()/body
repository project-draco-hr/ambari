{
  if (this.hostPortRestrictionHandler == null) {
    HostPortRestrictionHandler hostPortRestrictionHandlerTmp=new HostPortRestrictionHandler(this.viewContext.getAmbariProperty(Configuration.PROXY_ALLOWED_HOST_PORTS));
    this.hostPortRestrictionHandler=hostPortRestrictionHandlerTmp;
  }
  return this.hostPortRestrictionHandler;
}
