{
  errorMetric.count++;
  final String LOG_MESSAGE_KEY=this.getClass().getSimpleName() + "_PARSEERROR";
  LogFeederUtil.logErrorMessageByInterval(LOG_MESSAGE_KEY,"Error parsing string. length=" + inputStr.length() + ", input="+ input.getShortDescription()+ ". First upto 100 characters="+ LogFeederUtil.subString(inputStr,100),null,logger,Level.ERROR);
}
