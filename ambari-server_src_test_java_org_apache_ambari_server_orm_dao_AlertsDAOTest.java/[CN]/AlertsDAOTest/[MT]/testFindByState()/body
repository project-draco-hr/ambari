{
  List<AlertState> allStates=new ArrayList<AlertState>();
  allStates.add(AlertState.OK);
  allStates.add(AlertState.WARNING);
  allStates.add(AlertState.CRITICAL);
  List<AlertHistoryEntity> history=m_dao.findAll(m_clusterId,allStates);
  assertNotNull(history);
  assertEquals(50,history.size());
  history=m_dao.findAll(m_clusterId,Collections.singletonList(AlertState.OK));
  assertNotNull(history);
  assertEquals(40,history.size());
  history=m_dao.findAll(m_clusterId,Collections.singletonList(AlertState.CRITICAL));
  assertNotNull(history);
  assertEquals(10,history.size());
  history=m_dao.findAll(m_clusterId,Collections.singletonList(AlertState.WARNING));
  assertNotNull(history);
  assertEquals(0,history.size());
}
