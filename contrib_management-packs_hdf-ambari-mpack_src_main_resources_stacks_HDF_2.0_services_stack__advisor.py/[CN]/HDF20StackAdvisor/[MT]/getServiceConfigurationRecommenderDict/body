def getServiceConfigurationRecommenderDict(self):
    return {'KAFKA': self.recommendKAFKAConfigurations, 'NIFI': self.recommendNIFIConfigurations, 'STORM': self.recommendStormConfigurations, 'AMBARI_METRICS': self.recommendAmsConfigurations, 'RANGER': self.recommendRangerConfigurations, 'LOGSEARCH': self.recommendLogsearchConfigurations, }
