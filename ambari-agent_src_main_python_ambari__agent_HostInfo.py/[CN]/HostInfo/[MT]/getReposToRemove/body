def getReposToRemove(self, repos, ignoreList):
    reposToRemove = []
    for repo in repos:
        addToRemoveList = True
        for ignoreRepo in ignoreList:
            if self.packages.nameMatch(ignoreRepo, repo):
                addToRemoveList = False
                continue
        if addToRemoveList:
            reposToRemove.append(repo)
    return reposToRemove
