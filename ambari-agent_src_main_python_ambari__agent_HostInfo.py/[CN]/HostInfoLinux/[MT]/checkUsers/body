def checkUsers(self, users, results):
    f = open('/etc/passwd', 'r')
    for userLine in f:
        fields = userLine.split(':')
        if (fields[0] in users):
            result = {}
            homeDir = fields[5]
            result['name'] = fields[0]
            result['homeDir'] = fields[5]
            result['status'] = 'Available'
            results.append(result)
