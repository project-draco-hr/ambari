{
  return new Predicate<PrivilegeEntity>(){
    @Override public boolean apply(    final PrivilegeEntity privilegeEntity){
      return !FluentIterable.from(existingPrivileges).anyMatch(new com.google.common.base.Predicate<PrivilegeEntity>(){
        @Override public boolean apply(        PrivilegeEntity directPrivilegeEntity){
          return directPrivilegeEntity.getResource().getId().equals(privilegeEntity.getResource().getId()) && directPrivilegeEntity.getPermission().getId().equals(privilegeEntity.getPermission().getId());
        }
      }
);
    }
  }
;
}
