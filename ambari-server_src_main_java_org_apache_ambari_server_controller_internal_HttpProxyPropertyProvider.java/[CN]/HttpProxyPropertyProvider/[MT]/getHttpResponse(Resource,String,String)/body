{
  InputStream in=null;
  try {
    in=streamProvider.readFrom(url);
    r.setProperty(propertyIdToSet,new JsonPropertyParser().parse(IOUtils.toString(in,"UTF-8")));
  }
 catch (  IOException ioe) {
    LOG.error("Error reading HTTP response from " + url);
  }
 finally {
    if (null != in) {
      try {
        in.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
}
