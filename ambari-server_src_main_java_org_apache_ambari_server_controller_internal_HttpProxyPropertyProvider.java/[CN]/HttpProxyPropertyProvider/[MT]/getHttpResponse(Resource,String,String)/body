{
  InputStream in=null;
  try {
    in=streamProvider.readFrom(url);
    String str=IOUtils.toString(in,"UTF-8");
    r.setProperty(propertyIdToSet,str);
  }
 catch (  IOException ioe) {
    LOG.error("Error reading HTTP response from " + url);
  }
 finally {
    if (null != in) {
      try {
        in.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
}
