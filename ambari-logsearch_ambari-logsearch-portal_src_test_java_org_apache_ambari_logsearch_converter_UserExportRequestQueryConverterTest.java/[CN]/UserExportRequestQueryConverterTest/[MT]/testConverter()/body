{
  UserExportRequest request=new UserExportRequest();
  fillBaseLogRequestWithTestData(request);
  request.setFormat("myFormat");
  SolrQuery query=new DefaultQueryParser().doConstructSolrQuery(underTest.convert(request));
  assertEquals("?q=*%3A*&rows=0&fq=evtTime%3A%5B2016-09-13T22%3A00%3A01.000Z+TO+2016-09-14T22%3A00%3A01.000Z%5D" + "&fq=log_message%3A*myincludemessage*&fq=-log_message%3A*myexcludemessage*&fq=repo%3A%28logsearch_app+secure_log%29" + "&fq=-repo%3A%28hst_agent+system_message%29&facet=true&facet.mincount=1&facet.limit=10&facet.pivot=reqUser%2Crepo&facet.pivot=resource%2Crepo",query.toQueryString());
}
