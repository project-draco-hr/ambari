{
  UserExportRequest request=new UserExportRequest();
  SolrQuery query=new DefaultQueryParser().doConstructSolrQuery(underTest.convert(request));
  assertEquals("?q=*%3A*&rows=0&fq=evtTime%3A%5B*+TO+*%5D&facet=true&facet.mincount=1&facet.limit=10" + "&facet.pivot=reqUser%2Crepo&facet.pivot=resource%2Crepo",query.toQueryString());
}
