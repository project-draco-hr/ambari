{
  String[] urlSplit=remoteAmbariClusterEntity.getUrl().split("/");
  this.name=urlSplit[urlSplit.length - 1];
  this.streamProvider=new RemoteAmbariStreamProvider(remoteAmbariClusterEntity.getUrl(),remoteAmbariClusterEntity.getUsername(),remoteAmbariClusterEntity.getPassword(),config.getRequestConnectTimeout(),config.getRequestReadTimeout());
}
