{
  ComponentSSLConfiguration configuration=ComponentSSLConfiguration.instance();
  org.apache.ambari.server.controller.internal.URLStreamProvider streamProvider=new org.apache.ambari.server.controller.internal.URLStreamProvider(DEFAULT_REQUEST_CONNECT_TIMEOUT,DEFAULT_REQUEST_READ_TIMEOUT,configuration.getTruststorePath(),configuration.getTruststorePassword(),configuration.getTruststoreType());
  return new ViewURLStreamProvider(streamProvider);
}
