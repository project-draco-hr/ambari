{
  if (authToLocalRules != null && !authToLocalRules.isEmpty()) {
    String[] rules=authToLocalRules.split("RULE:|DEFAULT");
    for (    String r : rules) {
      r=r.trim();
      if (!r.isEmpty()) {
        Rule rule=createRule(r);
        setRules.add(rule);
        addDefaultRealmRule(rule.getPrincipal());
      }
    }
  }
}
