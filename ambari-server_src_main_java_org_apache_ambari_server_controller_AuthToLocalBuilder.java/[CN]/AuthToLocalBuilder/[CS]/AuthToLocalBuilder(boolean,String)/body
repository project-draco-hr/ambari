{
  this.caseInsensitiveUser=caseInsensitiveUserSupport;
  if ((additionalRealms != null) && !additionalRealms.isEmpty()) {
    for (    String realm : additionalRealms.split("\\s*(?:\\r?\\n|,)\\s*")) {
      realm=realm.trim();
      if (!realm.isEmpty()) {
        this.additionalRealms.add(realm);
      }
    }
  }
}
