{
  String strValue=getStringValue(key);
  int retValue=defaultValue;
  if (!StringUtils.isEmpty(strValue)) {
    try {
      retValue=Integer.parseInt(strValue);
    }
 catch (    Throwable t) {
      logger.error("Error parsing integer value. key=" + key + ", value="+ strValue);
    }
  }
  return retValue;
}
