{
  String orderByStr=" ORDER BY ";
  if (!orderByColumns.isEmpty()) {
    StringBuilder sb=new StringBuilder(orderByStr);
    for (    String orderByColumn : orderByColumns) {
      if (sb.length() != orderByStr.length()) {
        sb.append(", ");
      }
      sb.append(orderByColumn);
    }
    sb.append(" ");
    return sb.toString();
  }
  return null;
}
