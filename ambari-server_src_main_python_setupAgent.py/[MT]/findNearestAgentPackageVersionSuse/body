def findNearestAgentPackageVersionSuse(projectVersion):
    if (projectVersion == ''):
        projectVersion = '  '
    zypperCommand = ['bash', '-c', (("zypper search -s --match-exact ambari-agent | grep '" + projectVersion) + "' | cut -d '|' -f 4 | head -n1 | sed -e 's/-\\w[^:]*//1' ")]
    return execOsCommand(zypperCommand)
