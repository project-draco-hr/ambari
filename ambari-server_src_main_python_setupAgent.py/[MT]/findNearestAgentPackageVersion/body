def findNearestAgentPackageVersion(projectVersion):
    if (projectVersion == ''):
        projectVersion = '  '
    yumCommand = ['bash', '-c', (("yum list all ambari-agent | grep '" + projectVersion) + "' | sed -re 's/\\s+/ /g' | cut -d ' ' -f 2 | head -n1 | sed -e 's/-\\w[^:]*//1' ")]
    return execOsCommand(yumCommand)
