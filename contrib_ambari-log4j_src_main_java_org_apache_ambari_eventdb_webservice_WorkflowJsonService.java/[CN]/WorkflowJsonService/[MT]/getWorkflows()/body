{
  Workflows workflows=new Workflows();
  try {
    PostgresConnector conn=new PostgresConnector(servletContext.getInitParameter(HOSTNAME),servletContext.getInitParameter(DBNAME),servletContext.getInitParameter(USERNAME),servletContext.getInitParameter(PASSWORD));
    workflows.setWorkflows(conn.fetchWorkflows());
    conn.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
    workflows.setWorkflows(EMPTY_WORKFLOWS);
  }
  return workflows;
}
