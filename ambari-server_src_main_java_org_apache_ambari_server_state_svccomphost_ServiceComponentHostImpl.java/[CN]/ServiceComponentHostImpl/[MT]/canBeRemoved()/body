{
  try {
    readLock.lock();
    State desiredState=getDesiredState();
    State liveState=getState();
    if ((desiredState == State.INIT || desiredState == State.UNINSTALLED) && (liveState == State.INIT || liveState == State.UNINSTALLED)) {
      return true;
    }
  }
  finally {
    readLock.unlock();
  }
  return false;
}
