{
  clusterGlobalLock.readLock().lock();
  boolean schLockAcquired=false;
  try {
    schLockAcquired=readLock.tryLock();
    if (!schLockAcquired) {
      return false;
    }
    return (getState().isRemovableState());
  }
  finally {
    if (schLockAcquired) {
      readLock.unlock();
    }
    clusterGlobalLock.readLock().unlock();
  }
}
