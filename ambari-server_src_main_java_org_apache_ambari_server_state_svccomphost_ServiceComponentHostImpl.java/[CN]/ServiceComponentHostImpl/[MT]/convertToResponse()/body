{
  clusterGlobalLock.readLock().lock();
  try {
    readLock.lock();
    try {
      ServiceComponentHostResponse r=new ServiceComponentHostResponse(serviceComponent.getClusterName(),serviceComponent.getServiceName(),serviceComponent.getName(),getHostName(),getState().toString(),getStackVersion().getStackId(),getDesiredState().toString(),getDesiredStackVersion().getStackId());
      r.setActualConfigs(actualConfigs);
      try {
        r.setStaleConfig(helper.isStaleConfigs(this));
      }
 catch (      Exception e) {
        LOG.error("Could not determine stale config",e);
      }
      return r;
    }
  finally {
      readLock.unlock();
    }
  }
  finally {
    clusterGlobalLock.readLock().unlock();
  }
}
