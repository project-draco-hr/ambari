{
  try {
    readLock.lock();
    ServiceComponentHostResponse r=new ServiceComponentHostResponse(serviceComponent.getClusterName(),serviceComponent.getServiceName(),serviceComponent.getName(),getHostName(),configs,desiredConfigs,getState().toString(),getStackVersion().getStackId(),getDesiredState().toString());
    r.setHa_status(ha_status);
    return r;
  }
  finally {
    readLock.unlock();
  }
}
