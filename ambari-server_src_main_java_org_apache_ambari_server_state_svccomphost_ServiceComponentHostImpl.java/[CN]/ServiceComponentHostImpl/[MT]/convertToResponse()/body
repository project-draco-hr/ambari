{
  try {
    readLock.lock();
    ServiceComponentHostResponse r=new ServiceComponentHostResponse(serviceComponent.getClusterName(),serviceComponent.getServiceName(),serviceComponent.getName(),hostName,getConfigVersions(),getState().toString(),getStackVersion().getStackVersion(),getDesiredState().toString());
    return r;
  }
  finally {
    readLock.unlock();
  }
}
