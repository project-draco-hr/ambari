{
  clusterGlobalLock.readLock().lock();
  try {
    writeLock.lock();
    try {
      hostComponentDesiredConfigMappingDAO.removeByType(configTypes);
      for (      String configType : configTypes) {
        desiredConfigs.remove(configType);
      }
    }
  finally {
      writeLock.unlock();
    }
  }
  finally {
    clusterGlobalLock.readLock().unlock();
  }
}
