{
  LOG.info("Creating new repository version " + stackId.getStackName() + "-"+ version);
  RepositoryVersionEntity repositoryVersion=repositoryVersionDAO.create(stackId.getStackId(),version,stackId.getStackName() + "-" + version,repositoryVersionHelper.getUpgradePackageNameSafe(stackId.getStackName(),stackId.getStackVersion(),version),repositoryVersionHelper.serializeOperatingSystems(stackInfo.getRepositories()));
  return repositoryVersion;
}
