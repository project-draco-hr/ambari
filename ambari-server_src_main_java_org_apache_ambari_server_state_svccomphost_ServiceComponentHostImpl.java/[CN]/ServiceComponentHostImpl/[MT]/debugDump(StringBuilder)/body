{
  try {
    readLock.lock();
    sb.append("ServiceComponentHost={ hostname=" + hostName + ", serviceComponentName="+ serviceComponent.getName()+ ", clusterName="+ serviceComponent.getClusterName()+ ", serviceName="+ serviceComponent.getServiceName()+ ", desiredStackVersion="+ desiredStackVersion+ ", desiredState="+ desiredState+ ", stackVersion="+ stackVersion+ ", state="+ getState()+ " }");
  }
  finally {
    readLock.unlock();
  }
}
