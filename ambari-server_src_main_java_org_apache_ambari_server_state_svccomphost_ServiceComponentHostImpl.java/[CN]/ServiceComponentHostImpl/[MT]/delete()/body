{
  clusterGlobalLock.writeLock().lock();
  try {
    writeLock.lock();
    try {
      if (persisted) {
        removeEntities();
        persisted=false;
      }
      desiredConfigs.clear();
    }
  finally {
      writeLock.unlock();
    }
  }
  finally {
    clusterGlobalLock.writeLock().unlock();
  }
}
