{
  clusterGlobalLock.readLock().lock();
  try {
    readLock.lock();
    try {
      if (this.configs != null) {
        return Collections.unmodifiableMap(configs);
      }
 else {
        return new HashMap<String,String>();
      }
    }
  finally {
      readLock.unlock();
    }
  }
  finally {
    clusterGlobalLock.readLock().unlock();
  }
}
