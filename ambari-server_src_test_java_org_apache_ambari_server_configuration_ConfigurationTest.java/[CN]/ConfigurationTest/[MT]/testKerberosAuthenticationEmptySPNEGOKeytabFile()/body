{
  Properties properties=new Properties();
  properties.put(Configuration.KERBEROS_AUTH_ENABLED.getKey(),"true");
  properties.put(Configuration.KERBEROS_AUTH_SPNEGO_KEYTAB_FILE.getKey(),"");
