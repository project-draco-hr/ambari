{
  String masterKey="ThisissomeSecretPassPhrasse";
  String masterKeyLocation="/var/lib/ambari-server/keys/master";
  boolean persistMasterKey=false;
  if (args != null && args.length > 0) {
    masterKey=args[0];
    if (args.length > 1)     masterKeyLocation=args[1];
    if (args.length > 2 && !args[2].isEmpty())     persistMasterKey=args[2].toLowerCase().equals("true");
  }
  MasterKeyService masterKeyService=new MasterKeyServiceImpl(masterKey,masterKeyLocation,persistMasterKey);
  if (!masterKeyService.isMasterKeyInitialized()) {
    System.exit(1);
  }
  System.exit(0);
}
