{
  if (!metricName.equals(metric.getMetricName()))   return false;
  if (hostName != null ? !hostName.equals(metric.getHostName()) : metric.getHostName() != null)   return false;
  if (appId != null ? !appId.equals(metric.getAppId()) : metric.getAppId() != null)   return false;
  if (instanceId != null ? !instanceId.equals(metric.getInstanceId()) : metric.getInstanceId() != null)   return false;
  return true;
}
