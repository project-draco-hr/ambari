{
  if (outWriter == null) {
    return;
  }
  if (System.currentTimeMillis() - localFileCreateTime.getTime() > localFileRolloverSec * 1000) {
    logger.info("Closing file. Rolling over. name=" + localcurrentFile.getName() + ", filePath="+ localcurrentFile.getAbsolutePath());
    try {
      outWriter.flush();
      outWriter.close();
      addFileInReadyList(localcurrentFile);
    }
 catch (    Throwable t) {
      logger.error("Error on closing output writter. Exception will be ignored. name=" + localcurrentFile.getName() + ", filePath="+ localcurrentFile.getAbsolutePath());
    }
    outWriter=null;
    localcurrentFile=null;
  }
}
