{
  long timeSinceCreation=new Date().getTime() - currentSpoolerContext.getActiveLogCreationTime().getTime();
  boolean shouldRollover=timeSinceCreation > rolloverThresholdTimeMillis;
  if (shouldRollover) {
    logger.info("Detecting that time since file creation time " + currentSpoolerContext.getActiveLogCreationTime() + " has crossed threshold (msecs) "+ rolloverThresholdTimeMillis);
  }
  return shouldRollover;
}
