{
  return new StringBuilder().append("ActionDefinition:").append(" actionName: ").append(actionName).append(" actionType: ").append(actionType).append(" inputs: ").append(inputs).append(" description: ").append(description).append(" targetService: ").append(targetService).append(" targetComponent: ").append(targetComponent).append(" defaultTimeout: ").append(defaultTimeout).append(" targetType: ").append(targetType).toString();
}
