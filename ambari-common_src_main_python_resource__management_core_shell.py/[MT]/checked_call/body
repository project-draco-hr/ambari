@log_function_call
def checked_call(command, quiet=False, logoutput=None, cwd=None, env=None, preexec_fn=None, user=None, wait_for_finish=True, timeout=None, path=None, sudo=False, on_new_line=None):
    '\n  Execute the shell command and throw an exception on failure.\n  @throws Fail\n  @return: return_code, output\n  '
    return _call(command, logoutput, True, cwd, env, preexec_fn, user, wait_for_finish, timeout, path, sudo, on_new_line)
