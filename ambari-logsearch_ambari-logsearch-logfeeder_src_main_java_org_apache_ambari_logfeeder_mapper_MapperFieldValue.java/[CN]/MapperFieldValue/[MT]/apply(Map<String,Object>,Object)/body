{
  if (newValue != null) {
    if (prevValue != null) {
      if (prevValue.equalsIgnoreCase(value.toString())) {
        value=newValue;
        jsonObj.put(fieldName,value);
      }
    }
  }
 else {
    LogFeederUtil.logErrorMessageByInterval(this.getClass().getSimpleName() + ":apply","New value is null, so transformation is not applied. " + this.toString(),null,logger,Level.ERROR);
  }
  return value;
}
