{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Handling API Request: '" + getURI() + "'");
  }
  Result result;
  try {
    parseRenderer();
    parseQueryPredicate();
    result=getRequestHandler().handleRequest(this);
  }
 catch (  InvalidQueryException e) {
    result=new ResultImpl(new ResultStatus(ResultStatus.STATUS.BAD_REQUEST,"Unable to compile query predicate: " + e.getMessage()));
  }
catch (  IllegalArgumentException e) {
    result=new ResultImpl(new ResultStatus(ResultStatus.STATUS.BAD_REQUEST,"Invalid Request: " + e.getMessage()));
  }
  if (!result.getStatus().isErrorState()) {
    getResultPostProcessor().process(result);
  }
  return result;
}
