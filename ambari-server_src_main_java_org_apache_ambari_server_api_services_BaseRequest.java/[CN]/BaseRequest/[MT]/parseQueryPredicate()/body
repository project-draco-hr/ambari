{
  String queryString=m_body.getQueryString();
  if (queryString == null) {
    String uri=getURI();
    int qsBegin=uri.indexOf("?");
    queryString=(qsBegin == -1) ? null : uri.substring(qsBegin + 1);
  }
  if (queryString != null) {
    m_predicate=getPredicateCompiler().compile(queryString);
  }
}
