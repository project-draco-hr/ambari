{
  return new ServiceComponentHostRequest(properties.get(HOST_COMPONENT_CLUSTER_NAME_PROPERTY_ID),properties.get(HOST_COMPONENT_SERVICE_NAME_PROPERTY_ID),properties.get(HOST_COMPONENT_COMPONENT_NAME_PROPERTY_ID),properties.get(HOST_COMPONENT_HOST_NAME_PROPERTY_ID),null,properties.get(HOST_COMPONENT_STATE_PROPERTY_ID));
}
