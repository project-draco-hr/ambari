{
  Map<String,ResourceDefinition> mapChildren=new HashMap<String,ResourceDefinition>();
  TaskResourceDefinition taskResourceDefinition=new TaskResourceDefinition(null,m_clusterId,getId());
  taskResourceDefinition.getQuery().addProperty(getClusterController().getSchema(Resource.Type.Task).getKeyPropertyId(Resource.Type.Task));
  mapChildren.put(taskResourceDefinition.getPluralName(),taskResourceDefinition);
  return mapChildren;
}
