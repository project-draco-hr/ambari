{
  TypedQuery<ServiceConfigEntity> query=entityManagerProvider.get().createQuery("SELECT scv FROM ServiceConfigEntity scv " + "WHERE scv.clusterId = ?1 AND scv.createTimestamp = (" + "SELECT MAX(scv2.createTimestamp) FROM ServiceConfigEntity scv2 "+ "WHERE scv2.serviceName = scv.serviceName AND scv2.clusterId = ?1)",ServiceConfigEntity.class);
  return daoUtils.selectList(query,clusterId);
}
