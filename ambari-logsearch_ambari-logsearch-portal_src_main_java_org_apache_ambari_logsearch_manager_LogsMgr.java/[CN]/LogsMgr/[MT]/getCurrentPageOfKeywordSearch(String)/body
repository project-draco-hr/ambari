{
  if (stringUtil.isEmpty(requestDate)) {
    logger.error("Unique id is Empty");
    throw restErrorUtil.createRESTException("Unique id is Empty",MessageEnums.DATA_NOT_FOUND);
  }
  String pageNumber=mapUniqueId.get(requestDate);
  if (stringUtil.isEmpty(pageNumber)) {
    logger.error("Not able to find Page Number");
    throw restErrorUtil.createRESTException("Page Number not found",MessageEnums.DATA_NOT_FOUND);
  }
  return pageNumber;
}
