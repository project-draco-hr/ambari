{
  checkLoaded();
  Map<String,Host> hostMap=new HashMap<String,Host>();
  Host host=null;
  for (  String hostName : hostSet) {
    if (null != hostName) {
      host=hosts.get(hostName);
      if (host == null) {
        throw new HostNotFoundException(hostName);
      }
    }
 else {
      throw new HostNotFoundException(hostName);
    }
    hostMap.put(hostName,host);
  }
  return hostMap;
}
