{
  loadClustersAndHosts();
  Map<String,Host> hostMap=new HashMap<String,Host>();
  r.lock();
  try {
    for (    String host : hostSet) {
      if (!hosts.containsKey(host))       throw new HostNotFoundException(host);
 else       hostMap.put(host,hosts.get(host));
    }
  }
  finally {
    r.unlock();
  }
  return hostMap;
}
