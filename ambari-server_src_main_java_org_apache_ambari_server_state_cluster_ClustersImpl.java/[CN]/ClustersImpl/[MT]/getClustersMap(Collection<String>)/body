{
  loadClustersAndHosts();
  Map<String,Cluster> clusterMap=new HashMap<String,Cluster>();
  r.lock();
  try {
    for (    String c : clusterSet) {
      if (c != null) {
        if (!clusters.containsKey(c))         throw new ClusterNotFoundException(c);
 else         clusterMap.put(c,clusters.get(c));
      }
    }
  }
  finally {
    r.unlock();
  }
  return clusterMap;
}
