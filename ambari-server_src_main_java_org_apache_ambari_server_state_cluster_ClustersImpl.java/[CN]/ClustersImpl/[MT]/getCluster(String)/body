{
  if (!clusters.containsKey(clusterName)) {
    ClusterEntity clusterEntity=clusterDAO.findByName(clusterName);
    if (clusterEntity != null) {
      Cluster cl=getClusterById(clusterEntity.getClusterId());
      clustersById.put(cl.getClusterId(),cl);
      clusters.put(cl.getClusterName(),cl);
      if (!clusterHostMap.containsKey(clusterEntity.getClusterName()))       clusterHostMap.put(clusterEntity.getClusterName(),new HashSet<Host>());
    }
 else {
      throw new ClusterNotFoundException(clusterName);
    }
  }
  return clusters.get(clusterName);
}
