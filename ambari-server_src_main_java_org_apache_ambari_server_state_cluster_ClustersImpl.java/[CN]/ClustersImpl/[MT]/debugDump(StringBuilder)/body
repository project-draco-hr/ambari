{
  r.lock();
  try {
    sb.append("Clusters=[ ");
    boolean first=true;
    for (    Cluster c : clusters.values()) {
      if (!first) {
        sb.append(" , ");
      }
      first=false;
      sb.append("\n  ");
      c.debugDump(sb);
      sb.append(" ");
    }
    sb.append(" ]");
  }
  finally {
    r.unlock();
  }
}
