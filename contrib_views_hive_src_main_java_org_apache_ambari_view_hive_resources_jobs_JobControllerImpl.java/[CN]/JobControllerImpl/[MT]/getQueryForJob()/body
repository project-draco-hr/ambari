{
  FilePaginator paginator=new FilePaginator(job.getQueryFile(),context);
  String query;
  try {
    query=paginator.readPage(0);
  }
 catch (  IOException e) {
    throw new ServiceFormattedException("Error when reading file: " + e.toString(),e);
  }
catch (  InterruptedException e) {
    throw new ServiceFormattedException("Error when reading file: " + e.toString(),e);
  }
  return query;
}
