{
  Apps apps=new Apps();
  apps.setApps(EMPTY_APPS);
  PostgresConnector conn=null;
  try {
    conn=getConnector();
    if (workflowId != null)     apps.setApps(conn.fetchAppDetails(workflowId));
  }
 catch (  IOException e) {
    LOG.error("Error interacting with RCA database ",e);
  }
 finally {
    if (conn != null) {
      conn.close();
    }
  }
  return apps;
}
