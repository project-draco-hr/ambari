{
  JSONArray list;
  try {
    list=Stacks.getInstance().getStackList();
    if (list.length() == 0) {
      throw new WebApplicationException(Response.Status.NO_CONTENT);
    }
  }
 catch (  WebApplicationException we) {
    throw we;
  }
catch (  Exception e) {
    throw new WebApplicationException((new ExceptionResponse(e)).get());
  }
  return list;
}
