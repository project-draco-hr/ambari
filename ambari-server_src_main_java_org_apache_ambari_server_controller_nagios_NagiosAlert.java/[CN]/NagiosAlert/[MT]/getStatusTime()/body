{
  long l=-1L;
  try {
switch (getStatus()) {
case 0:
      l=Long.parseLong(last_time_ok);
    break;
case 1:
  l=Long.parseLong(last_time_warning);
break;
case 2:
l=Long.parseLong(last_time_critical);
break;
default :
l=Long.parseLong(last_time_unknown);
break;
}
}
 catch (Exception e) {
}
return l;
}
