{
  if (!isReady) {
    s3LogPathFiles=getActualFiles(logPath);
    if (s3LogPathFiles != null && s3LogPathFiles.length > 0) {
      if (isTail() && s3LogPathFiles.length > 1) {
        logger.warn("Found multiple files (" + s3LogPathFiles.length + ") for the file filter "+ filePath+ ". Will use only the first one. Using "+ s3LogPathFiles[0]);
      }
      logger.info("File filter " + filePath + " expanded to "+ s3LogPathFiles[0]);
      isReady=true;
    }
 else {
      logger.debug(logPath + " file doesn't exist. Ignoring for now");
    }
  }
  return isReady;
}
