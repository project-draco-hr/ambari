{
  createActionDefinition("d1",ActionType.SYSTEM,"fileName",Service.Type.HDFS.toString(),Role.DATANODE,Short.parseShort("101"),"a1",TargetHostType.ANY,true);
  ActionEntity newOne=createActionDefinition("d1",ActionType.SYSTEM,"fileValue",Service.Type.HDFS.toString(),Role.DATANODE,Short.parseShort("101"),"a1",TargetHostType.ANY,false);
  actionDefinitionDAO.merge(newOne);
  ActionEntity actionDefinitionEntity=actionDefinitionDAO.findByPK("d1");
  Assert.assertEquals("fileValue",actionDefinitionEntity.getInputs());
  actionDefinitionDAO.remove(newOne);
  actionDefinitionEntity=actionDefinitionDAO.findByPK("d1");
  Assert.assertNull(actionDefinitionEntity);
}
