{
  if (client == null)   return;
  try {
    closeSession();
  }
 catch (  HiveClientException e) {
    LOG.error("Unable to close Hive session: " + e.getMessage());
  }
 finally {
    transport.close();
    transport=null;
    client=null;
    sessHandle=null;
    protocol=null;
  }
  LOG.info("Connection to Hive closed");
}
