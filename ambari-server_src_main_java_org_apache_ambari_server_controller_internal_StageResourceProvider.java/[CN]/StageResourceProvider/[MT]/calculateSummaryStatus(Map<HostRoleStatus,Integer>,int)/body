{
  return counters.get(HostRoleStatus.FAILED) > 0 ? StageStatus.FAILED : counters.get(HostRoleStatus.ABORTED) > 0 ? StageStatus.ABORTED : counters.get(HostRoleStatus.TIMEDOUT) > 0 ? StageStatus.TIMEDOUT : counters.get(HostRoleStatus.IN_PROGRESS) > 0 ? StageStatus.IN_PROGRESS : counters.get(HostRoleStatus.COMPLETED) == totalTasks ? StageStatus.COMPLETED : StageStatus.PENDING;
}
