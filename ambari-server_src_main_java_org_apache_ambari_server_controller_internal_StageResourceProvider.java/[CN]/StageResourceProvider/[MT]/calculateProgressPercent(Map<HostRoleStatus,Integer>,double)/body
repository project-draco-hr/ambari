{
  return ((counters.get(HostRoleStatus.QUEUED) * 0.09 + counters.get(HostRoleStatus.IN_PROGRESS) * 0.35 + counters.get(HostRoleStatus.HOLDING) * 0.35 + counters.get(HostRoleStatus.HOLDING_FAILED) * 0.35 + counters.get(HostRoleStatus.HOLDING_TIMEDOUT) * 0.35 + counters.get(HostRoleStatus.COMPLETED)) / total) * 100.0;
}
