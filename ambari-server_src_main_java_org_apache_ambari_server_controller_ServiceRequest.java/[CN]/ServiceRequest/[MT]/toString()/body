{
  StringBuilder sb=new StringBuilder();
  sb.append("clusterName=" + clusterName + ", serviceName="+ serviceName+ ", desiredState="+ desiredState+ ", configs=[ ");
  if (configVersions != null) {
    for (    Entry<String,String> entry : configVersions.entrySet()) {
      sb.append("{ type=" + entry.getKey() + ", versionTag="+ entry.getValue()+ "}, ");
    }
  }
  sb.append(" ]");
  return sb.toString();
}
