def do_erase_files_silent(self, pathList):
    if pathList:
        for path in pathList:
            if (path and os.path.exists(path)):
                try:
                    os.remove(path)
                except:
                    logger.warn(((('Failed to delete file: ' + path) + ', error: ') + str(sys.exc_info()[0])))
            else:
                logger.info(("File doesn't exists: " + path))
    return 0
