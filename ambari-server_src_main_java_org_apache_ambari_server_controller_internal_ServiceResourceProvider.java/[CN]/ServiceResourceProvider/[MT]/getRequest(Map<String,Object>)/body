{
  ServiceRequest svcRequest=new ServiceRequest((String)properties.get(SERVICE_CLUSTER_NAME_PROPERTY_ID),(String)properties.get(SERVICE_SERVICE_NAME_PROPERTY_ID),(String)properties.get(SERVICE_SERVICE_STATE_PROPERTY_ID));
  Object o=properties.get(SERVICE_PASSIVE_STATE_PROPERTY_ID);
  if (null != o)   svcRequest.setPassiveState(o.toString());
  return svcRequest;
}
