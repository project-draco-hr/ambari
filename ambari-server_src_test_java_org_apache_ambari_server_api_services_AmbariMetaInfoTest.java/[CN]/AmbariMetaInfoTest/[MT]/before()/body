{
  injector=Guice.createInjector(Modules.override(new InMemoryDefaultTestModule()).with(new MockModule()));
  injector.getInstance(GuiceJpaInitializer.class);
  injector.getInstance(EntityManager.class);
  File stackRoot=new File("src/test/resources/stacks");
  LOG.info("Stacks file " + stackRoot.getAbsolutePath());
  metaInfo=new AmbariMetaInfo(stackRoot,new File("target/version"));
  metaInfo.injector=injector;
  try {
    metaInfo.init();
  }
 catch (  Exception e) {
    LOG.info("Error in initializing ",e);
  }
}
