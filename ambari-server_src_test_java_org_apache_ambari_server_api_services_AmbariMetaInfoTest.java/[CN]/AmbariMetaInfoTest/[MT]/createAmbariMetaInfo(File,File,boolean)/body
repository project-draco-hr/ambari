{
  TestAmbariMetaInfo metaInfo=new TestAmbariMetaInfo(stackRoot,versionFile);
  if (replayMocks) {
    metaInfo.replayAllMocks();
    try {
      metaInfo.init();
    }
 catch (    Exception e) {
      LOG.info("Error in initializing ",e);
      throw e;
    }
    waitForAllReposToBeResolved(metaInfo);
  }
  return metaInfo;
}
