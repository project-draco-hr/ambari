{
  if (null == pc) {
    return Collections.emptyList();
  }
  if (forUpgrade) {
    return preTasks ? pc.preTasks : pc.postTasks;
  }
 else {
    return preTasks ? (null == pc.preDowngradeTasks ? pc.preTasks : pc.preDowngradeTasks) : (null == pc.postDowngradeTasks ? pc.postTasks : pc.postDowngradeTasks);
  }
}
