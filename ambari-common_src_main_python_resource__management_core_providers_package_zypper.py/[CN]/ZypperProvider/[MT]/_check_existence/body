def _check_existence(self, name):
    (code, out) = shell.call((CHECK_CMD % name))
    if bool(code):
        return False
    elif (('*' in name) or ('?' in name)):
        (code1, out1) = shell.call((GET_NOT_INSTALLED_CMD % name))
        return (NO_PACKAGES_FOUND_STATUS in out1.splitlines())
    else:
        return True
