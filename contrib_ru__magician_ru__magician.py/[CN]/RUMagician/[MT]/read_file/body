def read_file(self, file_path):
    '\n    :param file_path: File to read. Typically the ambari database password file.\n    :return: Return the contents of the file\n    '
    if os.path.exists(file_path):
        with open(file_path, 'r') as f:
            lines = f.readlines()
            return '\n'.join(lines)
    return None
