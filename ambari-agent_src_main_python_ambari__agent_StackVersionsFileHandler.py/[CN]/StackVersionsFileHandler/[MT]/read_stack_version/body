def read_stack_version(self, component):
    try:
        self.touch_file()
        for line in open(self.versionsFilePath):
            (comp, ver) = self.extract(line)
            if (comp == component):
                return ver
        return self.DEFAULT_VER
    except Exception as err:
        logger.error(("Can't read versions file: %s " % err.message))
        traceback.print_exc()
        return self.DEFAULT_VER
