def test_stop_secured(self):
    self.executeScript('2.1.1/services/WEBHCAT/package/scripts/webhcat_server.py', classname='WebHCatServer', command='stop', config_file='secured.json')
    self.assertResourceCalled('Execute', 'env HADOOP_HOME=/usr /usr/lib/hcatalog/sbin/webhcat_server.sh stop', user='hcat')
    self.assertResourceCalled('Execute', 'rm -f /var/run/webhcat/webhcat.pid')
    self.assertNoMoreResources()
