def build(self):
    results = []
    while (not ActionQueue.r.empty()):
        result = {'clusterId': 'unknown', 'id': 'action-001', 'kind': 'STOP_ACTION', 'commandResults': [], 'cleanUpCommandResults': [], 'serverName': 'hadoop.datanode', }
        results.append(result)
    logger.info(results)
    return results
