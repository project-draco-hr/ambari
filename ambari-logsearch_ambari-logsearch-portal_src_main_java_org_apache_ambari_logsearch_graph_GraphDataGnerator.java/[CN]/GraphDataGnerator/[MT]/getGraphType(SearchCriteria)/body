{
  String xAxisField=(String)searchCriteria.getParamValue("xAxis");
  String stackField=(String)searchCriteria.getParamValue("stackBy");
  String from=(String)searchCriteria.getParamValue("from");
  String to=(String)searchCriteria.getParamValue("to");
  String xType=(String)searchCriteria.getParamValue("type");
  if (xType == null)   return 0;
  String yAxisField=(String)searchCriteria.getParamValue("yAxis");
  if (stringUtil.isEmpty(xAxisField) || stringUtil.isEmpty(yAxisField)) {
  }
 else   if (stringUtil.isEmpty(stackField) && !stringUtil.isEmpty(to) && !stringUtil.isEmpty(from)&& !(xType.contains("date") || xType.contains("time")))   return 1;
 else   if (stringUtil.isEmpty(stackField) && !stringUtil.isEmpty(to) && !stringUtil.isEmpty(from)&& (xType.contains("date") || xType.contains("time")))   return 2;
 else   if (!stringUtil.isEmpty(stackField) && !stringUtil.isEmpty(to) && !stringUtil.isEmpty(from)&& !(xType.contains("date") || xType.contains("time")))   return 3;
 else   if (!stringUtil.isEmpty(stackField) && !stringUtil.isEmpty(to) && !stringUtil.isEmpty(from)&& (xType.contains("date") || xType.contains("time")))   return 4;
  return 0;
}
