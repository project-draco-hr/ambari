def test_resync_hawq_standby(self):
    ' Run custom command Resync HAWQ Standby '
    self.executeScript((self.COMMON_SERVICES_PACKAGE_DIR + '/scripts/hawqmaster.py'), classname='HawqMaster', command='resync_hawq_standby', config_dict=self.config_dict, stack_version=self.STACK_VERSION, target=RMFTestCase.TARGET_COMMON_SERVICES)
    self.assertResourceCalled('Execute', 'source /usr/local/hawq/greenplum_path.sh && hawq init standby -n -a -v -M fast', user=self.GPADMIN, timeout=900, not_if=None, only_if=None, logoutput=True)
    self.assertNoMoreResources()
