{
  Long hostId=0L;
  if (resultSet != null) {
    try {
      while (resultSet.next()) {
        hostId++;
        final String hostName=resultSet.getString(1);
        Long currHostID=resultSet.getLong(2);
        if (currHostID == null && StringUtils.isNotBlank(hostName)) {
          dbAccessor.executeQuery("UPDATE " + HOSTS_TABLE + " SET host_id = "+ hostId+ " WHERE host_name = '"+ hostName+ "'");
        }
      }
    }
 catch (    Exception e) {
      LOG.error("Unable to populate the id of the hosts. " + e.getMessage());
    }
  }
  return hostId;
}
