{
  Long hostId=0L;
  if (resultSet != null) {
    try {
      while (resultSet.next()) {
        final String hostName=resultSet.getString(1);
        HostEntity host=hostDAO.findByName(hostName);
        host.setId(++hostId);
        hostDAO.merge(host);
      }
    }
 catch (    Exception e) {
      LOG.error("Unable to populate the id of the hosts. " + e.getMessage());
    }
  }
  return hostId;
}
