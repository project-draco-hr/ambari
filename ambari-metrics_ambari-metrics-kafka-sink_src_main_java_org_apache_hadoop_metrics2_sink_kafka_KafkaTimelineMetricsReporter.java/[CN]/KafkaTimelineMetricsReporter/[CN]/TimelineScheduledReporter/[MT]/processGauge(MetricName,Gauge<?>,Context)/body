{
  final long currentTimeMillis=System.currentTimeMillis();
  final String sanitizedName=sanitizeName(name);
  final TimelineMetric metric=createTimelineMetric(currentTimeMillis,APP_ID,sanitizedName,Double.parseDouble(String.valueOf(gauge.value())));
  metricsCache.putTimelineMetric(metric);
  populateMetricsList(context,sanitizedName);
}
