def reset(self):
    self.system = System.get_instance()
    self.config = AttributeDictionary()
    self.resources = {}
    self.resource_list = []
    self.delayed_actions = set()
    self.update_config({'date': datetime.now(), 'resource_management.backup.path': '/tmp/resource_management/backup', 'resource_management.backup.prefix': datetime.now().strftime('%Y%m%d%H%M%S'), })
