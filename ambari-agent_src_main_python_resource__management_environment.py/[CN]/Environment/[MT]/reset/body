def reset(self, basedir, params):
    self.system = System.get_instance()
    self.config = AttributeDictionary()
    self.resources = {}
    self.resource_list = []
    self.delayed_actions = set()
    self.update_config({'date': datetime.now(), 'backup.path': '/tmp/resource_management/backup', 'backup.prefix': datetime.now().strftime('%Y%m%d%H%M%S'), 'basedir': basedir, 'params': params, })
