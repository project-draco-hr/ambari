@staticmethod
def substitute(val):
    env = Environment.get_instance()
    dic = env.config.params
    if (dic and isinstance(val, str)):
        result = Substitutor.ExtendedTemplate(val).substitute(dic)
        if ('$' in val):
            Substitutor.log.debug('%s after substitution is %s', val, result)
        return result
    return val
