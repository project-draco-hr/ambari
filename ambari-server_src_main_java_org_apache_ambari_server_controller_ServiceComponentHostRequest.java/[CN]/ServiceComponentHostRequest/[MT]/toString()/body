{
  StringBuilder sb=new StringBuilder();
  sb.append("{" + " clusterName=").append(clusterName).append(", serviceName=").append(serviceName).append(", componentName=").append(componentName).append(", hostname=").append(hostname).append(", desiredState=").append(desiredState).append(", state=").append(state).append(", desiredStackId=").append(desiredStackId).append(", staleConfig=").append(staleConfig).append(", adminState=").append(adminState).append(", maintenanceState=").append(maintenanceState).append("}");
  return sb.toString();
}
