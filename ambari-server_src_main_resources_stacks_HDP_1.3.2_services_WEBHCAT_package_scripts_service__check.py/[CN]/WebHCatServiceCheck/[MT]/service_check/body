def service_check(self, env):
    import params
    env.set_params(params)
    File('/tmp/templetonSmoke.sh', content=StaticFile('templetonSmoke.sh'), mode=493)
    cmd = format('sh /tmp/templetonSmoke.sh {webhcat_server_host[0]} {smokeuser} {smokeuser_keytab} {security_param} {kinit_path_local}', smokeuser_keytab=(params.smoke_user_keytab if params.security_enabled else 'no_keytab'))
    Execute(cmd, tries=3, try_sleep=5, path='/usr/sbin:/sbin:/usr/local/bin:/bin:/usr/bin', logoutput=True)
