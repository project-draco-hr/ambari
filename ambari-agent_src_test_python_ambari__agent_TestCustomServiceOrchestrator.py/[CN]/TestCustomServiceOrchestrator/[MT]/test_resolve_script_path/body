@patch('os.path.exists')
def test_resolve_script_path(self, exists_mock):
    config = AmbariConfig().getConfig()
    orchestrator = CustomServiceOrchestrator(config)
    exists_mock.return_value = True
    path = orchestrator.resolve_script_path('/HBASE', 'scripts/hbase_master.py', 'PYTHON')
    self.assertEqual('/HBASE/package/scripts/hbase_master.py', path)
    exists_mock.return_value = False
    try:
        orchestrator.resolve_script_path('/HBASE', 'scripts/hbase_master.py', 'PYTHON')
        self.fail('ExpectedException not thrown')
    except AgentException:
        pass
