def write(self, owner, group, permission, path, content):
    try:
        f = open(path, 'w')
        f.write(content)
        f.close()
        if (os.getuid() == 0):
            os.chmod(path, permission)
            os.chown(path, owner, group)
        result = {'exitCode': 0, }
    except Exception:
        result = {'exitCode': 1, }
    return result
