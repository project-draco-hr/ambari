{
  super(hBaseAccessor,metricsConf,checkpointLocation,sleepIntervalMillis,checkpointCutOffMultiplier,hostAggregatorDisabledParam,inputTableName,outputTableName,nativeTimeRangeDelay);
  if (inputTableName.equals(METRICS_CLUSTER_AGGREGATE_TABLE_NAME)) {
    aggregateColumnName="HOSTS_COUNT";
  }
 else {
    aggregateColumnName="METRIC_COUNT";
  }
}
