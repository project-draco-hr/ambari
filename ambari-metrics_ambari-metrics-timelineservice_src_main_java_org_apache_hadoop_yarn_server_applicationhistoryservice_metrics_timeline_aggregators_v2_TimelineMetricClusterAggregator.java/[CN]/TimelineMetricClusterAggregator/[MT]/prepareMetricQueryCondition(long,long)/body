{
  EmptyCondition condition=new EmptyCondition();
  condition.setDoUpdate(true);
  condition.setStatement(String.format(GET_AGGREGATED_APP_METRIC_GROUPBY_SQL,getQueryHint(startTime),outputTableName,aggregateColumnName,tableName,startTime,endTime));
  if (LOG.isDebugEnabled()) {
    LOG.debug("Condition: " + condition.toString());
  }
  return condition;
}
