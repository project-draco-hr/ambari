{
  readWriteLock.readLock().lock();
  try {
    RequestScheduleResponse response=new RequestScheduleResponse(getId(),getClusterName(),getDescription(),getStatus(),getBatch(),getSchedule(),requestScheduleEntity.getCreateUser(),DateUtils.convertToReadableTime(requestScheduleEntity.getCreateTimestamp()),requestScheduleEntity.getUpdateUser(),DateUtils.convertToReadableTime(requestScheduleEntity.getUpdateTimestamp()));
    return response;
  }
  finally {
    readWriteLock.readLock().unlock();
  }
}
