{
  String c1="c1";
  String c2="c2";
  String h1="h1";
  String h2="h2";
  String h3="h3";
  clusters.addCluster(c1);
  clusters.addCluster(c2);
  clusters.addHost(h1);
  clusters.addHost(h2);
