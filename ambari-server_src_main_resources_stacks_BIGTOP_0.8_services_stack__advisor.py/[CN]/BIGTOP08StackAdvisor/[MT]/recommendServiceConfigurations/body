def recommendServiceConfigurations(self, service):
    calculator = super(BIGTOP08StackAdvisor, self).recommendServiceConfigurations(service)
    if (calculator is None):
        return {'OOZIE': self.recommendOozieConfigurations, 'HIVE': self.recommendHiveConfigurations, 'TEZ': self.recommendTezConfigurations, }.get(service, None)
    else:
        return calculator
