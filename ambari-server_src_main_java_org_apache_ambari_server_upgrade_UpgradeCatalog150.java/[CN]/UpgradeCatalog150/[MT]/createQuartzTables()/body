{
  String dbType=getDbType();
  String scriptPath=configuration.getResourceDirPath() + File.separator + "upgrade"+ File.separator+ "ddl"+ File.separator+ String.format(quartzScriptFilePattern,dbType);
  try {
    dbAccessor.executeScript(scriptPath);
  }
 catch (  IOException e) {
    LOG.error("Error reading file.",e);
  }
  if (dbType.equals(Configuration.POSTGRES_DB_NAME)) {
    grantAllPostgresPrivileges();
  }
}
