{
  this.sourceVersion=sourceVersion;
  properties.setProperty(Configuration.SERVER_PERSISTENCE_TYPE.getKey(),"remote");
  properties.setProperty(Configuration.SERVER_JDBC_URL.getKey(),Configuration.JDBC_IN_MEMORY_URL);
