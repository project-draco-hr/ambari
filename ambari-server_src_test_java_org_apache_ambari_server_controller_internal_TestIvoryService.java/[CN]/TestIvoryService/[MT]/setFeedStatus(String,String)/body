{
  String currentStatus=null;
  Feed feed=feeds.get(feedName);
  if (feed != null) {
    currentStatus=feed.getStatus();
    if (!currentStatus.equals(status)) {
      feed=new Feed(feed.getName(),feed.getDescription(),status,feed.getSchedule(),feed.getSourceClusterName(),feed.getTargetClusterName());
      feeds.put(feed.getName(),feed);
    }
  }
  return currentStatus;
}
