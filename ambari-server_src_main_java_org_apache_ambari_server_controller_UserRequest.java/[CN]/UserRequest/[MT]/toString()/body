{
  StringBuilder sb=new StringBuilder();
  sb.append("User" + ", username=" + userName + ", roles=[ ");
  if (roles != null && !roles.isEmpty()) {
    boolean first=true;
    for (    String role : roles) {
      if (!first) {
        sb.append(",");
      }
      first=false;
      sb.append(role);
    }
  }
  sb.append(" ]");
  return sb.toString();
}
