def get_stack_to_component(self):
    '\n    Gets the hdp-select component name given the script component\n    :return:  the name of the component on the HDP stack which is used by\n              hdp-select\n    '
    if (self.component not in self.COMPONENT_TO_HDP_SELECT_MAPPING):
        return None
    hdp_component = self.COMPONENT_TO_HDP_SELECT_MAPPING[self.component]
    return {'HDP': hdp_component, }
