def main(argv):
    if (len(argv) < 2):
        print_help()
        exit(1)
    command = argv[1]
    if (command == 'all'):
        all(argv)
    elif (command == 'request'):
        request_cluster(argv)
    elif (command == 'run'):
        run_cluster(argv)
    elif (command == 'terminate'):
        terminate()
    elif (command == 'help'):
        print_help()
    else:
        print_help()
