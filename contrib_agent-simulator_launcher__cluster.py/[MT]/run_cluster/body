def run_cluster(argv):
    if (len(argv) < 4):
        print_help()
        exit(1)
    server_Weave_IP = argv[2]
    server_external_IP = argv[3]
    config = Config
    config.load()
    cluster = Cluster()
    cluster.load_cluster_info(config.ATTRIBUTES['cluster_info_file'])
    cluster.run_docker_on_cluster(server_external_IP, server_Weave_IP)
